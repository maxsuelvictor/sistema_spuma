function TdmGeral.CriarReg_LOT(lid_empresa, lid_item,
  lnum_lote, lid_almoxarifado: String; lqtde_entrada, lqtde_saida,
  lqtde_atual: Double; lid_fiscal, lid_controle: Integer; lid_cor,
  lid_tamanho: String): boolean;
begin
  EST_CD_M_LOT.Insert;
  EST_CD_M_LOT.FieldByName('id_empresa').AsString      := lid_empresa;
  EST_CD_M_LOT.FieldByName('id_item').AsString         := lid_item;
  EST_CD_M_LOT.FieldByName('num_lote').AsString        := lnum_lote;
  EST_CD_M_LOT.FieldByName('id_almoxarifado').AsString := lid_almoxarifado;
  EST_CD_M_LOT.FieldByName('id_cor').AsString          := lid_cor;
  EST_CD_M_LOT.FieldByName('id_tamanho').AsString      := lid_tamanho;
  EST_CD_M_LOT.FieldByName('qtde_entrada').AsFloat     := lqtde_entrada;
  EST_CD_M_LOT.FieldByName('qtde_saida').AsFloat       := lqtde_saida;
  EST_CD_M_LOT.FieldByName('qtde_atual').AsFloat       := lqtde_atual;
  EST_CD_M_LOT.FieldByName('id_fiscal').AsInteger      := lid_fiscal;
