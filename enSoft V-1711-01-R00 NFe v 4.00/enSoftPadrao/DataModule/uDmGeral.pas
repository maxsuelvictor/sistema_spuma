function TdmGeral.BusSaldoItem(empresa, id_item, id_cor, id_tamanho,
  id_almoxarifado: String; var saldo_fisico: String): Boolean;
var
  index: integer;
  filtro: string;
begin
    { Filtros do EST_CD_M_FES:
      Index 0 - Empresa, Item
      Index 1 - Empresa, Item, Cor
      Index 2 - Empresa, Item, Tamanho
      Index 3 - Empresa, Item, Cor,Tamanho
      Index 4 - empresa, in(item),  in(cores)
      Index 5 - empresa, in(item),  in(tamanho)
      Index 6 - empresa, int(item), in(cor) in(tamanho)
      Index 7 - empresa, in(item)
      }

    { Filtros do EST_CD_M_FES_ALM:
      Index 0 - Empresa, Item, almoxarifado
      Index 1 - Empresa, Item, Cor, almoxarifado
      Index 2 - Empresa, Item, Tamanho, almoxarifado
      Index 3 - Empresa, Item, Cor,Tamanho, almoxarifado
      Index 4 - empresa, in(item),  in(cores), almoxarifado
      Index 5 - empresa, in(item),  in(tamanho), almoxarifado
      Index 6 - empresa, int(item), in(cor) in(tamanho), almoxarifado
      Index 7 - empresa, in(item), almoxarifado
      }

     result := true;

     saldo_fisico := '0';

     index  := 0;
     filtro := empresa + ',' + id_item;

     if (CAD_CD_C_PAR_CTRativa_tamanho.AsBoolean = false) and (CAD_CD_C_PAR_CTRativa_cor.AsBoolean = true) then
        begin
          index := 1;
          filtro := filtro + ',' + id_cor;
        end;

     if (CAD_CD_C_PAR_CTRativa_cor.AsBoolean = false) and (CAD_CD_C_PAR_CTRativa_tamanho.AsBoolean = true) then
        begin
          index := 2;
          filtro := filtro + ',' + id_tamanho;
        end;

     if (CAD_CD_C_PAR_CTRativa_cor.AsBoolean = true) and (CAD_CD_C_PAR_CTRativa_tamanho.AsBoolean = true) then
        begin
          index := 3;
          filtro := filtro + ',' + id_cor + ',' + id_tamanho;
        end;


     if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean = true then
        begin
          EST_CD_M_FES_ALM.Close;

          if index = 0 then
              EST_CD_M_FES_ALM.Data :=
                   EST_CD_M_FES_ALM.DataRequest(VarArrayOf([index, empresa, id_item, id_almoxarifado]));

          if index = 1 then
              EST_CD_M_FES_ALM.Data :=
                   EST_CD_M_FES_ALM.DataRequest(VarArrayOf([index, empresa, id_item, id_cor, id_almoxarifado]));
          if index = 2 then
              EST_CD_M_FES_ALM.Data :=
                   EST_CD_M_FES_ALM.DataRequest(VarArrayOf([index, empresa, id_item, id_tamanho, id_almoxarifado]));

          if index = 3 then
              EST_CD_M_FES_ALM.Data :=
                   EST_CD_M_FES_ALM.DataRequest(VarArrayOf([index, empresa, id_item, id_cor, id_tamanho, id_almoxarifado]));

          if not EST_CD_M_FES_ALM.IsEmpty then
             begin
               saldo_fisico := EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').Text;
             end
          else
             begin
               result := false;
             end;
        end
     else
        begin
           EST_CD_M_FES.Close;

           if index = 0 then
               EST_CD_M_FES.Data :=
                   EST_CD_M_FES.DataRequest(VarArrayOf([index, empresa, id_item]));

           if index = 1 then
               EST_CD_M_FES.Data :=
                   EST_CD_M_FES.DataRequest(VarArrayOf([index, empresa, id_item, id_cor]));
           if index = 2 then
               EST_CD_M_FES.Data :=
                    EST_CD_M_FES.DataRequest(VarArrayOf([index, empresa, id_item, id_tamanho]));

           if index = 3 then
               EST_CD_M_FES.Data :=
                    EST_CD_M_FES.DataRequest(VarArrayOf([index, empresa, id_item, id_cor, id_tamanho]));

           if not EST_CD_M_FES.IsEmpty then
              begin
                saldo_fisico := EST_CD_M_FES.FieldByName('SALDO_FISICO').Text;
              end
          else
             begin
               result := false;
             end;
        end;
end;


function TdmGeral.BusSaldoItemCoj(empresa, id_item, id_cor, id_tamanho,
  id_almoxarifado:String;qtde:Currency): String;
var
  BUS_ITE_COJ,BUS_ITE:TClientDataSet;
  saldo_fisico,msg:String;
begin
  BUS_ITE_COJ := TClientDataSet.Create(nil);
  BUS_ITE_COJ.RemoteServer := pcConecao;
  BUS_ITE_COJ.ProviderName := 'BUS_DP_C_ITE_COJ';
  BUS_ITE_COJ.Close;
  BUS_ITE_COJ.Data :=
  BUS_ITE_COJ.DataRequest(
              VarArrayOf([0,id_item]));

  BUS_ITE := TClientDataSet.Create(nil);
  BUS_ITE.RemoteServer := pcConecao;
  BUS_ITE.ProviderName := 'CAD_DP_C_ITE';
  BUS_ITE.Close;
  BUS_ITE.Data :=
  BUS_ITE.DataRequest(
              VarArrayOf([0,id_item]));

  msg:='';
  if (BUS_ITE.FieldByName('INT_TIPO_ITEM').AsString<>'09') then
    begin
       if BusSaldoItem(empresa, id_item, id_cor, id_tamanho,
               id_almoxarifado,saldo_fisico) then
         begin
           if qtde>StrToCurr(saldo_fisico) then
             begin
               msg := msg+'Item '+id_item+' saldo disponvel  '+saldo_fisico+'.'+#13;
             end;
         end
       else
         begin
           msg := msg+'Item '+id_item+' saldo no encontrado.'+#13;
         end;
    end;

  if not BUS_ITE_COJ.IsEmpty then
    begin
     BUS_ITE_COJ.First;
     while not BUS_ITE_COJ.EOF do
       begin
         msg := msg + BusSaldoItemCoj(empresa,BUS_ITE_COJ.FieldByName('ID_ITEM_CONJUGADO').AsString, id_cor, id_tamanho,id_almoxarifado,
                            BUS_ITE_COJ.FieldByName('QTDE').AsCurrency*qtde);

         BUS_ITE_COJ.Next;
       end;
     BUS_ITE_COJ.Close;
    end;

    Result := msg;
    BUS_ITE_COJ.Close;
    BUS_ITE_COJ.Free;
    BUS_ITE.Close;
    BUS_ITE.Free;


end;


function TdmGeral.PesquisaItemCoj(id_item:String;firstNivel:Boolean=true): String;
var
  BUS_ITE_COJ:TClientDataSet;
  msg,msg_parcial:String;
begin
  BUS_ITE_COJ := TClientDataSet.Create(nil);
  BUS_ITE_COJ.RemoteServer := pcConecao;
  BUS_ITE_COJ.ProviderName := 'BUS_DP_C_ITE_COJ';
  BUS_ITE_COJ.Close;
  BUS_ITE_COJ.Data :=
  BUS_ITE_COJ.DataRequest(
              VarArrayOf([0,id_item]));



  msg:='';

  if not BUS_ITE_COJ.IsEmpty then
    begin
     BUS_ITE_COJ.First;
     while not BUS_ITE_COJ.EOF do
       begin
       if msg <> '' then
         msg := msg +','+PesquisaItemCoj(BUS_ITE_COJ.FieldByName('ID_ITEM_CONJUGADO').AsString,false)
       else
         msg := msg + PesquisaItemCoj(BUS_ITE_COJ.FieldByName('ID_ITEM_CONJUGADO').AsString,false);
         BUS_ITE_COJ.Next;
       end;
     BUS_ITE_COJ.Close;
    end
  else
    begin
     if firstNivel=false then
       begin
         if msg<>'' then
            msg := msg +','+''''+id_item+''''
         else
            msg := msg +''''+id_item+'''';
       end;
    end;

    Result := msg;
    BUS_ITE_COJ.Close;
    BUS_ITE_COJ.Free;



end;


