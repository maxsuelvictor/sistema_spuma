function TdmGeral.BusCodigoRevListMestre(const fazer_busca, exibi_mensagens: boolean;
  name_formulario: string; var codigo, revisao: string; Tabela: TClientDataSet): Boolean;
var
  codigo_lista_mestre: String;
begin
  codigo  := '';
  revisao := '';

  result := true;
  if fazer_busca = true then
     begin
       if not cad_cd_c_par_ctr.fieldByName('ID_LME').isnull then
          begin
            codigo_lista_mestre := cad_cd_c_par_ctr.FieldByName('ID_LME').AsString;
          end
       else
          begin
            if exibi_mensagens then
               begin
                 Showmessage('Cdigo da lista mestre no foi encontrado nos parmetros .');
               end;
            result := false;
          end;
       if result = true then
          begin
            dmGeral.BUS_CD_C_LME_DOC_DPG.Close;
            dmGeral.BUS_CD_C_LME_DOC_DPG.Data :=
            dmGeral.BUS_CD_C_LME_DOC_DPG.DataRequest(VarArrayOf([0,codigo_lista_mestre, name_formulario]));
            if dmGeral.BUS_CD_C_LME_DOC_DPG.IsEmpty then
               begin
                 if exibi_mensagens then
                    begin
                      Showmessage('A codificao da lista mestre para este documento no foi encontrado.');
                    end;
                 result := false;
               end
            else
               begin
                 codigo  := BUS_CD_C_LME_DOC_DPG.FieldByName('CODIGO').AsString;
                 revisao := BUS_CD_C_LME_DOC_DPG.FieldByName('REVISAO').AsString;
                 if tabela <> nil then
                    begin
                      tabela.FieldByName('COD_LME').text  := codigo;
                      tabela.FieldByName('REV_LME').text := revisao;
                    end;
               end;
            dmGeral.BUS_CD_C_LME_DOC_DPG.Close;
          end;
     end;
end;

function TdmGeral.ExisteRegNoListBox(ListBox: TListBox; TamCampoChave: integer;
  TxtProcurar: String): Boolean;
var
  i: integer;
  txtLinha: string;
begin
  result := false;
  for i := 0 to ListBox.Count - 1 do
     begin
       txtLinha := copy(ListBox.Items[i],1,TamCampoChave);
       if pos(trim(TxtProcurar),txtLinha) <> 0 then
          begin
            result := true;
          end;
     end;
  if result = true then
     begin
       ShowMessage('Este registro j foi selecionado.');
     end;
end;

