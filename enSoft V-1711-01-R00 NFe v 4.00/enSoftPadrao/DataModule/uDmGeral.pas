function TdmGeral.VerMascaraCodigo(const codigo: string): string;
var
  i: integer;
  txt,n,grau1, grau2, grau3: string;
begin
  grau1 := copy(codigo,1,1);
  grau2 := copy(codigo,3,2);
  grau3 := copy(codigo,6,3);

  if Length(trim(grau2)) <> 2 then
     begin
       n := '';
       if length(trim(grau2)) = 1 then
          begin
            for i := 1 to 2 do
                begin
                  txt := copy(grau2,i,1);
                  if trim(txt) = '' then
                     begin
                       n := n+'0';
                     end
                  else
                     begin
                       n := n+txt;
                     end;
                end;
            grau2 := n;
          end
       else
          begin
            grau2 := '00';
          end;
     end;

  if length(trim(grau3)) <> 3 then
     begin
       n := '';
       if length(trim(grau3)) > 0 then
          begin
            for i := 1 to 3 do
                begin
                  txt := copy(grau3,i,1);
                  if trim(txt) = '' then
                     begin
                       n := n+'0';
                     end
                  else
                     begin
                       n := n+txt;
                     end;
                end;
            grau3 := n;
          end
       else
          begin
            grau3 := '000';
          end;
     end;

  result := '0.00.000';

  if trim(codigo) <> '' then
     begin
        if (StrToInt(trim(grau3)) > 0) then
           begin
             result := grau1+'.'+grau2+'.'+grau3;  //codigo;
           end
        else
           begin
              if (StrToInt(trim(grau2)) > 0) then
                 begin
                   result := grau1+'.'+grau2; //copy(codigo,1,4);
                 end
              else
                 begin
                    result := copy(codigo,1,1);
                    if result = '0' then
                       begin
                         result := '0.00.000';
                       end;
                 end;
           end;
     end;
end;




function TdmGeral.ValCFOP(const TIPO_NF, UF_EMPRESA,
  UF_EMITENTE, Cfop: String): Boolean;
var
  n: String;
begin
  result := true;
  n := copy(Cfop,1,1);
  if TIPO_NF[1] in ['E','R','F'] then
     begin
        if UF_EMPRESA = UF_EMITENTE then
           begin
             if n <> '1' then
                begin
                  ShowMessage('Emitente com UF igual da empresa e CFOP invlido!');
                  result := false;
                end;
           end
        else if UF_EMPRESA <> UF_EMITENTE then
           begin
             if (n <> '2') and (n <> '3') then
                begin
                  ShowMessage('Emitente com UF diferente da empresa e CFOP invlido!');
                  result := false;
                end
             else if (n = '3') and (UF_EMITENTE <> 'EX') then // 'EX' quer dizer exterior
                begin
                  ShowMessage('CFOP invlido.' + #13 + 'O ESTADO do emitente no  do EXTERIOR.');
                  result := false;
                end;
           end;
     end;
  if TIPO_NF[1] in ['S','D'] then
     begin
       if UF_EMPRESA = UF_EMITENTE then
          begin
            if n <> '5' then
               begin
                 ShowMessage('CFOP invlido!');
                 result := false;
               end;
          end
       else if UF_EMPRESA <> UF_EMITENTE then
          begin
            if (n <> '6') and (n <> '7') then
               begin
                 ShowMessage('CFOP invlido!');
                 result := false;
               end
            else if (n = '7') and (UF_EMITENTE <> 'EX') then // 'EX' quer dizer exterior
               begin
                 ShowMessage('CFOP invlido.' + #13 +
                             'O ESTADO do emitente no  do EXTERIOR.');
                 result := false;
               end;
          end;
     end;
end;


function TdmGeral.ValChaveNFe(const ChaveNFe: string): boolean;
const
  PESO : Array[0..43] of Integer = (4, 3, 2, 9, 8, 7, 6, 5, 4, 3, 2, 9, 8, 7, 6, 5, 4, 3, 2, 9, 8, 7, 6, 5, 4, 3, 2, 9, 8, 7, 6, 5, 4, 3, 2, 9, 8, 7, 6, 5, 4, 3, 2, 0);
var
  Retorno : boolean;
  aChave  : Array[0..43] of Char;
  Soma    : Integer;
  Verif   : Integer;
  I       : Integer;
begin
  Retorno := false;
  try
    try
      if not Length(ChaveNFe) = 44 then
        raise Exception.Create('');

      StrPCopy(aChave,StringReplace(ChaveNFe,' ', '',[rfReplaceAll]));
      Soma := 0;
      for I := Low(aChave) to High(aChave) do
        Soma := Soma + (StrToInt(aChave[i]) * PESO[i]);

      if Soma = 0 then
        raise Exception.Create('');

      Soma := Soma - (11 * (Trunc(Soma / 11)));
      if (Soma = 0) or (Soma = 1) then
        Verif := 0
      else
        Verif := 11 - Soma;

      Retorno := Verif = StrToInt(aChave[43]);
    except
      Retorno := false;
    end;
  finally
    Result := Retorno;
  end;
end;


function TdmGeral.ValComposicaoChaveNFe(id_fiscal:Integer;ChaveNFe: string): boolean;
var
  cnpj,modelo,serie,nNF,AAMM,cUF:String;
begin
   Result := true;

   if ChaveNFe.Length <> 44 then
     begin
       Result := false;
       exit;
     end;



    cUF := copy(ChaveNFe,1,2);
    AAMM := copy(ChaveNFe,3,4);
    cnpj := copy(ChaveNFe,7,14);
    modelo := copy(ChaveNFe,21,2);
    serie := copy(ChaveNFe,23,3);
    nNF := copy(ChaveNFe,26,9);



   dmGeral.BUS_CD_M_NFE_2.Close;
   dmgeral.BUS_CD_M_NFE_2.Data :=
   dmGeral.BUS_CD_M_NFE_2.DataRequest(VarArrayOf([102,id_fiscal]));

   dmGeral.BUS_CD_C_PAR.Close;
   dmgeral.BUS_CD_C_PAR.Data :=
   dmGeral.BUS_CD_C_PAR.DataRequest(VarArrayOf([0,dmGeral.BUS_CD_M_NFE_2.FieldByName('ID_EMPRESA').AsString]));



  if cnpj <> dmGeral.BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString then
    Result := false;

  if StrToInt(cUF)<> UFtoCUF(dmgeral.BUS_CD_C_PAR.FieldByname('INT_UF').AsString) then
    Result := false;

  if AAMM <> formatdatetime('YYMM',dmGeral.BUS_CD_M_NFE_2.FieldByName('DTA_EMISSAO').AsDateTime) then
    Result := false;


  if StrToInt(modelo) <> StrToInt(trim(dmGeral.BUS_CD_M_NFE_2.FieldByName('modelo').AsString)) then
    Result := false;

  if StrToInt(serie) <> StrToInt(trim(dmGeral.BUS_CD_M_NFE_2.FieldByName('serie').AsString)) then
    Result := false;

  if StrToInt(nNF) <> dmGeral.BUS_CD_M_NFE_2.FieldByName('numero').AsInteger then
    Result := false;

  dmGeral.BUS_CD_M_NFE_2.Close;
  dmGeral.BUS_CD_C_PAR.Close;

end;



