function TdmGeral.BusCfoTme(const id_emitente, id_tipo_estoque: String): String;
var
  uf_empresa, uf_emitente, tipo_movimento: String;
  pessoa_emitente: Integer;
begin

  result          := '';
  uf_empresa      := '';
  uf_emitente     := '';
  tipo_movimento  := '';

  uf_empresa := dmGeral.CAD_CD_C_PAR.FieldByName('INT_UF').AsString;

  dmGeral.CAD_CD_C_TME.Close;
  dmGeral.CAD_CD_C_TME.Data :=
  dmGeral.CAD_CD_C_TME.DataRequest(VarArrayOf([0, id_tipo_estoque]));

  if not dmGeral.CAD_CD_C_TME.IsEmpty then
     begin
        if ((dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0) and
           (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger      = 0)) or

           ((dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1) and
           (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger      = 1))  then
           begin
             dmGeral.CAD_CD_C_FOR.Close;
             dmGeral.CAD_CD_C_FOR.Data :=
             dmGeral.CAD_CD_C_FOR.DataRequest(VarArrayOf([0, id_emitente]));

             if dmGeral.CAD_CD_C_FOR.IsEmpty Then
                begin
                  Showmessage('UF do fornecedor no encontrado!');
                  exit;
                end;

             uf_emitente := dmGeral.CAD_CD_C_FOR.FieldByName('INT_UF').AsString;

             // Para Fornecedor o sistema no precisa buscar CFOP de fornecedor pessoa fsica.
             if uf_empresa = uf_emitente then
                begin
                  result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_INTERNO_PJ').Text;
                end;

              if uf_empresa <> uf_emitente then
                 begin
                   result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_EXTERNO_PJ').Text;
                 end;

              if trim(result) = '' then
                 begin
                    ShowMessage('CFOP do tipo de estoque no foi encontrado!');
                    exit;
                 end;
           end;

        if ((dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1)  and
           (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger       = 0)) or
           ((dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0)  and
           (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger       = 1)) then
           begin
             dmGeral.CAD_CD_C_CLI.Close;
             dmGeral.CAD_CD_C_CLI.Data :=
             dmGeral.CAD_CD_C_CLI.DataRequest(VarArrayOf([0, id_emitente]));

             if dmGeral.CAD_CD_C_CLI.IsEmpty Then
                begin
                  Showmessage('UF do cliente no encontrado!');
                  exit;
                end;

             pessoa_emitente := dmGeral.CAD_CD_C_CLI.FieldByName('PESSOA').AsInteger;
             uf_emitente     := dmGeral.CAD_CD_C_CLI.FieldByName('INT_UF').AsString;


             if uf_empresa = uf_emitente then
                begin
                  if pessoa_emitente = 0 then
                     begin
                       result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_INTERNO_PF').Text;
                     end
                  else if pessoa_emitente = 1 then
                     begin
                       result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_INTERNO_PJ').Text;
                     end
                end;

             if uf_empresa <> uf_emitente then
                begin
                  if pessoa_emitente = 0 then
                     begin
                       result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_EXTERNO_PF').Text;
                     end
                  else if pessoa_emitente = 1 then
                     begin
                       result := dmGeral.CAD_CD_C_TME.FieldByName('CFO_EXTERNO_PJ').Text;
                     end
                end;

             if trim(result) = '' then
                begin
                  ShowMessage('CFOP do tipo de estoque no foi encontrado!');
                  exit;
                end;
           end;
     end;
end;


