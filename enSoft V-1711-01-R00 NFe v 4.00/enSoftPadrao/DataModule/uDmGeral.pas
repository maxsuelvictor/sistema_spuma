function TdmGeral.VerifEstNeg(cds_pai,cds_itens, cds_nf_itens_antigo: TClientDataSet): Boolean;
var
  lPonteiroItens : TBookmark;
  lIndex: integer;
  lSaldoFisico,lQtdeAnterior: Double;
  lMens: String;
begin

    { Esse mtodo verifica os itens que esto com estoque negativo }

    lMens := '';

    result := false;
    { Filtros do EST_CD_M_FES:
      Index 0 - Empresa, Item
      Index 1 - Empresa, Item, Cor
      Index 2 - Empresa, Item, Tamanho
      Index 3 - Empresa, Item, Cor,Tamanho
      Index 4 - empresa, in(item),  in(cores)
      Index 5 - empresa, in(item),  in(tamanho)
      Index 6 - empresa, int(item), in(cor) in(tamanho)
    }

    { Filtros do EST_CD_M_FES_ALM:
      Index 0 - Empresa, Item, almoxarifado
      Index 1 - Empresa, Item, almoxarifado, Cor
      Index 2 - Empresa, Item, almoxarifado, Tamanho
      Index 3 - Empresa, Item, almoxarifado, Cor,Tamanho
    }

    lIndex := 0;
    if CAD_CD_C_PAR_CTRATIVA_COR.AsBoolean = true then
       begin
          lIndex := 1;
       end;

    // busca os tamanhos dos itens do pedido de venda
    if CAD_CD_C_PAR_CTRATIVA_TAMANHO.AsBoolean = true then
       begin
          lIndex := 2;
       end;

    if (CAD_CD_C_PAR_CTRATIVA_COR.AsBoolean = true) and (CAD_CD_C_PAR_CTRATIVA_TAMANHO.AsBoolean = true) then
       begin
          lIndex := 3;
       end;

   lPonteiroItens := cds_itens.GetBookmark;
   cds_itens.First;
   while not cds_itens.Eof do
      begin
         if cds_itens.FieldByName('INT_TIPO_ITEM').AsInteger <> 9 then // Se no for item do tipo SERVIO.
            begin
              if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean = true then
                 begin
                   BUS_CD_M_FES_ALM.Close;
                   case lIndex of
                     0:
                       BUS_CD_M_FES_ALM.Data :=
                           BUS_CD_M_FES_ALM.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                       cds_itens.FieldByName('ID_ITEM').AsString,
                                                                       cds_pai.FieldByName('ID_ALMOXARIFADO').AsString]));
                     1:
                       BUS_CD_M_FES_ALM.Data :=
                           BUS_CD_M_FES_ALM.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                       cds_itens.FieldByName('ID_ITEM').AsString,
                                                                       cds_pai.FieldByName('ID_ALMOXARIFADO').AsString,
                                                                       cds_itens.FieldByName('ID_COR').AsString]));
                     2:
                       BUS_CD_M_FES_ALM.Data :=
                           BUS_CD_M_FES_ALM.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                       cds_itens.FieldByName('ID_ITEM').AsString,
                                                                       cds_pai.FieldByName('ID_ALMOXARIFADO').AsString,
                                                                       cds_itens.FieldByName('ID_TAMANHO').AsString]));
                     3:
                       BUS_CD_M_FES_ALM.Data :=
                           BUS_CD_M_FES_ALM.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                       cds_itens.FieldByName('ID_ITEM').AsString,
                                                                       cds_pai.FieldByName('ID_ALMOXARIFADO').AsString,
                                                                       cds_itens.FieldByName('ID_COR').AsString,
                                                                       cds_itens.FieldByName('ID_TAMANHO').AsString]));
                   end;
                   lSaldoFisico := BUS_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat;
                 end
              else
                 begin
                   BUS_CD_M_FES.Close;
                   case lIndex of
                     0:
                       BUS_CD_M_FES.Data :=
                           BUS_CD_M_FES.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                   cds_itens.FieldByName('ID_ITEM').AsString]));
                     1:
                       BUS_CD_M_FES.Data :=
                           BUS_CD_M_FES.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                   cds_itens.FieldByName('ID_ITEM').AsString,
                                                                   cds_itens.FieldByName('ID_COR').AsString]));
                     2:
                       BUS_CD_M_FES.Data :=
                           BUS_CD_M_FES.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                   cds_itens.FieldByName('ID_ITEM').AsString,
                                                                   cds_itens.FieldByName('ID_TAMANHO').AsString]));
                     3:
                       BUS_CD_M_FES.Data :=
                           BUS_CD_M_FES.DataRequest(VarArrayOf([0, cds_pai.FieldByName('ID_EMPRESA').AsString,
                                                                   cds_itens.FieldByName('ID_ITEM').AsString,
                                                                   cds_itens.FieldByName('ID_COR').AsString,
                                                                   cds_itens.FieldByName('ID_TAMANHO').AsString]));
                   end;

                   lSaldoFisico := BUS_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat;
                 end;

              lQtdeAnterior := 0;
              if cds_nf_itens_antigo <> nil then
                 begin
                   if cds_nf_itens_antigo.Locate('ID_ITEM;ID_SEQUENCIA;',VarArrayOf([cds_itens.FieldByName('ID_ITEM').AsString,
                                                                                     cds_itens.FieldByName('ID_SEQUENCIA').AsString]),[]) then
                      begin
                        lQtdeAnterior := cds_nf_itens_antigo.FieldByName('QTDE').AsFloat;
                      end;
                 end;

               if cds_itens.FieldByName('QTDE').AsFloat > (lSaldoFisico + lQtdeAnterior) then
                  begin
                    result := true;
                    lMens  := 'Item: ' + cds_itens.FieldByName('ID_ITEM').AsString + ' - Sequncia: ' + cds_itens.FieldByName('ID_SEQUENCIA').AsString + #13;
                  end;
               if trim(lMens) <> '' then
                  begin
                    lMens := 'Aviso: Itens com saldo negativo: ' + #13 + lMens;
                  end;
            end;
         cds_itens.Next;
      end;
   cds_itens.GotoBookmark(lPonteiroItens);
end;

