procedure TdmGeral.GerarFPG_Doc(id_forma_fpg, id_plano, id_Ccusto: String; qtde_parcelas: integer;  valor: Currency; cds: TClientDataSet;
                                Origem:Integer);
  procedure MostraMensagem;
  begin
    ShowMessage('Mensagem');
  end;
var
  SMPrincipal: TSMClient;
  i,ultDiasPrazo: integer;
  lTotFpgInserido, lSoma, lVlrParcela, lVlrRestante, lVlrUltParc,lVlrJurosParc,lvlrJurosUltParc,lVlrJurosRestante: Currency;
  IdFormaPag: String;
  vlrTitulo: Currency;
begin

   {
    Origem:
       0 - Pedido de venda
       1 - Movimento de cheque
       2 - Ordem de sada
       3 - Nota Fiscal(Controle de Caixa)
       4 - Fechamento de Ordem de Servio
       5 - Pedido Cupon - FAT_FM_M_PNF
       6 - Nota fiscal de Entrada
   }

  lTotFpgInserido := 0;
  lVlrParcela     := 0;
  lSoma           := 0;

  if origem = 5 then
     begin
       IdFormaPag := cds.fieldbyname('ID_FORMA_PAG').AsString;
       dmGeral.BusFormaPgtos2(0,cds.fieldbyname('ID_FORMA_PAG').AsString);
       vlrTitulo  := cds.fieldbyname('VLR_TITULO').AsCurrency;
     end;

  cds.Cancel;

  if not (Origem in [5,4]) then
     begin
        if not cds.IsEmpty then
           begin
             if Dc_MessageDlgCheck('Existe forma de pagamento j lanado!' + #13 + 'Deseja refazer?', mtConfirmation, [mbYes, mbNo],
                0, mrNo, true, false,'', @MostraMensagem) = 7 then
                //if MessageDlg('Existe forma de pagamento j lanado!' + #13 +
                //               'Deseja refazer?', mtConfirmation, [mbYes, mbNo], 1) = mrNo then
                begin
                  cds.First;
                  while not cds.eof do
                     begin
                       if (origem = 0) or (origem = 2)  or (origem = 3) or (origem = 4) or (origem = 6)  then
                          lTotFpgInserido :=  lTotFpgInserido + cds.fieldbyname('VLR_TITULO').AsCurrency;
                       if origem = 1 then
                          lTotFpgInserido :=  lTotFpgInserido + cds.fieldbyname('VLR_TITULO').AsCurrency;

                       cds.Next;
                     end;
                   if lTotFpgInserido > 0 then
                      begin
                        if lTotFpgInserido >= Valor then
                           begin
                             cds.Cancel;
                             ShowMessage('No h saldo valor para poder adicionar nos vencimentos.');
                             abort;
                           end;
                      end;

                end
             else
                begin
                  cds.First;
                  while not cds.eof do
                     begin
                       cds.delete;
                     end;
                end;
           end;
     end;

  if (Origem = 4) then
    begin
       if not cds.IsEmpty then
           begin
             if Dc_MessageDlgCheck('Existe forma de pagamento j lanado!' + #13 + 'Deseja refazer?', mtConfirmation, [mbYes, mbNo],
                0, mrNo, true, false,'', @MostraMensagem) = 7 then
                begin
                 if Dc_MessageDlgCheck('Deseja adicionar mais parcelas?', mtConfirmation, [mbYes, mbNo],
                      0, mrNo, true, false,'', @MostraMensagem) = 7 then
                    begin
                      exit;
                    end;
                end
             else
                begin
                  cds.First;
                  while not cds.eof do
                     begin
                       cds.delete;
                     end;
                end;
           end;
    end;

  if (Origem = 5) then
     begin
     // if not cds.IsEmpty then
     //    begin
            //if dmGeral.BUS_CD_C_FPG2.FieldByName('DOC_IMPRESSO').AsInteger <> 5 then
               //begin
                  cds.First;
                  while not cds.eof do
                     begin
                       dmGeral.BusFormaPgtos2(0,cds.fieldbyname('ID_FORMA_PAG').AsString);
                       if dmGeral.BUS_CD_C_FPG2.FieldByName('DOC_IMPRESSO').AsInteger <> 5 then
                          begin
                            lTotFpgInserido :=  lTotFpgInserido + cds.fieldbyname('VLR_TITULO').AsCurrency;
                          end;
                       cds.Next;
                     end;

                  if lTotFpgInserido > 0 then
                     begin
                        if lTotFpgInserido >= Valor then
                           begin
                             cds.Cancel;
                             ShowMessage('No h saldo valor para poder adicionar nos vencimentos.');
                             abort;
                           end;
                     end
                  else
                     begin
                       dmGeral.BusFormaPgtos2(0,IdFormaPag);
                       if dmGeral.BUS_CD_C_FPG2.FieldByName('DOC_IMPRESSO').AsInteger <> 5 then
                          begin
                             if vlrTitulo >= Valor then
                                begin
                                  cds.Cancel;
                                  ShowMessage('Valor da forma de pagamento no pode ser maior que o do pedido de venda.');
                                  abort;
                                end;
                          end;
                     end;
              // end;
      //   end;
     end;






  if not (Origem in [5,4,1]) then
     begin
        if lTotFpgInserido > 0 then
           begin
             lVlrRestante := Valor - lTotFpgInserido;
             lVlrParcela :=  RoundTo(lVlrRestante / qtde_parcelas,-2);
             for i := 1 to qtde_parcelas do
                begin
                   if i <> qtde_parcelas then
                      begin
                        lSoma := lSoma + lVlrParcela
                      end
                   else
                      begin
                        lVlrUltParc := RoundTo(lVlrRestante - lSoma,-2);
                      end;
                end;
           end
        else
           begin
             if (dmGeral.CAD_CD_C_PAR_MOD.FieldByName('SGQ').AsBoolean = false) then
                 begin
                   lVlrParcela :=  RoundTo(Valor / qtde_parcelas,-2);
                   for i := 1 to qtde_parcelas do
                      begin
                         if i <> qtde_parcelas then
                            begin
                              lSoma := lSoma + lVlrParcela
                            end
                         else
                            begin
                              lVlrUltParc := RoundTo(Valor - lSoma,-2);
                            end;
                      end;
                 end
             else
                 begin
                   // Deixar somente a tima parcela com valor decimal
                   lVlrParcela :=  trunc(Valor / qtde_parcelas);
                   for i := 1 to qtde_parcelas do
                      begin
                         if i <> qtde_parcelas then
                            begin
                              lSoma := lSoma + lVlrParcela
                            end
                         else
                            begin
                              lVlrUltParc := RoundTo(Valor - lSoma,-2);
                            end;
                      end;
                 end;
           end;
     end;



   if (Origem = 1) then
     begin
        if lTotFpgInserido > 0 then
           begin
             lVlrRestante := Valor - lTotFpgInserido;
             lVlrParcela := RoundTo(lVlrRestante/ qtde_parcelas,-2);
             lVlrUltParc := lVlrRestante - lVlrParcela*(qtde_parcelas-1);
           end
        else
           begin
             lVlrParcela := RoundTo(Valor / qtde_parcelas,-2);
             lVlrUltParc := Valor - lVlrParcela*(qtde_parcelas-1);
           end;

        if (dmgeral.FIN_CD_M_MCH.FieldBYName('OPERACAO').AsInteger = 4) then
           begin
              if (dmGeral.FIN_CD_M_MCH.FieldBYName('VLR_TOT_JUROS').AsCurrency <=
                  dmGeral.FIN_CD_M_MCH.FieldByName('VLR_TOT_JUROS_CHE').AsCurrency) then
                begin
                  lVlrJurosRestante := dmGeral.FIN_CD_M_MCH.FieldByName('VLR_TOT_JUROS_CHE').AsCurrency
                                      - dmGeral.FIN_CD_M_MCH.FieldBYName('VLR_TOT_JUROS').AsCurrency;
                  lVlrJurosParc := RoundTo(lVlrJurosRestante/ qtde_parcelas,-2);
                  lVlrJurosUltParc := lVlrJurosRestante - lVlrJurosParc*(qtde_parcelas-1);
                end;
           end;

     end;



   if Origem = 4 then
     begin
       lVlrParcela := RoundTo(Valor / qtde_parcelas,-2);
       lVlrUltParc := Valor - lVlrParcela*(qtde_parcelas-1);
     end;


   if Origem = 5 then
     begin
       dmGeral.BusFormaPgtos2(0,IdFormaPag);
       if dmGeral.BUS_CD_C_FPG2.FieldByName('DOC_IMPRESSO').AsInteger <> 5 then
          begin
            if lTotFpgInserido > 0 then
               begin
                 lVlrRestante := vlrTitulo;
                 lVlrParcela :=  RoundTo(lVlrRestante / qtde_parcelas,-2);
                 for i := 1 to qtde_parcelas do
                    begin
                       if i <> qtde_parcelas then
                          begin
                            lSoma := lSoma + lVlrParcela
                          end
                       else
                          begin
                            lVlrUltParc := RoundTo(lVlrRestante - lSoma,-2);
                          end;
                    end;
               end
            else
               begin
                 lVlrParcela :=  RoundTo(vlrTitulo / qtde_parcelas,-2);
                 for i := 1 to qtde_parcelas do
                    begin
                       if i <> qtde_parcelas then
                          begin
                            lSoma := lSoma + lVlrParcela
                          end
                       else
                          begin
                            lVlrUltParc := RoundTo(vlrTitulo - lSoma,-2);
                          end;
                    end;
               end;
          end
       else
          begin
            lSoma := 0;
            cds.First;
            while not cds.eof do
               begin
                 lSoma :=  lSoma + cds.fieldbyname('VLR_TITULO').AsCurrency;
                 cds.Next;
               end;
            cds.First;

            if vlrTitulo > (valor - lSoma) then
               begin
                 lVlrUltParc := valor - lSoma;
               end
            else
               begin
                 lVlrUltParc := vlrTitulo;
               end;
            lSoma := 0;
          end;
     end;



  if (not BUS_CD_C_CLI_PRD.IsEmpty) then
     begin
       BUS_CD_C_CLI_PRD.First;
     end;

  for i := 1 to qtde_parcelas do
      begin
        cds.Insert;

        // O id_titulo  gerado no OnNewRecord do cds.

        cds.FieldByName('ID_FORMA_PAG').AsInteger    := strtoint(id_forma_fpg);
        dmGeral.BusFormaPgtos(0,cds.FieldByName('ID_FORMA_PAG').text);
        cds.FieldByName('int_nomefpg').AsString      := BUS_CD_C_FPG.FieldByName('DESCRICAO').AsString;
        if origem = 0 then // Pedido de Venda
          begin
            cds.FieldByName('INT_DOCIMPRESSO').AsInteger := BUS_CD_C_FPG.FieldByName('DOC_IMPRESSO').AsInteger;
          end;
        {if BUS_CD_C_FPG.FieldByName('DOC_IMPRESSO').AsInteger = 5 then
          cds.FieldByName('DTA_VENCIMENTO').AsDateTime := xDataSis
        else
          cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis + (i * 30));
        cds.FieldByName('DIAS').AsInteger            :=  i * 30;  }

         if BUS_CD_C_FPG.FieldByName('TIPO_PAGAMENTO').AsInteger = 1 then
           begin
              if origem <> 0 then
               begin
                cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis + (i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger)+ dmGeral.BUS_CD_C_FPG.FieldByName('acresc_dias_em_vencto').AsInteger);
                cds.FieldByName('DIAS').AsInteger            :=  (i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger) +
                                                                      dmGeral.BUS_CD_C_FPG.FieldByName('acresc_dias_em_vencto').AsInteger;
               end;

              if origem = 0 then // Pedido de Venda
                begin
                  if (dmGeral.CAD_CD_C_PAR_MOD.FieldByName('REV').AsBoolean=false) then
                    begin
                      cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis + (i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger)+ dmGeral.BUS_CD_C_FPG.FieldByName('acresc_dias_em_vencto').AsInteger);
                      cds.FieldByName('DIAS').AsInteger            :=  (i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger) + dmGeral.BUS_CD_C_FPG.FieldByName('acresc_dias_em_vencto').AsInteger;
                    end;


                  if (dmGeral.CAD_CD_C_PAR_MOD.FieldByName('REV').AsBoolean=true) then
                    begin                                                              // 3 - Carto //
                      if (not BUS_CD_C_CLI_PRD.IsEmpty) and (BUS_CD_C_FPG.FieldByName('doc_impresso').AsInteger <> 3) then
                        begin
                          if not BUS_CD_C_CLI_PRD.Eof then
                            begin
                              cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis +
                                               (BUS_CD_C_CLI_PRD.FieldByName('DIAS').AsInteger ));
                              cds.FieldByName('DIAS').AsInteger            :=
                                                BUS_CD_C_CLI_PRD.FieldByName('DIAS').AsInteger;
                            end
                          else
                            begin
                             ultDiasPrazo := BUS_CD_C_CLI_PRD.FieldByName('DIAS').AsInteger;
                             cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis + ultDiasPrazo);
                             cds.FieldByName('DIAS').AsInteger            :=  ultDiasPrazo;
                            end;

                          BUS_CD_C_CLI_PRD.Next;
                         { if BUS_CD_C_CLI_PRD.Eof  then
                            begin
                              ultDiasPrazo := BUS_CD_C_CLI_PRD.FieldByName('DIAS').AsInteger;
                            end;}
                        end
                      else
                        begin
                          cds.FieldByName('DTA_VENCIMENTO').AsDateTime := (xDataSis + (i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger));
                          cds.FieldByName('DIAS').AsInteger            :=  i * dmGeral.BUS_CD_C_FPG.FieldByName('DIAS_VENCIMENTO').AsInteger;
                        end;
                    end;
                end
           end
        else
           begin
             cds.FieldByName('DTA_VENCIMENTO').AsDateTime := xDataSis;
             cds.FieldByName('DIAS').AsInteger            :=  0;
           end;

        cds.FieldByName('CHE_BANCO').text            := '';
        cds.FieldByName('CHE_AGENCIA').text          := '';
        cds.FieldByName('CHE_CONTA').AsString        := '';
        cds.FieldByName('CHE_NUMERO').AsInteger      :=  0;
        cds.FieldByName('CHE_EMITENTE').text         := '';


        if origem in [3,6] then // Nota Fiscal(Controle de Caixa)
           begin
             SMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);
             try
                cds.FieldByName('id_nfe_tit').AsInteger :=
                     SMPrincipal.enValorChave('FAT_TB_M_NFE_TIT');
             finally
                FreeAndNil(SMPrincipal);
             end;
             cds.FieldByName('ID_PLANO').AsString     := id_plano;
             cds.FieldByName('ID_CCUSTO').AsString    := id_Ccusto;

             if trim(id_plano) <> '' then
                begin
                  dmGeral.BusPlanoCta(0,id_plano);
                  cds.FieldByName('INT_NOMEPCT').Text := dmGeral.BUS_CD_C_PCT.FieldByName('DESCRICAO').AsString;
                  dmGeral.BUS_CD_C_PCT.close;
                end;

             cds.FieldByName('num_parcela').AsString  := '';
             if i < qtde_parcelas then
               begin
                cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrParcela;
                if lVlrParcela = 0 then
                   begin
                     cds.Cancel;
                     ShowMessage('No  permitido parcela com valor 0.');
                     abort;
                   end;

               end
             else
               begin
                cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrUltParc;
                if lVlrUltParc = 0 then
                   begin
                     cds.Cancel;
                     ShowMessage('No  permitido parcela com valor 0.');
                     abort;
                   end;
               end;
           end;

        {if origem = 2 then // Ordem de sada
           begin

              cds.FieldByName('num_parcela').AsString       := '';

             if i < qtde_parcelas then
               begin
                cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrParcela;
               end
             else
               begin
                cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrUltParc;
               end;
           end;}

        if origem = 1 then  // Movimento de Cheques
           begin
             if i < qtde_parcelas then
               begin
                cds.FieldByName('VLR_TOTAL').AsCurrency     := lVlrParcela;
                cds.FieldByName('VLR_TITULO').AsCurrency := lVlrParcela;

                if lVlrParcela = 0 then
                   begin
                     cds.Cancel;
                     ShowMessage('No  permitido parcela com valor 0.');
                     abort;
                   end;

                if (dmGeral.FIN_CD_M_MCH.FieldByName('OPERACAO').AsInteger=4) then
                   begin
                      try
                        cds.FieldByName('VLR_JUROS').OnChange := nil;
                        cds.FieldByName('VLR_TITULO').OnChange := nil;
                        cds.FieldByName('VLR_JUROS').AsCurrency := lVlrJurosParc;
                        cds.FieldByName('VLR_TOTAL').AsCurrency :=  cds.FieldByName('VLR_TITULO').AsCurrency+
                                                                   cds.FieldByName('VLR_JUROS').AsCurrency;
                      finally
                        cds.FieldByName('VLR_JUROS').OnChange := FIN_CD_M_MCH_TITvlr_jurosChange;
                        cds.FieldByName('VLR_TITULO').OnChange := FIN_CD_M_MCH_TITvlr_tituloChange;
                      end;
                   end;

               end
             else
               begin
                 cds.FieldByName('VLR_TOTAL').AsCurrency     := lVlrUltParc;
                 cds.FieldByName('VLR_TITULO').AsCurrency := lVlrUltParc;
                 if lVlrUltParc = 0 then
                   begin
                     cds.Cancel;
                     ShowMessage('No  permitido parcela com valor 0.');
                     abort;
                   end;

                 if (dmGeral.FIN_CD_M_MCH.FieldByName('OPERACAO').AsInteger=4) then
                   begin
                      try
                        cds.FieldByName('VLR_JUROS').OnChange := nil;
                        cds.FieldByName('VLR_TITULO').OnChange := nil;
                        cds.FieldByName('VLR_JUROS').AsCurrency := lVlrJurosUltParc;
                        cds.FieldByName('VLR_TOTAL').AsCurrency :=  cds.FieldByName('VLR_TITULO').AsCurrency+
                                                                   cds.FieldByName('VLR_JUROS').AsCurrency;
                      finally
                        cds.FieldByName('VLR_JUROS').OnChange := FIN_CD_M_MCH_TITvlr_jurosChange;
                        cds.FieldByName('VLR_TITULO').OnChange := FIN_CD_M_MCH_TITvlr_tituloChange;
                      end;

                   end;

               end;
           end;

        if origem in [0,2,4,5,6] then // Pedido de Venda, Ordem de sada,  Fechamento de Ordem de Servio e Pedido Cupon - FAT_FM_M_PNF
           begin
             if i < qtde_parcelas then
               begin
                 cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrParcela;
                 if lVlrParcela = 0 then
                    begin
                      cds.Cancel;
                      ShowMessage('No  permitido parcela com valor 0.');
                      abort;
                    end;
               end
             else
               begin
                 cds.FieldByName('VLR_TITULO').AsCurrency     := lVlrUltParc;
                 if lVlrUltParc = 0 then
                    begin
                      cds.Cancel;
                      ShowMessage('No  permitido parcela com valor 0.');
                      abort;
                    end;
               end;

             if (origem = 0) then
               cds.FieldByName('BOL_NOSSO_NUMERO').text     := '';
             if (origem = 2) then
               cds.FieldByName('num_parcela').AsString       := '';
           end;
        cds.Post;
      end;
  cds.First;
  cds.Edit;
end;



procedure TdmGeral.GerarFPG_Entrada(origem: integer; id_forma_fpg,id_plano: string; valor: Currency;
  cds: TClientDataSet);
var
  SMPrincipal: TSMClient;
begin

  { origem
        1 - Pedido de venda
        2 - Nota fiscal
        3 - Ordem de sada
        4 - Orde de Sada (Oficina)
        5 - Fechamento de Ordem de Servio
  }

  cds.Cancel;
  if not cds.IsEmpty then
     begin
       ShowMessage('No  possvel gerar a "entrada" pois j existe vencimento lanado.');
       cds.edit;
       abort;
     end;

  cds.Insert;

  // O id_titulo est no OnNewRecord do cds.

  cds.FieldByName('ID_FORMA_PAG').AsInteger    := strtoint(id_forma_fpg);
  dmGeral.BusFormaPgtos(0,cds.FieldByName('ID_FORMA_PAG').text);
  cds.FieldByName('int_nomefpg').AsString      := BUS_CD_C_FPG.FieldByName('DESCRICAO').AsString;
  cds.FieldByName('DIAS').AsInteger            := 0;
  cds.FieldByName('DTA_VENCIMENTO').AsDateTime := xDataSis;
  cds.FieldByName('VLR_TITULO').AsCurrency     := valor;
  cds.FieldByName('CHE_BANCO').text            := '';
  cds.FieldByName('CHE_AGENCIA').text          := '';
  cds.FieldByName('CHE_CONTA').AsString        := '';
  cds.FieldByName('CHE_NUMERO').AsInteger      := 0;
  cds.FieldByName('CHE_EMITENTE').text         := '';

    { origem
        1 - Pedido de venda
        2 - Nota fiscal
        3 - Ordem de sada
        4 - Orde de Sada (Oficina)
        5 - Fechamento de Ordem de Servio
  }

  if origem = 1 then
    begin
      cds.FieldByName('BOL_NOSSO_NUMERO').text := '';
      cds.FieldByName('INT_DOCIMPRESSO').AsInteger := BUS_CD_C_FPG.FieldByName('DOC_IMPRESSO').AsInteger;
    end
  else
    if origem in [2,3,4] then
      cds.FieldByName('num_parcela').AsString    := '';

  if origem = 2 then
     begin
       SMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);
       try
          cds.FieldByName('id_nfe_tit').AsInteger :=
               SMPrincipal.enValorChave('FAT_TB_M_NFE_TIT');
       finally
          FreeAndNil(SMPrincipal);
       end;
       cds.FieldByName('ID_PLANO').text := id_plano;
       if trim(id_plano) <> '' then
          begin
            dmGeral.BusPlanoCta(0,id_plano);
            cds.FieldByName('INT_NOMEPCT').Text := dmGeral.BUS_CD_C_PCT.FieldByName('DESCRICAO').AsString;
            dmGeral.BUS_CD_C_PCT.close;
          end;
     end;

  cds.Post;
  cds.Edit;
end;


procedure TdmGeral.Grava(Tabela: TClientDataSet);
var
  retorno: integer;
begin
   try
     Tabela.Post;
     try
        retorno := Tabela.ApplyUpdates(0);
     if retorno = 0 then
        begin
          Tabela.Close;
          Tabela.Open;
          Showmessage('Gravao realizada com sucesso.');
        end
     finally
       if retorno <> 0 then
          Tabela.edit;
     end;
   except
     on e:EReconcileError do
       begin
         showmessage(e.Message);
       end;
   end;
end;


