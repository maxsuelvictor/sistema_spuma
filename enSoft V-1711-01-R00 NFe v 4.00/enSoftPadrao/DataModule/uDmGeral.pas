function TdmGeral.PCP_CD_M_PSC_TesCampos: Boolean;
var
  mens: String;
begin
  result := true;

  mens := '';

  if PCP_CD_M_PSC.FieldByName('ID_PSC').AsInteger = 0 then
     begin
       mens := mens + '.Controle' + #13;
     end;

  if PCP_CD_M_PSC.FieldByName('ID_EMPRESA').AsInteger = 0 then
     begin
       mens := mens + '.Empresa' + #13;
     end;

  if trim(PCP_CD_M_PSC.FieldByName('NOME_CLIENTE').text) = '' then
     begin
       mens := mens + '.Cliente' + #13;
     end;

  if trim(PCP_CD_M_PSC.FieldByName('DATA').text) = '' then
     begin
       mens := mens + '.Data' + #13;
     end;

  if trim(PCP_CD_M_PSC.FieldByName('CIDADE').text) = '' then
     begin
       mens := mens + '.Cidade' + #13;
     end;

  if trim(PCP_CD_M_PSC.FieldByName('ESTADO').text) = '' then
     begin
       mens := mens + '.Estado' + #13;
     end;

  if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_1').text) = '') then
      begin
         mens := mens + '.Voc comprar ou utilizar o colcho da Colches GLOBO novamente?'+#13;
      end
  else
     begin
       if PCP_CD_M_PSC.FieldByName('PERGUNTA_1').AsInteger = 0 then
          begin
            if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_1_RESPOSTA').text) = '') then
                begin
                  mens := mens + '.1 - Por que no?'+#13;
                end;
          end;
     end;

  if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_2').text) = '') then
      begin
         mens := mens + '.Voc recomendaria o colcho da Colches GLOBO para outras pessoas?'+#10+#13;
      end
  else
      begin
       if PCP_CD_M_PSC.FieldByName('PERGUNTA_2').AsInteger = 0 then
          begin
            if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_2_RESPOSTA').text) = '') then
                begin
                  mens := mens + '.2 - Por que no?'+#10+#13;
                end;
          end;
      end;

  if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_3').text) = '') then
      mens := mens + '.H quanto tempo o Sr.(a) vem utilizando o colcho de Espuma da Colches GLOBO?'+#10+#13;

  if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_4').text) = '') then
      mens := mens + '.Em comparao com outras alternativas de colches, o colcho GLOBO :'+#10+#13;

  if (trim(PCP_CD_M_PSC.FieldByName('PERGUNTA_5').text) = '') then
      mens := mens + '.Qual seu grau de satisfao geral com o seu colcho da Colhes GLOBO?'+#10+#13;

  if mens <> '' then
     begin
       ShowMessage('Os campos abaixos devem ser preenchidos:' + #13 + mens);
       mens := '';
       result := false;
     end
  else
     begin

       if mens <> '' then
          begin
            ShowMessage('Ateno:' + #13 + mens);
            mens := '';
            result := false;
          end
     end;
end;

