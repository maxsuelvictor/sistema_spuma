  BUS_CD_X_VEI_RODDESCRICAO.Text := 'Toco';

  BUS_CD_X_VEI_ROD.Insert;
  BUS_CD_X_VEI_RODID_ROD.Text    := '3';
  BUS_CD_X_VEI_RODDESCRICAO.Text := 'Cavalo Mecanico';

  BUS_CD_X_VEI_ROD.Insert;
  BUS_CD_X_VEI_RODID_ROD.Text    := '4';
  BUS_CD_X_VEI_RODDESCRICAO.Text := 'VAN';

  BUS_CD_X_VEI_ROD.Insert;
  BUS_CD_X_VEI_RODID_ROD.Text    := '5';
  BUS_CD_X_VEI_RODDESCRICAO.Text := 'Utilitario';

