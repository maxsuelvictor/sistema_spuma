function TdmGeral.RemoveCaracteres(Campo: String): String;
Var Resultado1:String;
    Resultado2:String;
    Resultado3:String;
    Resultado4:String;
    Resultado5:String;
    Resultado6:String;
    Resultado7:String;
    x:Integer;
    Letra:String;
Begin
  Letra:=' ';
  For x:=1 to Length(Campo) do
    begin
      Letra:=Copy(Campo,x,1);
      if Letra<>'.' Then
         Begin
           Resultado1:=Resultado1+Letra;
         end;
    end;
  Letra:=' ';
  For x:=1 to Length(Resultado1) do
    begin
      Letra:=Copy(Resultado1,x,1);
      if Letra<>'-' Then
         Begin
           Resultado2:=Resultado2+Letra;
         end;
    end;
  Letra:=' ';
  For x:=1 to Length(Resultado2) do
    begin
      Letra:=Copy(Resultado2,x,1);
      if Letra<>'-' Then
         Begin
           Resultado3:=Resultado3+Letra;
         end;
    end;
  Letra:=' ';
  For x:=1 to Length(Resultado3) do
    begin
      Letra:=Copy(Resultado3,x,1);
      if Letra<>'/' Then
         Begin
           Resultado4:=Resultado4+Letra;
         end;
    end;

  Letra:=' ';
  For x:=1 to Length(Resultado4) do
    begin
      Letra:=Copy(Resultado4,x,1);
      if Letra<>',' Then
         Begin
           Resultado5:=Resultado5+Letra;
         end;
    end;
  result:=Resultado5;
end;




procedure TdmGeral.SaiGravaFea(const lorigem: integer; const cds_pai, cds_itens,
  cds_itens_ant: TClientDataSet; var lCond_Item, lCond_Cor, lCond_Tamanho,
  lcampo: String; lIndex: Integer);
var
  lData,lCampoSeq,lNumeroDoc: String;
  lVlrCustoMedio: Currency;
  lIndexFea: Integer;
begin

  { lOrigem:
     0 - Por Nota fiscal de entrada
     1 - Por Nota fiscal de sada
     2 - Por Ordem de sada
     3 - Por almoxarifado - Sada
     4 - Por inventrio
     5 - Por almoxarifado - Devolucao
     6 - Por Pedido de Venda
  }

  case lOrigem of
     1:
       begin
         lCampoSeq  := 'ID_SEQUENCIA';
         lData      := 'DTA_DOCUMENTO';
         lNumeroDoc := 'NUMERO';
         lIndexFea  := 2;
       end;
     3:
       begin
         lCampoSeq  := 'ID_SEQUENCIA';
         lData      := 'DATA';
         lNumeroDoc := 'ID_MAL';
         lIndexFea  := 3;
       end;
     6:
       begin
         lCampoSeq  := 'ID_SEQUENCIA';
         lData      := 'dta_pedido';
         lNumeroDoc := 'ID_PEDIDO';
         lIndexFea  := 1;
       end;
  end;


  { Filtros do BUS_CD_M_FEA:
    index 0, Empresa, Item               id_item
    index 1, Empresa, Pedido             id_pedido
    index 2, Empresa, Nota fiscal        id_fiscal
    index 3, Empresa, Mov. Almoxarifado  id_mal }


  EST_CD_M_FEA.Close;
  EST_CD_M_FEA.Data :=
  EST_CD_M_FEA.DataRequest(VarArrayOf([lIndexFea, cds_pai.FieldByName('ID_EMPRESA').text, cds_pai.FieldByName(lCampo).text]));


  { BUSCAR A MOVIMENTAO DO ESTOQUE}
  cds_itens.First;
  while not cds_itens.Eof do
      begin
        if cds_itens.FieldByName('INT_TIPO_ITEM').AsInteger <> 9 then // Se no for item do tipo SERVIO.
           begin

              // No precisa verificar a cor e tamanho por que o ID_SEQ_ITEM  nico.
              if EST_CD_M_FEA.Locate('ID_EMPRESA;'+lCampo+' ;ID_ITEM;ID_SEQ_ITEM',
                              VarArrayOf([cds_pai.FieldByName('ID_EMPRESA').AsString,
                                          cds_itens.FieldByName(lCampo).AsString,
                                          cds_itens.FieldByName('ID_ITEM').AsString,
                                          cds_itens.FieldByName(lCampoSeq).AsString]),[]) then
                 begin

                    EST_CD_M_FEA.edit;

                    // No precisa verificar a cor e tamanho por que o ID_SEQ_ITEM  nico.
                    if (cds_itens_ant <> nil) and (cds_itens_ant.Locate(lCampo+';ID_ITEM;ID_SEQUENCIA',
                                                        VarArrayOf([cds_itens.FieldByName(lCampo).AsString,
                                                                    cds_itens.FieldByName('ID_ITEM').AsString,
                                                                    cds_itens.FieldByName(lCampoSeq).AsString]),[])) then
                       begin

                          if cds_itens_ant.FieldByName('QTDE').AsFloat <> cds_itens.FieldByName('QTDE').AsFloat  then
                             begin
                                EST_CD_M_FEA.FieldByName('QTDE').AsFloat :=
                                        (EST_CD_M_FEA.FieldByName('QTDE').AsFloat -

                                         cds_itens_ant.FieldByName('QTDE').AsFloat) +
                                               cds_itens.FieldByName('QTDE').AsFloat;
                             end;
                       end
                    else
                       begin
                          EST_CD_M_FEA.FieldByName('QTDE').AsFloat :=
                                  EST_CD_M_FEA.FieldByName('QTDE').AsFloat +
                                         cds_itens.FieldByName('QTDE').AsFloat;
                       end;

                    lVlrCustoMedio := 0;

                    if lOrigem <> 6 then // Se for de Pedido de Venda
                       begin
                         EST_CD_M_FEA.FieldByName('NUM_LOTE').text       :=
                             cds_itens.FieldByName('NUM_LOTE').text;
                       end;

                    EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency     := 0;
                    EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency := 0;

                    if lorigem in [1] then // NF entradas ou sadas
                       begin
                         EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency  :=
                               cds_itens.FieldByName('VLR_LIQUIDO').AsCurrency;
                         EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency :=
                               EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency;
                       end;


                    if lorigem = 3 then // Sada por almoxarifado
                       begin
                         lVlrCustoMedio := 0;
                         if BusCustoMedItem(cds_pai.FieldByName('ID_EMPRESA').AsString,
                                            cds_itens.FieldByName('ID_ITEM').AsString,
                                            cds_itens.FieldByName('ID_COR').AsString,
                                            cds_itens.FieldByName('ID_TAMANHO').AsString,
                                            lVlrCustoMedio) then
                            begin
                              if lVlrCustoMedio > 0 then
                                 begin
                                   EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency     :=
                                            roundTo(cds_itens.FieldByName('QTDE').AsFloat * lVlrCustoMedio,-2);
                                   EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency :=
                                            EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency;
                                 end;
                            end;
                       end;
                    EST_CD_M_FEA.Post;
                 end
              else
                 begin

                   // Criando a Ficha de Estoque
                   EST_CD_M_FEA.Insert;

                   EST_CD_M_FEA.FieldByName('ID_EMPRESA').AsInteger :=
                           cds_pai.FieldByName('ID_EMPRESA').AsInteger;

                   EST_CD_M_FEA.FieldByName('ID_ITEM').AsInteger :=
                           cds_itens.FieldByName('ID_ITEM').AsInteger;

                   EST_CD_M_FEA.FieldByName('ID_COR').text :=
                           cds_itens.FieldByName('ID_COR').text;

                   EST_CD_M_FEA.FieldByName('ID_TAMANHO').text :=
                           cds_itens.FieldByName('ID_TAMANHO').text;

                   EST_CD_M_FEA.FieldByName('ID_SEQ_ITEM').Asfloat :=
                           cds_itens.FieldByName(lCampoSeq).Asfloat;

                   EST_CD_M_FEA.FieldByName('DATA').AsDateTime :=
                           cds_pai.FieldByName(lData).AsDateTime;

                   EST_CD_M_FEA.FieldByName('ID_TIPO_MOV_ESTOQUE').AsInteger :=
                           cds_pai.FieldByName('ID_TIPO_MOV_ESTOQUE').AsInteger;

                   EST_CD_M_FEA.FieldByName('ORIGEM').AsInteger := lOrigem;

                   if CAD_CD_C_PAR_CTR.FieldByName('UTILIZA_ALMOXARIFADO').AsBoolean = true then
                      begin
                        EST_CD_M_FEA.FieldByName('ID_ALMOXARIFADO').AsInteger :=
                           cds_pai.FieldByName('id_almoxarifado').AsInteger;
                      end;

                   EST_CD_M_FEA.FieldByName('ID_PEDIDO').AsInteger   := 0;
                   EST_CD_M_FEA.FieldByName('DOCUMENTO').AsInteger   := cds_pai.FieldByName(lNumeroDoc).AsInteger;
                   EST_CD_M_FEA.FieldByName('ID_CONTROLE').AsInteger := 0;
                   EST_CD_M_FEA.FieldByName('ID_MAL').AsInteger      := 0;
                   EST_CD_M_FEA.FieldByName('ID_FISCAL').AsInteger   := 0;

                   if lOrigem <> 6 then // Se for de Pedido de Venda
                      begin
                        EST_CD_M_FEA.FieldByName('NUM_LOTE').text            :=
                            cds_itens.FieldByName('NUM_LOTE').text;
                      end;

                   case lOrigem of
                      1:
                        begin
                          EST_CD_M_FEA.FieldByName('ID_FISCAL').AsInteger     :=
                              cds_pai.FieldByName('ID_FISCAL').AsInteger;

                          EST_CD_M_FEA.FieldByName('PER_DESCONTO').AsCurrency :=
                              cds_itens.FieldByName('PER_DESCONTO').AsCurrency;

                          EST_CD_M_FEA.FieldByName('PER_FRETE').AsCurrency :=
                              cds_itens.FieldByName('FRE_PERCENTUAL').AsCurrency;

                          EST_CD_M_FEA.FieldByName('VLR_FRETE').AsCurrency :=
                              cds_itens.FieldByName('FRE_VALOR').AsCurrency;

                          EST_CD_M_FEA.FieldByName('VLR_DESCONTO').AsCurrency :=
                              cds_itens.FieldByName('VLR_DESCONTO').AsCurrency;

                          EST_CD_M_FEA.FieldByName('VLR_CUSTO').AsCurrency :=
                              cds_itens.FieldByName('VLR_CUSTO').AsCurrency;
                        end;
                      3:
                        begin
                          EST_CD_M_FEA.FieldByName('ID_MAL').AsInteger       := cds_pai.FieldByName('ID_MAL').AsInteger;
                        end;
                   end;


                   EST_CD_M_FEA.FieldByName('QTDE').AsFloat :=  cds_itens.FieldByName('QTDE').AsFloat;


                  if lorigem = 1 then // Sada por Nota de Sada
                     begin
                        EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency :=
                           cds_itens.FieldByName('VLR_MERCADORIA').AsCurrency;

                        EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency :=
                           cds_itens.FieldByName('VLR_LIQUIDO').AsCurrency;
                     end;

                  if lorigem = 3 then // Sada por almoxarifado
                     begin
                       lVlrCustoMedio := 0;
                       if BusCustoMedItem(cds_pai.FieldByName('ID_EMPRESA').AsString,
                                          cds_itens.FieldByName('ID_ITEM').AsString,
                                          cds_itens.FieldByName('ID_COR').AsString,
                                          cds_itens.FieldByName('ID_TAMANHO').AsString,
                                          lVlrCustoMedio) then
                          begin
                            if lVlrCustoMedio > 0 then
                               begin
                                 EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency     :=
                                          roundTo(cds_itens.FieldByName('QTDE').AsFloat * lVlrCustoMedio,-2);
                                 EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency :=
                                          EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency;
                               end;
                          end;
                     end;

                  if lorigem = 6 then // Sada por Pedido de Venda
                     begin
                       EST_CD_M_FEA.FieldByName('PER_DESCONTO').AsCurrency :=
                           cds_itens.FieldByName('PER_DESCONTO').AsCurrency;

                       EST_CD_M_FEA.FieldByName('PER_FRETE').AsCurrency := 0;

                       EST_CD_M_FEA.FieldByName('VLR_DESCONTO').AsCurrency :=
                               cds_itens.FieldByName('VLR_DESCONTO').AsCurrency;

                       EST_CD_M_FEA.FieldByName('VLR_FRETE').AsCurrency := 0;

                       EST_CD_M_FEA.FieldByName('VLR_CUSTO').AsCurrency := 0;

                       EST_CD_M_FEA.FieldByName('ID_PEDIDO').AsInteger :=
                               cds_itens.FieldByName('ID_PEDIDO').AsInteger;

                        EST_CD_M_FEA.FieldByName('VLR_BRUTO').AsCurrency :=
                           cds_itens.FieldByName('VLR_BRUTO').AsCurrency;

                        EST_CD_M_FEA.FieldByName('VLR_MOVIMENTO').AsCurrency :=
                           cds_itens.FieldByName('VLR_LIQUIDO').AsCurrency;
                     end;
                   EST_CD_M_FEA.Post;
                 end;
           end;
        cds_itens.Next;
      end;


   // Caso algum produto tenha sido retirado do pedido de venda, o registro deste item deve ser retirado da tabela de movimento.
   if (cds_itens_ant <> nil) and (not cds_itens_ant.IsEmpty) then
      begin
        cds_itens_ant.First;
        while not cds_itens_ant.eof do
           begin
             if not cds_itens.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                          VarArrayOf([cds_itens_ant.FieldByName(lCampo).AsString,
                                      cds_itens_ant.FieldByName('ID_ITEM').AsString,
                                      cds_itens_ant.FieldByName(lCampoSeq).AsString]),[]) then
                begin
                  if EST_CD_M_FEA.Locate('ID_EMPRESA;'+lCampo+';ID_ITEM;ID_SEQ_ITEM',
                                 VarArrayOf([cds_pai.FieldByName('ID_EMPRESA').AsString,
                                             cds_itens_ant.FieldByName(lCampo).AsString,
                                             cds_itens_ant.FieldByName('ID_ITEM').AsString,
                                             cds_itens_ant.FieldByName(lCampoSeq).AsString]),[]) then
                     begin
                       EST_CD_M_FEA.delete;
                     end;
                end;
             cds_itens_ant.next;
           end;
      end;
end;

procedure TdmGeral.SaiGravaFes(const lorigem: integer; const cds_pai, cds_itens,
  cds_itens_ant: TClientDataSet; var lCond_Item, lCond_Cor, lCond_Tamanho,
  lCond_Lote, lcampo: String; lIndex: Integer);
var
  lAchou_Registro: Boolean;
  lData,lCampoSeq,lNumeroDoc,lCampoQtde: String;
  lVlrCustoMedio: Currency;
  lIndexLot: integer;
begin

  { lOrigem:
     0 - Por Nota fiscal de entrada
     1 - Por Nota fiscal de sada
     2 - Por Ordem de sada
     3 - Por almoxarifado - Sada
     4 - Por inventrio
     5 - Por almoxarifado - Devolucao
  }

  case lOrigem of
     1:
       begin
         lCampoSeq  := 'ID_SEQUENCIA';
         lData      := 'DTA_DOCUMENTO';
         lNumeroDoc := 'NUMERO';
         lCampoQtde := 'QTDE';
       end;
     3:
       begin
         lCampoSeq  := 'ID_SEQUENCIA';
         lData      := 'DATA';
         lNumeroDoc := 'ID_MAL';
         lCampoQtde := 'QTDE';
       end;
     6:
       begin
         lCampo     := 'ID_PEDIDO';
         lCampoSeq  := 'ID_SEQUENCIA';
         lNumeroDoc := 'ID_PEDIDO';
         lCampoQtde := 'QTDE';
       end;
  end;



  // Trabalhando com Tabela de Saldos -----------------------------------------

  { Filtros do EST_CD_M_FES:
      Index 0 - Empresa, Item
      Index 1 - Empresa, Item, Cor
      Index 2 - Empresa, Item, Tamanho
      Index 3 - Empresa, Item, Cor,Tamanho
      Index 4 - empresa, in(item),  in(cores)
      Index 5 - empresa, in(item),  in(tamanho)
      Index 6 - empresa, int(item), in(cor) in(tamanho)
      Index 7 - empresa, in(item)
      }

     EST_CD_M_FES.Close;
     EST_CD_M_FES.Data :=
             EST_CD_M_FES.DataRequest(VarArrayOf([lIndex, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Item,lCond_Cor, lCond_Tamanho]));

     if CAD_CD_C_PAR_CTRUTILIZA_ALMOXARIFADO.AsBoolean then
        begin
          EST_CD_M_FES_ALM.close;

          if lIndex = 7 then
             begin
               EST_CD_M_FES_ALM.Data :=
                  EST_CD_M_FES_ALM.DataRequest(VarArrayOf([lIndex, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Item,cds_pai.FieldByName('ID_ALMOXARIFADO').text]));
             end;
          if lIndex = 4 then
             begin
               EST_CD_M_FES_ALM.Data :=
                  EST_CD_M_FES_ALM.DataRequest(VarArrayOf([lIndex, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Item,lCond_Cor,cds_pai.FieldByName('ID_ALMOXARIFADO').text]));
             end;
          if lIndex = 5 then
             begin
               EST_CD_M_FES_ALM.Data :=
                  EST_CD_M_FES_ALM.DataRequest(VarArrayOf([lIndex, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Item,lCond_Tamanho,cds_pai.FieldByName('ID_ALMOXARIFADO').text]));
             end;
          if lIndex = 6 then
             begin
               EST_CD_M_FES_ALM.Data :=
                  EST_CD_M_FES_ALM.DataRequest(VarArrayOf([lIndex, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Item,lCond_Cor,lCond_Tamanho,cds_pai.FieldByName('ID_ALMOXARIFADO').text]));
             end;
        end;

      //if CAD_CD_C_PAR_CTRutiliza_lote_na_entrada.AsBoolean then
      if trim(lCond_Lote) <> '' then
         begin
           { Filtros do EST_CD_M_LOT
             8-  impresa + in(num lote) + in(item)
             9-  impresa + in(num lote) + in(item) + in(cor)
             10- impresa + in(num lote) + in(item) + in(cor) + in(tamanho)
             11- impresa + in(num lote) + in(item) + in(tam)

             12- impresa + in(num lote) + in(item) + almoxarifado
             13- impresa + in(num lote) + in(item) + in(cor) + almoxarifado
             14- impresa + in(num lote) + in(item) + in(tam) + almoxarifado
             15- impresa + in(num lote) + in(item) + in(cor) + in(tam) + almoxarifado
           }
           if lIndex = 7 then
              if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean then
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([12, cds_pai.FieldByName('ID_EMPRESA').text,
                                                              lCond_Lote,lCond_Item,cds_pai.FieldByName('ID_ALMOXARIFADO').text]))
              else
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([8, cds_pai.FieldByName('ID_EMPRESA').text,lCond_Lote,lCond_Item]));


           if lIndex = 4 then
              if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean then
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([13, cds_pai.FieldByName('ID_EMPRESA').text,
                                                              lCond_Lote,lCond_Item,lCond_Cor,
                                                              cds_pai.FieldByName('ID_ALMOXARIFADO').text]))
              else
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([9, cds_pai.FieldByName('ID_EMPRESA').text,
                                                              lCond_Lote,lCond_Item,lCond_Cor]));

           if lIndex = 5 then
              if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean then
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([14, cds_pai.FieldByName('ID_EMPRESA').text,
                                                              lCond_Lote,lCond_Item,lCond_Tamanho,
                                                              cds_pai.FieldByName('ID_ALMOXARIFADO').text]))
              else
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([11, cds_pai.FieldByName('ID_EMPRESA').text,
                                                              lCond_Lote,lCond_Item,lCond_Tamanho]));

           if lIndex = 6 then
              if CAD_CD_C_PAR_CTRutiliza_almoxarifado.AsBoolean then
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([15, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Lote,lCond_Item,lCond_Cor,lCond_Tamanho,cds_pai.FieldByName('ID_ALMOXARIFADO').text]))
              else
                 EST_CD_M_LOT.Data :=
                     EST_CD_M_LOT.DataRequest(VarArrayOf([10, cds_pai.FieldByName('ID_EMPRESA').text, lCond_Lote,lCond_Item,lCond_Cor,lCond_Tamanho]));
         end;

     cds_itens.First;
     while not cds_itens.Eof do
        begin
          lAchou_Registro :=  localizar_FES(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                    cds_itens.FieldByName('ID_ITEM').text,
                                                    cds_itens.FieldByName('ID_COR').text,
                                                    cds_itens.FieldByName('ID_TAMANHO').text);
          if lAchou_Registro then
             begin
                EST_CD_M_FES.edit;

                if (cds_itens_ant <> nil) and (cds_itens_ant.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                          VarArrayOf([cds_itens.FieldByName(lCampo).AsString,
                                      cds_itens.FieldByName('ID_ITEM').AsString,
                                      cds_itens.FieldByName(lCampoSeq).AsString]),[])) then
                   begin

                      if (cds_itens_ant.FieldByName('ID_COR').text     <> cds_itens.FieldByName('ID_COR').text) or
                         (cds_itens_ant.FieldByName('ID_TAMANHO').text <> cds_itens.FieldByName('ID_TAMANHO').text) then
                         begin
                           lAchou_Registro :=  localizar_FES(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                               cds_itens_ant.FieldByName('ID_ITEM').text,
                                               cds_itens_ant.FieldByName('ID_COR').text,
                                               cds_itens_ant.FieldByName('ID_TAMANHO').text);
                           if lAchou_Registro then
                              begin
                                EST_CD_M_FES.edit;

                                EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat :=
                                    (EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat -
                                     cds_itens_ant.FieldByName(lCampoQtde).AsFloat);

                                EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat :=
                                    (EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat +
                                     cds_itens_ant.FieldByName(lCampoQtde).AsFloat);


                                EST_CD_M_FES.Post;

                                lAchou_Registro := localizar_FES(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                         cds_itens.FieldByName('ID_ITEM').text,
                                                                         cds_itens.FieldByName('ID_COR').text,
                                                                         cds_itens.FieldByName('ID_TAMANHO').text);

                                if lAchou_Registro then
                                   begin
                                     EST_CD_M_FES.edit;

                                     EST_CD_M_FES.FieldByName('data_ult_movimento').AsDateTime := xDataSis;

                                     if lOrigem = 1 then // NF Entrada
                                        begin
                                           EST_CD_M_FES.FieldByName('vlr_custo_medio').AsCurrency :=
                                               cds_itens.FieldByName('VLR_CUSTO').AsCurrency;
                                        end;

                                     EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat :=
                                         EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat +
                                                    cds_itens.FieldByName(lCampoQtde).AsFloat;

                                     EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat :=
                                           EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat -
                                           cds_itens.FieldByName(lCampoQtde).AsFloat;

                                   end;
                              end;
                         end;


                      localizar_FES(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                            cds_itens.FieldByName('ID_ITEM').text,
                                            cds_itens.FieldByName('ID_COR').text,
                                            cds_itens.FieldByName('ID_TAMANHO').text);

                      if cds_itens_ant.FieldByName(lCampoQtde).AsFloat <> cds_itens.FieldByName(lCampoQtde).AsFloat  then
                         begin
                            EST_CD_M_FES.edit;
                            EST_CD_M_FES.FieldByName('data_ult_movimento').AsDateTime := xDataSis;

                            if lOrigem = 1 then // NF Entrada
                               begin
                                 EST_CD_M_FES.FieldByName('vlr_custo_medio').AsCurrency    :=
                                     cds_itens.FieldByName('VLR_CUSTO').AsCurrency;
                               end;

                            EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat :=
                                    (EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat -

                                     cds_itens_ant.FieldByName(lCampoQtde).AsFloat) +

                                           cds_itens.FieldByName(lCampoQtde).AsFloat;

                            EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat :=
                                    (EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat +

                                     cds_itens_ant.FieldByName(lCampoQtde).AsFloat) -

                                            cds_itens.FieldByName(lCampoQtde).AsFloat;

                            EST_CD_M_FES.Post;
                         end;
                   end
                else
                   begin
                      EST_CD_M_FES.FieldByName('data_ult_movimento').AsDateTime := xDataSis;
                      if lOrigem = 1 then // NF Entrada
                         begin
                           EST_CD_M_FES.FieldByName('vlr_custo_medio').AsCurrency    :=
                               cds_itens.FieldByName('VLR_CUSTO').AsCurrency;
                         end;

                      EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat :=
                              EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat +
                                     cds_itens.FieldByName(lCampoQtde).AsFloat;

                      EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat :=
                              EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat -
                                      cds_itens.FieldByName(lCampoQtde).AsFloat;
                      EST_CD_M_FES.Post;
                   end;

                // Caso utiliza o almoxarifado
                if CAD_CD_C_PAR_CTRUTILIZA_ALMOXARIFADO.AsBoolean = true then
                   begin

                     // Caso a tabela de saldo do produto por almoxarifado j exista
                     lAchou_Registro :=  localizar_FES_ALM(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                   cds_itens.FieldByName('ID_ITEM').text,
                                                                   cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                   cds_itens.FieldByName('ID_COR').text,
                                                                   cds_itens.FieldByName('ID_TAMANHO').text);

                     if lAchou_Registro then
                        begin
                          EST_CD_M_FES_ALM.edit;

                          if (cds_itens_ant <> nil) and (cds_itens_ant.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                                                                        VarArrayOf([cds_itens.FieldByName(lCampo).AsString,
                                                                                    cds_itens.FieldByName('ID_ITEM').AsString,
                                                                                    cds_itens.FieldByName(lCampoSeq).AsString]),[])) then
                             begin

                                if (cds_itens_ant.FieldByName('ID_COR').text <> cds_itens.FieldByName('ID_COR').text) or
                                   (cds_itens_ant.FieldByName('ID_TAMANHO').text <> cds_itens.FieldByName('ID_TAMANHO').text) then
                                   begin
                                     lAchou_Registro :=  localizar_FES_ALM(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                                   cds_itens_ant.FieldByName('ID_ITEM').text,
                                                                                   cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                                   cds_itens_ant.FieldByName('ID_COR').text,
                                                                                   cds_itens_ant.FieldByName('ID_TAMANHO').text);
                                     if lAchou_Registro then
                                        begin
                                          EST_CD_M_FES_ALM.edit;

                                          EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat :=
                                             (EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat -
                                                  cds_itens_ant.FieldByName(lCampoQtde).AsFloat);

                                          EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat :=
                                              (EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat +
                                               cds_itens_ant.FieldByName(lCampoQtde).AsFloat);


                                          EST_CD_M_FES_ALM.Post;


                                          lAchou_Registro := localizar_FES_ALM(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                                       cds_itens.FieldByName('ID_ITEM').text,
                                                                                       cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                                       cds_itens.FieldByName('ID_COR').text,
                                                                                       cds_itens.FieldByName('ID_TAMANHO').text);
                                          if lAchou_Registro then
                                             begin
                                                EST_CD_M_FES_ALM.Edit;

                                                EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat :=
                                                         EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat +
                                                                cds_itens.FieldByName(lCampoQtde).AsFloat;

                                                EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat :=
                                                         EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat -
                                                                 cds_itens.FieldByName(lCampoQtde).AsFloat;

                                                EST_CD_M_FES_ALM.post;
                                             end;
                                        end;
                                   end;


                               localizar_FES_ALM(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                         cds_itens.FieldByName('ID_ITEM').text,
                                                         cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                         cds_itens.FieldByName('ID_COR').text,
                                                         cds_itens.FieldByName('ID_TAMANHO').text);

                               if cds_itens_ant.FieldByName(lCampoQtde).AsFloat <> cds_itens.FieldByName(lCampoQtde).AsFloat  then
                                  begin
                                    EST_CD_M_FES_ALM.edit;

                                    EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat :=
                                            (EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat -
                                                   cds_itens_ant.FieldByName(lCampoQtde).AsFloat) +
                                                   cds_itens.FieldByName(lCampoQtde).AsFloat;

                                    EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat :=
                                            (EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat +
                                                    cds_itens_ant.FieldByName(lCampoQtde).AsFloat) -
                                                    cds_itens.FieldByName(lCampoQtde).AsFloat;

                                    EST_CD_M_FES_ALM.Post;
                                  end;
                             end
                          else
                             begin
                                EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat :=
                                        EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat +
                                               cds_itens.FieldByName(lCampoQtde).AsFloat;

                                EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat :=
                                        EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat -
                                                cds_itens.FieldByName(lCampoQtde).AsFloat;

                                EST_CD_M_FES_ALM.post;
                             end;
                        end
                   end;
                 // ----- Fim parte do saldo do almoxarifado


                // Caso utiliza lote
                if CAD_CD_C_PAR_CTRutiliza_lote_na_entrada.AsBoolean = true then
                   begin
                     if not (lOrigem in [1,2,6]) then // S entra se for diferente de pedido de venda, ordem de sada e nota de sada
                        begin
                           if (trim(cds_itens.FieldByName('NUM_LOTE').text) <> '') and
                              (trim(cds_itens.FieldByName('NUM_LOTE').text) <> '0') then
                              begin
                               // Caso existe o registro de saldo de item por lote j exista
                               lAchou_Registro :=  localizar_LOT(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                             cds_itens.FieldByName('ID_ITEM').text,
                                                                             cds_itens.FieldByName('NUM_LOTE').text,
                                                                             cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                             cds_itens.FieldByName('ID_COR').text,
                                                                             cds_itens.FieldByName('ID_TAMANHO').text);

                               if lAchou_Registro then
                                  begin
                                    EST_CD_M_LOT.edit;

                                    if (cds_itens_ant <> nil) and (cds_itens_ant.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                                                                                  VarArrayOf([cds_itens.FieldByName(lCampo).AsString,
                                                                                              cds_itens.FieldByName('ID_ITEM').AsString,
                                                                                              cds_itens.FieldByName(lCampoSeq).AsString]),[])) then
                                       begin

                                          if (cds_itens_ant.FieldByName('ID_COR').text <> cds_itens.FieldByName('ID_COR').text) or
                                             (cds_itens_ant.FieldByName('ID_TAMANHO').text <> cds_itens.FieldByName('ID_TAMANHO').text) then
                                             begin
                                               lAchou_Registro :=  localizar_LOT(lIndex,  cds_pai.FieldByName('ID_EMPRESA').text,
                                                                                          cds_itens_ant.FieldByName('ID_ITEM').text,
                                                                                          cds_itens_ant.FieldByName('NUM_LOTE').text,
                                                                                          cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                                          cds_itens_ant.FieldByName('ID_COR').text,
                                                                                          cds_itens_ant.FieldByName('ID_TAMANHO').text);
                                               if lAchou_Registro then
                                                  begin
                                                    EST_CD_M_LOT.edit;

                                                    // PAREI AQUI
                                                    EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat :=
                                                      (EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat -
                                                           cds_itens_ant.FieldByName(lCampoQtde).AsFloat);

                                                    EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat :=
                                                       (EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat +
                                                          cds_itens_ant.FieldByName(lCampoQtde).AsFloat);

                                                    EST_CD_M_LOT.Post;


                                                    lAchou_Registro := localizar_LOT(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                                             cds_itens.FieldByName('ID_ITEM').text,
                                                                                             cds_itens.FieldByName('NUM_LOTE').text,
                                                                                             cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                                             cds_itens.FieldByName('ID_COR').text,
                                                                                             cds_itens.FieldByName('ID_TAMANHO').text);
                                                    if lAchou_Registro then
                                                       begin
                                                          EST_CD_M_LOT.Edit;
                                                          EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat :=
                                                                   EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat +
                                                                          cds_itens.FieldByName(lCampoQtde).AsFloat;

                                                          EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat :=
                                                                   EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat -
                                                                           cds_itens.FieldByName(lCampoQtde).AsFloat;
                                                          EST_CD_M_LOT.post;
                                                       end;
                                                  end;
                                             end;


                                         lAchou_Registro :=  localizar_LOT(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                   cds_itens.FieldByName('ID_ITEM').text,
                                                                   cds_itens.FieldByName('NUM_LOTE').text,
                                                                   cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                   cds_itens.FieldByName('ID_COR').text,
                                                                   cds_itens.FieldByName('ID_TAMANHO').text);
                                         if lAchou_Registro then
                                            begin
                                               if cds_itens_ant.FieldByName(lCampoQtde).AsFloat <> cds_itens.FieldByName(lCampoQtde).AsFloat  then
                                                  begin
                                                    EST_CD_M_LOT.edit;

                                                    EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat :=
                                                            (EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat -
                                                                cds_itens_ant.FieldByName(lCampoQtde).AsFloat) +
                                                                   cds_itens.FieldByName(lCampoQtde).AsFloat;

                                                    EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat :=
                                                            (EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat +
                                                                cds_itens_ant.FieldByName(lCampoQtde).AsFloat) -
                                                                    cds_itens.FieldByName(lCampoQtde).AsFloat;

                                                    EST_CD_M_LOT.Post;
                                                  end;
                                            end;
                                       end
                                    else
                                       begin
                                          EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat :=
                                                  EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat +
                                                         cds_itens.FieldByName(lCampoQtde).AsFloat;

                                          EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat :=
                                                  EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat -
                                                          cds_itens.FieldByName(lCampoQtde).AsFloat;

                                          EST_CD_M_LOT.post;
                                       end;
                                  end
                              end;
                         end;
                   end;
                 // ----- Fim parte do saldo do lote
             end;
          cds_itens.Next;
        end;


       // Caso algum produto tenha sido retirado, a qtde deve ser retornada para tabela de saldo.
       if (cds_itens_ant <> nil) and (not cds_itens_ant.IsEmpty) then
          begin
           cds_itens_ant.First;
           while not cds_itens_ant.eof do
              begin
                if not cds_itens.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                              VarArrayOf([cds_itens_ant.FieldByName(lCampo).AsString,
                                          cds_itens_ant.FieldByName('ID_ITEM').AsString,
                                          cds_itens_ant.FieldByName(lCampoSeq).AsString]),[]) then
                   begin
                      lAchou_Registro :=  localizar_FES(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                cds_itens_ant.FieldByName('ID_ITEM').text,
                                                                cds_itens_ant.FieldByName('ID_COR').text,
                                                                cds_itens_ant.FieldByName('ID_TAMANHO').text);


                     if lAchou_Registro then
                        begin
                           EST_CD_M_FES.Edit;

                           EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat :=
                                  EST_CD_M_FES.FieldByName('QTDE_SAIDA').AsFloat -
                                  cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                           EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat :=
                                  EST_CD_M_FES.FieldByName('SALDO_FISICO').AsFloat +
                                  cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                           EST_CD_M_FES.post;


                           if CAD_CD_C_PAR_CTRUTILIZA_ALMOXARIFADO.AsBoolean = true then
                              begin

                                lAchou_Registro :=  localizar_FES_ALM(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                              cds_itens_ant.FieldByName('ID_ITEM').text,
                                                                              cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                              cds_itens_ant.FieldByName('ID_COR').text,
                                                                              cds_itens_ant.FieldByName('ID_TAMANHO').text);

                                if lAchou_Registro then
                                   begin
                                      EST_CD_M_FES_ALM.Edit;

                                      EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat :=
                                         EST_CD_M_FES_ALM.FieldByName('QTDE_SAIDA').AsFloat -
                                         cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                                      EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat :=
                                         EST_CD_M_FES_ALM.FieldByName('SALDO_FISICO').AsFloat +
                                               cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                                      EST_CD_M_FES_ALM.Post;
                                   end;
                              end;

                           if CAD_CD_C_PAR_CTRutiliza_lote_na_entrada.AsBoolean = true then
                              begin

                                if trim(lCond_Lote) <> '' then
                                   begin
                                      if (trim(cds_itens_ant.FieldByName('NUM_LOTE').text) <> '') and
                                         (trim(cds_itens_ant.FieldByName('NUM_LOTE').text) <> '0') then
                                         begin
                                            lAchou_Registro :=  localizar_LOT(lIndex, cds_pai.FieldByName('ID_EMPRESA').text,
                                                                                          cds_itens_ant.FieldByName('ID_ITEM').text,
                                                                                          cds_itens_ant.FieldByName('NUM_LOTE').text,
                                                                                          cds_pai.FieldByName('ID_ALMOXARIFADO').text,
                                                                                          cds_itens_ant.FieldByName('ID_COR').text,
                                                                                          cds_itens_ant.FieldByName('ID_TAMANHO').text);

                                            if lAchou_Registro then
                                               begin
                                                  EST_CD_M_LOT.Edit;

                                                  EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat :=
                                                     EST_CD_M_LOT.FieldByName('QTDE_SAIDA').AsFloat -
                                                     cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                                                  EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat :=
                                                     EST_CD_M_LOT.FieldByName('QTDE_ATUAL').AsFloat +
                                                           cds_itens_ant.FieldByName(lCampoQtde).AsFloat;

                                                  EST_CD_M_LOT.Post;
                                               end;
                                         end;
                                   end;
                              end;

                        end;
                   end;
                cds_itens_ant.next;
              end;
          end;
end;



procedure TdmGeral.SelecionarItens(const cds_pai, cds_itens,cds_itens_ant: TClientDataSet; var lCond_Item, lCond_Cor, lCond_Tamanho, lCond_Lote, lCampo,lCampoSeq: String; var lIndex:integer; lOrigem: integer);
begin

   lCond_Item    := '';
   lCond_Cor     := '';
   lCond_Tamanho := '';
   lCond_Lote    := '';


   if (cds_itens_ant <> nil) then
      begin
        cds_itens_ant.Filtered := true;
        cds_itens_ant.Filter   := lCampo + ' = ' + cds_pai.FieldByName(lCampo).AsString;
      end;

   cds_itens.First;
   while not cds_itens.Eof do
       begin
         if cds_itens.FieldByName('INT_TIPO_ITEM').AsInteger <> 9 then // Se no for item do tipo SERVIO.
            begin
              lIndex := 7;

              // busca os itens
              if lCond_Item <> '' then
                 begin
                   lCond_Item :=
                   lCond_Item + ',' +''''+ cds_itens.FieldByName('ID_ITEM').AsString+'''';
                 end;
              if lCond_Item = '' then
                 begin
                   lCond_Item :=
                   lCond_Item +''''+ cds_itens.FieldByName('ID_ITEM').AsString+'''';
                 end;

              // busca as cores dos itens

              if CAD_CD_C_PAR_CTRATIVA_COR.AsBoolean = true then
                 begin
                    lIndex := 4;

                    if cds_itens.FieldByName('ID_COR').text <> '' then
                       begin
                         if not pos(cds_itens.FieldByName('ID_COR').text,lCond_Cor) <> 0 then
                            begin
                              if lCond_Cor <> '' then
                                 begin
                                   lCond_Cor :=
                                   lCond_Cor + ',' +''''+cds_itens.FieldByName('ID_COR').AsString+'''';
                                 end;
                              if lCond_Cor = '' then
                                 begin
                                   lCond_Cor :=
                                   lCond_Cor +''''+ cds_itens.FieldByName('ID_COR').AsString+'''';
                                 end;
                            end;
                       end;
                 end;

              // busca os tamanhos dos itens
              if CAD_CD_C_PAR_CTRATIVA_TAMANHO.AsBoolean = true then
                 begin
                    lIndex := 5;

                    if cds_itens.FieldByName('ID_TAMANHO').text <> '' then
                       begin
                         if not pos(cds_itens.FieldByName('ID_TAMANHO').text,lCond_Tamanho) <> 0 then
                            begin
                              if lCond_Tamanho <> '' then
                                 begin
                                   lCond_Tamanho :=
                                   lCond_Tamanho + ',' +''''+cds_itens.FieldByName('ID_TAMANHO').AsString+'''';
                                 end;
                              if lCond_Tamanho = '' then
                                 begin
                                   lCond_Tamanho :=
                                   lCond_Tamanho +''''+ cds_itens.FieldByName('ID_TAMANHO').AsString+'''';
                                 end;
                            end;
                       end;
                 end;

              if CAD_CD_C_PAR_CTRutiliza_lote_na_entrada.AsBoolean = true then
                 begin
                    if not (lOrigem in [1,2,6]) then // S entra se for diferente de pedido de venda, ordem de sada e nota de sada
                       begin
                          if cds_itens.FieldByName('NUM_LOTE').text <> '' then
                             begin
                               if not pos(cds_itens.FieldByName('NUM_LOTE').text,lCond_Lote) <> 0 then
                                  begin
                                    if lCond_Lote <> '' then
                                       begin
                                         lCond_Lote :=
                                         lCond_Lote + ',' +''''+cds_itens.FieldByName('NUM_LOTE').AsString+'''';
                                       end;
                                    if lCond_Lote = '' then
                                       begin
                                         lCond_Lote :=
                                         lCond_Lote +''''+ cds_itens.FieldByName('NUM_LOTE').AsString+'''';
                                       end;
                                  end;
                             end;
                       end;
                 end;
            end;
          cds_itens.Next;
       end;

    // Caso algum produto tenha sido retirado
    if (cds_itens_ant <> nil) and (not cds_itens_ant.IsEmpty) then
       begin
         cds_itens_ant.First;
         while not cds_itens_ant.eof do
           begin
             if not cds_itens.Locate(lCampo+';ID_ITEM;'+lCampoSeq,
                          VarArrayOf([cds_itens_ant.FieldByName(lCampo).AsString,
                                      cds_itens_ant.FieldByName('ID_ITEM').AsString,
                                      cds_itens_ant.FieldByName(lCampoSeq).AsString]),[]) then
                begin
                   if cds_itens_ant.FieldByName('INT_TIPO_ITEM').AsInteger <> 9 then // Se no for item do tipo SERVIO.
                     begin

                       // busca os itens do pedido
                       if lCond_Item <> '' then
                          begin
                            lCond_Item :=
                            lCond_Item + ',' +''''+ cds_itens_ant.FieldByName('ID_ITEM').AsString+'''';
                          end;
                       if lCond_Item = '' then
                          begin
                            lCond_Item :=
                            lCond_Item +''''+ cds_itens_ant.FieldByName('ID_ITEM').AsString+'''';
                          end;

                       // busca as cores dos itens do pedido de venda

                       if CAD_CD_C_PAR_CTRATIVA_COR.AsBoolean = true then
                          begin
                             if cds_itens_ant.FieldByName('ID_COR').text <> '' then
                                begin
                                  if not pos(cds_itens_ant.FieldByName('ID_COR').text,lCond_Cor) <> 0 then
                                     begin
                                        if lCond_Cor <> '' then
                                         begin
                                            lCond_Cor :=
                                            lCond_Cor + ',' +''''+cds_itens_ant.FieldByName('ID_COR').AsString+'''';
                                          end;
                                       if lCond_Cor = '' then
                                          begin
                                            lCond_Cor :=
                                            lCond_Cor +''''+ cds_itens_ant.FieldByName('ID_COR').AsString+'''';
                                          end;
                                     end;
                                end;
                          end;

                       // busca os tamanhos dos itens do pedido de venda
                       if CAD_CD_C_PAR_CTRATIVA_TAMANHO.AsBoolean = true then
                          begin
                             if cds_itens_ant.FieldByName('ID_TAMANHO').text <> '' then
                                begin
                                  if not pos(cds_itens_ant.FieldByName('ID_TAMANHO').text,lCond_Tamanho) <> 0 then
                                     begin
                                       if lCond_Tamanho <> '' then
                                          begin
                                            lCond_Tamanho :=
                                            lCond_Tamanho + ',' +''''+cds_itens_ant.FieldByName('ID_TAMANHO').AsString+'''';
                                          end;
                                       if lCond_Tamanho = '' then
                                          begin
                                            lCond_Tamanho :=
                                            lCond_Tamanho +''''+ cds_itens_ant.FieldByName('ID_TAMANHO').AsString+'''';
                                          end;
                                     end;
                                end;
                          end;

                       if CAD_CD_C_PAR_CTRutiliza_lote_na_entrada.AsBoolean = true then
                          begin
                             if not (lOrigem in [1,2,6]) then // S entra se for diferente de pedido de venda, ordem de sada e nota de sada
                                begin
                                   if cds_itens_ant.FieldByName('NUM_LOTE').text <> '' then
                                      begin
                                        if not pos(cds_itens_ant.FieldByName('NUM_LOTE').text,lCond_Lote) <> 0 then
                                           begin
                                             if lCond_Lote <> '' then
                                                begin
                                                  lCond_Lote :=
                                                  lCond_Lote + ',' +''''+cds_itens_ant.FieldByName('NUM_LOTE').AsString+'''';
                                                end;
                                             if lCond_Lote = '' then
                                                begin
                                                  lCond_Lote :=
                                                  lCond_Lote +''''+ cds_itens_ant.FieldByName('NUM_LOTE').AsString+'''';
                                                end;
                                           end;
                                      end;
                                end;
                          end;
                     end;
                end;
             cds_itens_ant.next;
           end;
       end;

   if (CAD_CD_C_PAR_CTRATIVA_COR.AsBoolean = true) and (CAD_CD_C_PAR_CTRATIVA_TAMANHO.AsBoolean = true) then
       begin
         lIndex := 6;
       end;
end;

