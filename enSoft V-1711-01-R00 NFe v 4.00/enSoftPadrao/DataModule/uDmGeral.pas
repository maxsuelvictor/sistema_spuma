procedure TdmGeral.PCP_CD_M_OPRBeforePost(DataSet: TDataSet);
var
  SMPrincipal : TSMClient;
  lData,lAno:string;
  cdsOpr: TClientDataSet;
begin
  //Busca sequencia no servidor

  if trim(dmGeral.PCP_CD_M_OPR.FieldByName('NUM_LOTE').AsString) = '0' then
     begin
        SMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);
        lData :=  DateToStr(SMPrincipal.enDataServer);
        FreeAndNil(SMPrincipal);

        if length(lData) = 10 then
           begin
             lAno := copy(lData,9,2);
           end
        else
           begin
             lAno := copy(lData,8,2);
           end;

        cdsOpr := TClientDataSet.Create(nil);
        cdsOpr.RemoteServer := pcConecao;
        cdsOpr.ProviderName := 'BUS_DP_M_OPR_INT';

        cdsOpr.Close;
        cdsOpr.Data :=
            cdsOpr.DataRequest(VarArrayOf([4, dmGeral.PCP_CD_M_OPR.FieldByName('id_empresa').AsString,
                                                    '01/01/'+lAno,'31/12/'+lAno]));
        if cdsOpr.IsEmpty then
           begin
             CAD_CD_C_SEQ.Close;
             CAD_CD_C_SEQ.Data :=
                 CAD_CD_C_SEQ.DataRequest(VarArrayOf([0, 'LOT_OPR']));
             if not CAD_CD_C_SEQ.IsEmpty then
                begin
                  CAD_CD_C_SEQ.edit;
                  CAD_CD_C_SEQ.FieldByName('SEQUENCIA').AsInteger := 0;
                  CAD_CD_C_SEQ.ApplyUpdates(0);
                end;
             CAD_CD_C_SEQ.Close;
           end;
        cdsOpr.close;
        FreeAndNil(cdsOpr);

        try
           SMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);

           dmGeral.PCP_CD_M_OPR.FieldByName('NUM_LOTE').AsString :=
                                        inttostr(SMPrincipal.enValorChave('LOT_OPR'))+'-'+lAno;
        finally
          FreeAndNil(SMPrincipal);
        end;
     end;

  enAudRegistrarInfo(DataSet);
end;

