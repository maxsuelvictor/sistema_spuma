  code,cnpj,emp_cnpj,notify_deb,notify_bloq,data:String;
begin
  Result :=0;
  BusEmpresa(0,dmGeral.CAD_CD_C_PAR.FieldBYname('ID_EMPRESA').AsString);

  if (not (dmGeral.BUS_CD_C_PAR_CTR.FieldByName('status_internet').AsBoolean=true)) then
     begin
        lHTTP := TIdHTTP.Create(nil);
        lStream := TStringStream.Create;
        try
              emp_cnpj := trim(BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString);

              if Length(emp_cnpj)=14 then
                emp_cnpj := copy(emp_cnpj,1,2)+'.'+copy(emp_cnpj,3,3)+'.'+copy(emp_cnpj,6,3)+'/'+copy(emp_cnpj,9,4)+'-'+copy(emp_cnpj,13,2);


              lStream.Clear;
              try
                lHTTP.Get('http://sup.e-north.net.br/api/v1/clientes/consulta-situacao/?cnpj='+emp_cnpj, lStream);

                lStream.Position := 0;
                json := lStream.ReadString(lStream.Size);

                JsonRegistro :=  TJSONObject.ParseJSONValue(
                           TEncoding.ASCII.GetBytes(json),0) as TJSONValue;

                code := '0';
                cnpj := '';
                notify_deb := 'false';
                notify_bloq := 'false';
                for JsonCampo in TJSONObject(JsonRegistro) do
                  begin
                     if JsonCampo.JsonString.Value = 'code' then
                       begin
                         code := JsonCampo.JsonValue.Value;
                       end;
                     if JsonCampo.JsonString.Value = 'cnpj' then
                       begin
                         cnpj := trim(JsonCampo.JsonValue.Value);
                         cnpj := StringReplace(cnpj,'.','',[rfReplaceAll, rfIgnoreCase]);
                         cnpj := StringReplace(cnpj,'/','',[rfReplaceAll, rfIgnoreCase]);
                         cnpj := StringReplace(cnpj,'-','',[rfReplaceAll, rfIgnoreCase]);
                       end;
                      if JsonCampo.JsonString.Value = 'avisar_debito_existente' then
                       begin
                         notify_deb := JsonCampo.JsonValue.Value;
                       end;
                      if JsonCampo.JsonString.Value = 'avisar_cliente_congelamento' then
                       begin
                         notify_bloq := JsonCampo.JsonValue.Value;
                       end;
                  end;

                if ((code='0') and (cnpj=trim(BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString)) and (notify_deb='false') and (notify_bloq='false')) then
                   Result := 1
                else if ((code='0') and (cnpj=trim(BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString)) and (notify_deb='false') and (notify_bloq='true')) then
                   Result := 3
                else if ((code='0') and (cnpj=trim(BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString)) and (notify_deb='true') and (notify_bloq='false')) then
                   Result := 2
                else if ((code='0') and (cnpj=trim(BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString)) and (notify_deb='true') and (notify_bloq='true')) then
                   Result := 3
                else if (code='1') then
                   Result := 4
                else if (code='2') then
                   Result := 5
                else
                   Result := 7;
               except
                 Result := 6;
               end;

          if (Result in [3,4,5,6,7]) or ((Result=2) and (dmGeral.BUS_CD_C_FUN.FieldByName('adm_sistema').AsBoolean=true)
                                                  and (not(dmGeral.BUS_CD_C_PAR_CTR.FieldByName('id_fun_aviso_debito').AsInteger>0)) )   then
           begin
            frmBloq := TfrmBloq.create(self);
            frmBloq.codExibicao := Result;
            frmBloq.admSistema := dmGeral.BUS_CD_C_FUN.FieldByName('adm_sistema').AsBoolean;
            frmBloq.ShowModal;
            frmBloq.Free;
           end;

        finally
          FreeAndNil(lHTTP);
          FreeAndNil(lStream);
        end;
     end;
   BUS_CD_C_PAR.Close;
end;





function TdmGeral.VerificarLicenca:Integer;
var
  SMPrincipal : TSMClient;
  chave:String;

begin

   BusFuncionario(0,IntToStr(xFuncionario));

   BusEmpresa(0,dmGeral.CAD_CD_C_PAR.FieldBYname('ID_EMPRESA').AsString);

   if (dmgeral.BUS_CD_C_PAR_CTR.FieldByName('status_internet').AsBoolean) then
     begin
        SMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);
        try

           chave := trim(dmgeral.BUS_CD_C_PAR_CTR.FieldByName('chave_bloqueio').AsString);
           if (chave = Trim(SMPrincipal.enMD5('0'+dmgeral.BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString+'false'+'false'))) then
             begin
               Result := 1;
             end
           else if (chave = Trim(SMPrincipal.enMD5('0'+dmgeral.BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString+'false'+'true'))) then
             begin
               Result := 3;
             end
           else if (chave = Trim(SMPrincipal.enMD5('0'+dmgeral.BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString+'true'+'false'))) then
             begin
                Result := 2;
             end
           else if (chave = Trim(SMPrincipal.enMD5('0'+dmgeral.BUS_CD_C_PAR.FieldByName('emp_cnpj').AsString+'true'+'true'))) then
             begin
                Result := 3;
             end
           else if (chave = Trim(SMPrincipal.enMD5('1'))) then
             begin
                Result := 4;
             end
           else if (chave = Trim(SMPrincipal.enMD5('2'))) then
             begin
                Result := 5;
             end
           else
             begin
                Result := 7;
             end;

        finally
          FreeAndNil(SMPrincipal);
        end;
     end
   else
     begin
         Result := 6;
     end;

   if (Result in [3,4,5,7]) or ((Result=2) and (dmGeral.BUS_CD_C_FUN.FieldByName('adm_sistema').AsBoolean=true)
                                         and (not(dmGeral.BUS_CD_C_PAR_CTR.FieldByName('id_fun_aviso_debito').AsInteger>0)))   then
     begin
      frmBloq := TfrmBloq.create(self);
      frmBloq.codExibicao := Result;
      frmBloq.admSistema := dmGeral.BUS_CD_C_FUN.FieldByName('adm_sistema').AsBoolean;
      frmBloq.ShowModal;
      frmBloq.Free;
     end;

   BUS_CD_C_PAR.Close;
end;



