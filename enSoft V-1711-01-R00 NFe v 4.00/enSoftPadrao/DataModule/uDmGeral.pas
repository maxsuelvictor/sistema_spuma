  case mes of
     1: MesExt := 'Janeiro';
     2: MesExt := 'Fevereiro';
     3: MesExt := 'Maro';
     4: MesExt := 'Abril';
     5: MesExt := 'Maio';
     6: MesExt := 'Junho';
     7: MesExt := 'Julho';
     8: MesExt := 'Agosto';
     9: MesExt := 'Setembro';
     10: MesExt := 'Outubro';
     11: MesExt := 'Novembro';
     12: MesExt := 'Dezembro';
  end;

  result := dmGeral.CAD_CD_C_PARint_nomecid.Text + ' ' + inttostr(dia) + ' de ' + MesExt + ' de ' + inttostr(ano)
end;

