function TdmGeral.EST_CD_C_ITE_PRE_TesValObrigatorio(
  cds_item_pre: TClientDataSet): Boolean;
begin
  result := true;

  if cds_item_pre.IsEmpty then
     begin
       ShowMessage('Preo do item no encontrado.');
       result := false;
       exit;
     end;

  if cds_item_pre.FieldByName('PRECO_BASE').AsCurrency < 0 then
     begin
       ShowMessage('No  permitido item com preo negativo.');
       result := false;
       exit;
     end;

  if cds_item_pre.FieldByName('PRECO_BASE').AsCurrency = 0 then
     begin
       ShowMessage('No  permitido item com preo 0.');
       result := false;
       exit;
     end;

end;

function TdmGeral.EST_CD_C_ITE_TesValObrigatorio(cds_item: TClientDataSet): Boolean;
begin
  result := true;

  if cds_item.IsEmpty then
     begin
       ShowMessage('Item no encontrado.');
       result := false;
       exit;
     end;

  if cds_item.FieldByName('ATIVO').AsBoolean = false then
     begin
       ShowMessage('Item inativo.');
       result := false;
       exit;
     end;
end;



