procedure TdmGeral.AtualiarFPG_DOC(cdsPedIte,cdsPedTit: TClientDataSet; VlrLiqPed: Currency);
var
  CodFpgAnterior: Integer;
  QtdeVencto,i: integer;
  VlrParcela,vlrSoma: Currency;
begin
   CodFpgAnterior := 0;
   vlrSoma := 0;

   if cdsPedIte.RecordCount = 0 then
      begin
        cdsPedTit.first;
        while not cdsPedTit.eof do
           cdsPedTit.Delete;
        exit;
      end;


   cdsPedTit.First;
   while not cdsPedTit.eof do
      begin
        if (CodFpgAnterior > 0) then
           if CodFpgAnterior <> cdsPedTit.FieldByName('id_forma_pag').AsInteger then
              begin
                cdsPedTit.First;
                exit;
              end;
        CodFpgAnterior := cdsPedTit.FieldByName('id_forma_pag').AsInteger;

        cdsPedTit.Next;
      end;


   cdsPedTit.First;

   QtdeVencto := cdsPedTit.RecordCount;

   VlrParcela := trunc(VlrLiqPed / QtdeVencto);

   if qtdeVencto = 1 then
      begin
        cdsPedTit.Edit;
        cdsPedTit.FieldByName('vlr_titulo').AsCurrency := VlrLiqPed;
        cdsPedTit.Post;
      end
   else
      begin
        i := 0;
        while not cdsPedTit.eof do
           begin
             i := i + 1;

             cdsPedTit.edit;
             if i = QtdeVencto then
                begin
                  cdsPedTit.FieldByName('vlr_titulo').AsCurrency := VlrLiqPed - vlrSoma;
                end
             else
                begin
                  vlrSoma := vlrSoma + VlrParcela;
                  cdsPedTit.FieldByName('vlr_titulo').AsCurrency := VlrParcela;
                end;
             cdsPedTit.Post;

             cdsPedTit.Next;
           end;
      end;
end;

procedure TdmGeral.AtualizarGridItens(grdItens: TwwDBGrid; NomeColItem: String; TamLargCor, TamLargTam: Integer;NomeColCor:String='int_nomecor';NomeColTam:String='int_nometam');
begin
  grdItens.ColumnByName(NomeColItem).DisplayWidth :=
      (grdItens.ColumnByName(NomeColItem).DisplayWidth + TamLargCor + TamLargTam);



 if (dmgeral.CAD_CD_C_PAR_CTR.FieldByName('ativa_cor').AsBoolean=true) and
    (dmgeral.CAD_CD_C_PAR_CTR.FieldByName('ativa_tamanho').AsBoolean=true) then
    begin
       grdItens.ColumnByName(NomeColItem).DisplayWidth :=
      (grdItens.ColumnByName(NomeColItem).DisplayWidth - (TamLargCor + TamLargTam));
    end
 else
    begin
       if (dmgeral.CAD_CD_C_PAR_CTR.FieldByName('ativa_cor').AsBoolean = true) then
          begin
            grdItens.ColumnByName(NomeColItem).DisplayWidth :=
                  (grdItens.ColumnByName(NomeColItem).DisplayWidth - TamLargCor);

            grdItens.ColumnByName(NomeColTam).Visible := false;
          end
       else
          begin
             if dmgeral.CAD_CD_C_PAR_CTR.FieldByName('ativa_tamanho').AsBoolean = true then
                begin
                  grdItens.ColumnByName(NomeColItem).DisplayWidth :=
                        (grdItens.ColumnByName(NomeColItem).DisplayWidth - TamLargTam);
                  grdItens.ColumnByName(NomeColCor).Visible := false;
                end
             else
                begin
                  grdItens.ColumnByName(NomeColCor).Visible    := false;
                  grdItens.ColumnByName(NomeColTam).Visible    := false;
                end;
          end;
    end;

end;

