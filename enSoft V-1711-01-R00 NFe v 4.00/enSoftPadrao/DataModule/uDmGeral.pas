function TdmGeral.CalculaTributos(CdsNfe,CdsNfeIte: TClientDataSet):Boolean;
var
  lRegimeSimples: Boolean;
begin

  FAT_RN_M_NFE.VerifTribItem(CdsNfe.FieldByName('TIPO_NF').Text,CdsNfe.FieldByName('ID_EMPRESA').Text,
                             CdsNfe.FieldByName('ID_EMITENTE').Text,
                             CdsNfeIte.FieldByName('ID_ITEM').Text,
                             CdsNfe.FieldByName('id_tipo_mov_estoque').Text,
                             'C');


  dmGeral.BusCfop(0,CdsNfeIte.FieldByName('ID_CFO').Text);

  if CdsNfe.FieldByName('tipo_nf').Text = 'E' then // NF de Entrada  (Entrada)
     begin
       // Foi colcoado por Maxsuel Victor em 06/06/2017 a pedido da tarefa 3236
       if CdsNfe.FieldByName('numero').AsExtended = 0 then
          begin
            CdsNfeIte.FieldByName('ID_INF_FISCO').text :=
               dmGeral.BUS_CD_C_TRI_REG.FieldByName('ID_INF').text;
          end;

       if dmGeral.BUS_CD_C_GRU.FieldByName('TIPO_ITEM').AsInteger <> 9 then // Se no for servio
          begin
            // Os comentrios foram posto devido a uma conveno que Maxsuel e Sangia
            // fizeram no 23-09-2014, onde decidiram que o lanamento da NFe de entrada
            // a parte de ICMS seria manual.

            //BuscarCstIcm(CdsNfeIte,'F',lRegimeSimples);

            CalculaPIS(CdsNfe,CdsNfeIte,'E');
            CalculaCofins(CdsNfe,CdsNfeIte,'E');
            if (CdsNfe.FieldByName('nfe_finalidade').AsInteger = 1) then
              CalculaICMSNFE(CdsNfe,CdsNfeIte);
            //CalculaICMS(CdsNfe,CdsNfeIte, 'F',lRegimeSimples);
            if (CdsNfe.FieldByName('origem_nf').AsString <> 'X') then
              CalculaIPI(CdsNfe,CdsNfeIte);
          end;
     end;


  if CdsNfe.FieldByName('tipo_nf').Text = 'S' then // NF de sada     (Sada)
     begin
       CdsNfeIte.FieldByName('ID_INF_FISCO').text :=
                dmGeral.BUS_CD_C_TRI_REG.FieldByName('ID_INF').text;

       CalculaTriValor(CdsNfeIte);
       if dmGeral.BUS_CD_C_GRU.FieldByName('TIPO_ITEM').AsInteger <> 9 then // Se no for servio
          begin
            //Comentei essa instruo pois o CST j  calculado no OnExit do campo id_item.Responsavel:Luan.Data:09/09/2015
           // BuscarCstIcm(CdsNfeIte,'C',lRegimeSimples);
            CalculaPIS(CdsNfe,CdsNfeIte,'S');
            CalculaCofins(CdsNfe,CdsNfeIte,'S');

            CalculaICMS(CdsNfe,CdsNfeIte,'C',lRegimeSimples);
            CalculaIPI(CdsNfe,CdsNfeIte);
          end
       else
          begin
            CalculaISS(CdsNfeIte);
            CalculaISSRetido(CdsNfe,CdsNfeIte);
          end;
     end;


  if CdsNfe.FieldByName('tipo_nf').Text = 'R' then // NF de Devoluo de Cliente   (Entrada)
     begin

       CdsNfeIte.FieldByName('ID_INF_FISCO').text :=
                dmGeral.BUS_CD_C_TRI_REG.FieldByName('ID_INF').text;


       if dmGeral.BUS_CD_C_GRU.FieldByName('TIPO_ITEM').AsInteger <> 9 then // Se no for servio
          begin
            CalculaPIS(CdsNfe,CdsNfeIte,'E');
            CalculaCofins(CdsNfe,CdsNfeIte,'E');
          end;
     end;

  if CdsNfe.FieldByName('tipo_nf').Text = 'D' then //  NF de devoluo de fornecedor  (Sada)
     begin
       CdsNfeIte.FieldByName('ID_INF_FISCO').text :=
                dmGeral.BUS_CD_C_TRI_REG.FieldByName('ID_INF').text;

       if dmGeral.BUS_CD_C_GRU.FieldByName('TIPO_ITEM').AsInteger <> 9 then // Se no for servio
          begin

            CalculaPIS(CdsNfe,CdsNfeIte,'S');
            CalculaCofins(CdsNfe,CdsNfeIte,'S');
          end;
     end;

  FatCalculaPart(cdsNfe,cdsNfeIte);
end;


