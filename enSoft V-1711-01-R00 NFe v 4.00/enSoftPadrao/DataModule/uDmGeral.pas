procedure TdmGeral.FAT_CD_M_NFE_ITEPER_DESCONTOChange(Sender: TField);
begin

   if dmGeral.FAT_CD_M_NFE_ITE.FieldByName('PER_DESCONTO').AsCurrency > 0 then
      begin
        if FAT_CD_M_NFE_ITE.FieldByName('VLR_LIQUIDO').AsCurrency > 0 then
           begin
              FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := nil;
              FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := nil;

              dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_DESCONTO').AsCurrency :=
                 (dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_MERCADORIA').AsCurrency *
                  dmGeral.FAT_CD_M_NFE_ITE.FieldByName('PER_DESCONTO').AsCurrency) / 100;
              dmGeral.FAT_CD_M_NFE_ITE.FieldByName('PER_DESCONTO').AsCurrency :=
                 (dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_DESCONTO').AsCurrency * 100) /
                  dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_MERCADORIA').AsCurrency;

              FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := FAT_CD_M_NFE_ITEVLR_DESCONTOChange;
              FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := FAT_CD_M_NFE_ITEPER_DESCONTOChange;
           end
         else   //Foi adiconado esse else para atender quando a nota for complementar de icms,
                //que permite a qtde do item for igual a zero. Resp.: Luan. Data:19/01/2017
           begin
             FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := nil;
             FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := nil;

             dmGeral.FAT_CD_M_NFE_ITE.FieldByName('PER_DESCONTO').AsCurrency := 0;
             dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_DESCONTO').AsCurrency := 0;

             FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := FAT_CD_M_NFE_ITEPER_DESCONTOChange;
             FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := FAT_CD_M_NFE_ITEVLR_DESCONTOChange;
           end;
      end
   else
      begin
        FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := nil;
        FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := nil;

        dmGeral.FAT_CD_M_NFE_ITE.FieldByName('PER_DESCONTO').AsCurrency := 0;
        dmGeral.FAT_CD_M_NFE_ITE.FieldByName('VLR_DESCONTO').AsCurrency := 0;

        FAT_CD_M_NFE_ITEVLR_DESCONTO.OnChange := FAT_CD_M_NFE_ITEVLR_DESCONTOChange;
        FAT_CD_M_NFE_ITEPER_DESCONTO.OnChange := FAT_CD_M_NFE_ITEPER_DESCONTOChange;
      end;
