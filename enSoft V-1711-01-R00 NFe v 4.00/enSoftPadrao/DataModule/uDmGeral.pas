var
  mens: String;
begin
  result := true;

  mens := '';

  if mens <> '' then
     begin
       ShowMessage('Os campos abaixos devem ser preenchidos:' + #13 + mens);
       mens := '';
       result := false;
     end
  else
     begin
       if mens <> '' then
          begin
            ShowMessage('Ateno:' + #13 + mens);
            mens := '';
            result := false;
          end
     end;
end;

