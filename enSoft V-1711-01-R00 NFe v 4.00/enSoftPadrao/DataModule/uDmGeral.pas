                          begin
                            if BUS_CD_C_CLIpessoa.AsInteger = 1 then
                               begin
                                 result := BUS_CD_C_TMEcfo_exterior_pj.Text;
                               end
                          end;
                     end;

                  if trim(result) = '' then
                     begin
                       ShowMessage('CFOP do tipo de estoque no foi encontrado!');
                       BUS_CD_C_CLI.Close;
                       BUS_CD_C_TME.Close;
                       abort;
                     end;
