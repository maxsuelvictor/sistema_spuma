procedure TdmGeral.MontarNroLoteIteNfe(cds_itens: TClientDataSet; lTipo: integer);
var
  lSMPrincipal: TSMClient;
  lData,lAno: String;
begin

  // lTipo - 0 - Por nota de entrada
  //         1 - Por Inspeo de Recebimentos

  lData := DateToStr(xDataSis);
  if length(lData) = 10 then
     begin
       lAno := copy(lData,9,2);
     end
  else
     begin
       lAno := copy(lData,8,2);
     end;



  lSMPrincipal := TSMClient.Create(dmGeral.Conexao.DBXConnection);
  try
     if lTipo = 0 then // Lanado por nota de entrada
        begin
          try
            dmGeral.FAT_CD_M_NFE_ITE.AfterPost := nil;
            cds_itens.First;

            while not cds_itens.Eof do
                begin
                  BusItens(0,cds_itens.FieldByName('ID_ITEM').AsString);
                  if cds_itens.FieldByName('INT_TIPO_ITEM').AsInteger <> 9 then // Se for diferente de servio
                     begin
                       if BUS_CD_C_ITE.FieldByName('GERACAO_LOTE_INTERNO').AsInteger = 0 then // 0 - Por Nota Fiscal de Entrada
                          begin
                            if trim(cds_itens.FieldByName('NUM_LOTE').text) = ''  then
                               begin
                                 cds_itens.Edit;
                                 cds_itens.FieldByName('NUM_LOTE').AsString :=
                                  inttostr(lSMPrincipal.enValorChave('LOT_INT_NFE'))+'-'+lAno;
                                 cds_itens.Post;
                               end;
                          end;
                     end;
                  cds_itens.Next;
                end;
            cds_itens.First;

            finally
               dmGeral.FAT_CD_M_NFE_ITE.AfterPost := dmGeral.FAT_CD_M_NFE_ITEAfterPost;
            end;
        end;

   if lTipo = 1 then // Lanado por Inspeo de Recebimentos
       begin
         try
            dmGeral.FAT_CD_M_IQM_ITE.BeforePost := nil;
            cds_itens.First;
            while not cds_itens.Eof do
               begin
                 BusItens(0,cds_itens.FieldByName('ID_ITEM').AsString);
                 if BUS_CD_C_ITE.FieldByName('INT_TIPO_ITEM').text <> '9' then // Se for diferente de servio
                    begin
                      if BUS_CD_C_ITE.FieldByName('GERACAO_LOTE_INTERNO').AsInteger = 3 then // 1 - Por Inspeo de Recebimento de Itens
                         begin
                           if trim(cds_itens.FieldByName('NUM_LOTE').text) = ''  then
                              begin
                                cds_itens.Edit;
                                cds_itens.FieldByName('NUM_LOTE').AsString := inttostr(lSMPrincipal.enValorChave('LOT_INT_NFE'))+'-'+lAno;
                                cds_itens.Post;
                              end;
                         end;
                    end;
                 cds_itens.Next;
              end;
         finally
           dmGeral.FAT_CD_M_IQM_ITE.BeforePost := dmGeral.FAT_CD_M_IQM_ITEBeforePost;
         end;
       end;

  finally
    FreeAndNil(lSMPrincipal);
  end;
end;

function TdmGeral.MontaTipoNota(const id_tipo_estoque: string): String;
begin
  result := '';
  dmGeral.CAD_CD_C_TME.Close;
  dmGeral.CAD_CD_C_TME.Data :=
  dmGeral.CAD_CD_C_TME.DataRequest(VarArrayOf([0, id_tipo_estoque]));

  if not dmGeral.CAD_CD_C_TME.IsEmpty then
     begin
       if (dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0) and //  Entrada
          (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger in [0,2]) then     //  Normal ou Transferncia
           begin
             result := 'E';
           end;
       if (dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0) and  // Entrada
          (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger = 1) then      // Devoluo
          begin
             result := 'R';
          end;
       if (dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1) and  // Sada
          (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger in [0,2]) then      // Normal ou Transferncia
          begin
             result := 'S';
          end;
       if (dmGeral.CAD_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1) and  // Sada
          (dmGeral.CAD_CD_C_TME.FieldByName('INDICADOR').AsInteger = 1) then      // Normal
          begin
            result := 'D';
          end;
     end;
end;



