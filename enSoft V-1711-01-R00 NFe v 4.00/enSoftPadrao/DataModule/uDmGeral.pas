function TdmGeral.CriarReg_FES(lid_empresa,lid_item,
  ldata_ult_movimento,ldata_ult_inventario: String; lqtde_entrada,
  lqtde_saida,lsaldo_fisico,lqtde_ped_compra,
  lqtde_ped_venda: Double; lvlr_custo_medio,lvlr_custo_ult_compra: Currency;
  lid_cor,lid_tamanho: String): boolean;
begin
  EST_CD_M_FES.Insert;
  EST_CD_M_FES.FieldByName('id_empresa').AsString      := lid_empresa;
  EST_CD_M_FES.FieldByName('id_item').AsString        := lid_item;
  EST_CD_M_FES.FieldByName('id_cor').AsString         := lid_cor;
  EST_CD_M_FES.FieldByName('id_tamanho').AsString     := lid_tamanho;
  EST_CD_M_FES.FieldByName('data_ult_movimento').text  := ldata_ult_movimento;
  EST_CD_M_FES.FieldByName('data_ult_inventario').text := ldata_ult_inventario;
  EST_CD_M_FES.FieldByName('qtde_entrada').AsFloat     := lqtde_entrada;
  EST_CD_M_FES.FieldByName('qtde_saida').AsFloat       := lqtde_saida;
  EST_CD_M_FES.FieldByName('saldo_fisico').AsFloat     := lsaldo_fisico;
  EST_CD_M_FES.FieldByName('qtde_ped_compra').AsFloat  := lqtde_ped_compra;
  EST_CD_M_FES.FieldByName('qtde_ped_venda').AsFloat   := lqtde_ped_venda;
  EST_CD_M_FES.FieldByName('vlr_custo_medio').AsCurrency      := lvlr_custo_medio;
  EST_CD_M_FES.FieldByName('vlr_custo_ult_compra').AsCurrency := lvlr_custo_ult_compra;
  EST_CD_M_FES.Post;

end;

