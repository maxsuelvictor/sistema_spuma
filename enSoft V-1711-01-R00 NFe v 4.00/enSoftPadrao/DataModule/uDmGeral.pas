function TdmGeral.BusCFOPDoc(id_emitente, id_tipo_mov_est,tipo_item,cst_icms:String;cfop_pai:boolean): String;
begin

   result := '';
  BusTipoEstoque(0,id_tipo_mov_est);
  if (cfop_pai = True) then
     begin

  if not BUS_CD_C_TME.IsEmpty then
     begin
        if ((BUS_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0) and
           (BUS_CD_C_TME.FieldByName('INDICADOR').AsInteger      in [0,2])) or

           ((BUS_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1) and
            (BUS_CD_C_TME.FieldByName('INDICADOR').AsInteger      = 1))  then
           begin
              BusFornecedor(0,id_emitente);

              // Para Fornecedor o sistema no precisa buscar CFOP de fornecedor pessoa fsica.
              if CAD_CD_C_PARint_uf.Text = BUS_CD_C_FORint_uf.Text then
                 begin
                   result := BUS_CD_C_TMEcfo_interno_pj.Text;
                 end;

              if ((CAD_CD_C_PARint_uf.Text <> BUS_CD_C_FORint_uf.Text) and (trim(BUS_CD_C_FORint_uf.Text) <> 'EX')) then
                 begin
                   result := BUS_CD_C_TMEcfo_externo_pj.Text;
                 end;

              if ((CAD_CD_C_PARint_uf.Text <> BUS_CD_C_FORint_uf.Text) and (trim(BUS_CD_C_FORint_uf.Text) = 'EX')) then
                 begin
                   result := BUS_CD_C_TMEcfo_exterior_pj.Text;
                 end;

              if trim(result) = '' then
                 begin
                    ShowMessage('CFOP do tipo de estoque no foi encontrado!');
                    BUS_CD_C_TME.Close;
                    BUS_CD_C_FOR.Close;
                    abort;
                 end;
              BUS_CD_C_TME.Close;
              BUS_CD_C_FOR.Close;
           end
        else
           begin
             if ((BUS_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 1) and
                 (BUS_CD_C_TME.FieldByName('INDICADOR').AsInteger      in [0,2])) or

                ((BUS_CD_C_TME.FieldByName('TIPO_MOVIMENTO').AsInteger = 0) and
                 (BUS_CD_C_TME.FieldByName('INDICADOR').AsInteger      = 1)) then
                begin
                  BusCliente(0,id_emitente);

                  if CAD_CD_C_PARint_uf.Text = BUS_CD_C_CLIint_uf.Text then
                     begin
                       if BUS_CD_C_CLIpessoa.AsInteger = 0 then
                          begin
                            result := BUS_CD_C_TMEcfo_interno_pf.Text;
                          end
                       else
                          begin
                            if BUS_CD_C_CLIpessoa.AsInteger = 1 then
                               begin
                                 result := BUS_CD_C_TMEcfo_interno_pj.Text;
                               end
                          end;
                     end;

                  if ((CAD_CD_C_PARint_uf.Text <> BUS_CD_C_CLIint_uf.Text) and (trim(BUS_CD_C_CLIint_uf.Text)<>'EX')) then
                     begin
                        if BUS_CD_C_CLIpessoa.AsInteger = 0 then
                          begin
                            result := BUS_CD_C_TMEcfo_externo_pf.Text;
                          end
                       else
                          begin
                            if BUS_CD_C_CLIpessoa.AsInteger = 1 then
                               begin
                                 result := BUS_CD_C_TMEcfo_externo_pj.Text;
                               end
                          end;
                     end;

                   if ((CAD_CD_C_PARint_uf.Text <> BUS_CD_C_CLIint_uf.Text) and (trim(BUS_CD_C_CLIint_uf.Text)='EX')) then
                     begin
                        if BUS_CD_C_CLIpessoa.AsInteger = 0 then
                          begin
                            result := BUS_CD_C_TMEcfo_exterior_pf.Text;
