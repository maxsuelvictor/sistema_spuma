procedure TdmGeral.FAT_CD_M_NFE_ITEid_cfoChange(Sender: TField);
begin

  try
    FAT_CD_M_NFE_ITEid_cfo.OnChange := nil;
    //Inseri esse if para que essas instrues no sejam executadas no nota de sada.
    //Pois quando o CFOP  invalido o cursor fica sempre preso neste campo.Responsavel:Luan.Data:09/09/2015
  if (dmGeral.FAT_CD_M_NFE.FieldByName('TIPO_NF').AsString <>'S') then
     begin
      if not dmGeral.ValCfop(dmGeral.FAT_CD_M_NFE.FieldByName('TIPO_NF').text,
                         dmGeral.FAT_CD_M_NFE.FieldByName('UF_EMPRESA').text,
                         dmGeral.FAT_CD_M_NFE.FieldByName('UF_EMITENTE').text,
                         dmGeral.FAT_CD_M_NFE_ITE.FieldByName('ID_CFO').text) then
        begin
          dmGeral.FAT_CD_M_NFE_ITE.FieldByName('ID_CFO').text := '';
          dmGeral.FAT_CD_M_NFE_ITE.FieldByName('ID_CFO').FocusControl;
          abort;
        end;
       dmGeral.BusCfop(0,dmGeral.FAT_CD_M_NFE_ITE.FieldByName('ID_CFO').text);
     end;

  if dmGeral.FAT_CD_M_NFE.FieldByName('TIPO_NF').text = 'E' then
     begin
       if not (pos(dmGeral.BUS_CD_C_CFOid_classe.Text,CCfoNFE_ENTRADA) <> 0) then
          begin
            ShowMessage('Natureza do CFOP invlida para esta NFe.');
            dmGeral.FAT_CD_M_NFE_ITE.FieldByName('ID_CFO').FocusControl;
            abort;
          end;
     end;
  finally
    FAT_CD_M_NFE_ITEid_cfo.OnChange := FAT_CD_M_NFE_ITEid_cfoChange  
  end;
end;

