  object FAT_SQ_R_GAM_150: TSQLDataSet
    SchemaName = 'sa'
    CommandText = 
      'select '#13#10'   cli.id_cliente AS CODIGO,'#13#10'   cli.nome as nome,'#13#10'   ' +
