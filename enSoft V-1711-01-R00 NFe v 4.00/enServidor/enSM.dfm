    CommandText = 
      'SELECT '#13#10'   COALESCE(PGT.COM_SUBSTITUICAO,FALSE) AS INT_SUBSTITU' +
      'ICAO,'#13#10'  pgt.*, PAG.ID_FORNECEDOR, PAG.VLR_PARCELA AS VLR_TITULO' +
      ',pag.num_parcela, pag.dta_emissao, pag.dta_vencimento,pag.id_obr' +
      'a,       '#13#10'  FON.DESCRICAO AS INT_NOMEFOR, '#13#10'  FPG.DESCRICAO AS ' +
      'INT_NOMEFPG, '#13#10'  PCT.DESCRICAO AS INT_NOMEPCT,'#13#10'  CCU.descricao ' +
      'AS INT_NOMECCU,'#13#10'  FON.DOC_CNPJ  AS INT_CNPJCPF,'#13#10'OBR.descricao ' +
      'AS INT_NOMEOBR '#13#10#13#10'  '#13#10#13#10'FROM  FIN_TB_M_PAG_PAG  PGT'#13#10'   LEFT OU' +
      'TER JOIN FIN_TB_M_PAG PAG ON PAG.ID_TITULO              = PGT.ID' +
      '_TITULO'#13#10'   LEFT OUTER JOIN CAD_TB_C_FOR FON ON FON.ID_FORNECEDO' +
      'R =  pag.ID_FORNECEDOR'#13#10'   LEFT OUTER JOIN CAD_TB_C_FPG FPG ON F' +
      'PG.ID_FORMA_PAG     =  pgt.ID_FORMA_PAG'#13#10'   LEFT OUTER JOIN CAD_' +
      'TB_C_PCT PCT ON PCT.ID_PLANO               =  pgt.ID_PLANO'#13#10'   L' +
      'EFT OUTER JOIN cad_tb_c_ccu CCU ON CCU.id_ccusto                ' +
      '   =  pgt.ID_CCUSTO'#13#10'   LEFT OUTER JOIN ctc_tb_c_obr OBR ON OBR.' +
      'id_obra       =   PAG.ID_OBRA '#13#10'where 1  = 2'
