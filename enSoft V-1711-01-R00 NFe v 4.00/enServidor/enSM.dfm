  object FIN_SQ_R_MCH: TSQLDataSet
    CommandText = 
      'with tb_vlr_total    '#13#10'         as  ( with tb_vlr_total_por_mch ' +
      'as (select DISTINCT ON (mch.id_controle) mch.id_controle, mch.id' +
      '_empresa,mch.vlr_movimento,'#13#10'                      mch.vlr_tot_m' +
      'ovimento, mch.vlr_tot_juros'#13#10'            from fin_tb_m_mch_che m' +
      'che'#13#10'            left outer join fin_tb_m_mch mch on mch.id_cont' +
      'role = mche.id_controle '#13#10'            left outer join fin_tb_m_r' +
      'ec rec on rec.id_titulo = mche.id_titulo'#13#10'           WHERE 1=2 )' +
      '     '#13#10'              select vmch.id_empresa, sum(vmch.vlr_movime' +
      'nto) as tot_movimento, sum(vmch.vlr_tot_movimento) as tot_ger_mo' +
      'vimento,sum(vmch.vlr_tot_juros) as tot_juros from tb_vlr_total_p' +
      'or_mch vmch'#13#10'                               group by vmch.id_emp' +
      'resa'#13#10'             '#13#10'             )'#13#10#13#10#13#10'select mch.*, mche.*,vl' +
      't.tot_movimento,vlt.tot_ger_movimento,vlt.tot_juros,            ' +
      '     '#13#10'         cli.nome as int_nomecli,'#13#10'         rec.*,  hch.s' +
      'ituacao_chq,     '#13#10'         par.emp_fantasia as int_nomeemp,'#13#10'  ' +
      '      ctc.banco as int_bancoctc,'#13#10'        ctc.agencia as int_age' +
      'nciactc,'#13#10'        ctc.conta_corrente as int_contcorrctc,'#13#10'      ' +
      '   ctc.descricao as int_nomectc,'#13#10'         cast(  case mch.opera' +
      'cao     '#13#10'             when 1  then '#39'Depositar'#39#13#10'             wh' +
      'en 2 then '#39'Devolver'#39#13#10'             when 3 then '#39'Pagar parcial'#39#13#10 +
      '             when 4 then '#39'Resgatar'#39#13#10'           end as varchar(2' +
      '0) ) as int_operacao,  '#13#10'           fci.nome as int_nomefun,    ' +
      '            '#13#10'          cast(  case hch.situacao_chq            ' +
      '    '#13#10'              when 1  then '#39'Em aberto'#39'          '#13#10'        ' +
      '      when 2 then '#39'Depositado'#39'         '#13#10'              when 3 th' +
      'en '#39'Devolvido'#39'       '#13#10'              when 4 then '#39'Parcial'#39'      ' +
      #13#10'              when 5 then '#39'Resgatado'#39'            '#13#10'           ' +
      'end as varchar(20) ) as int_situacao '#13#10'          from fin_tb_m_m' +
      'ch_che mche            '#13#10'             left outer join fin_tb_m_m' +
      'ch mch on '#13#10'                 (mch.id_controle = mche.id_controle' +
      ')  '#13#10'             left outer join fin_tb_m_rec rec on'#13#10'         ' +
      '        (rec.id_titulo = mche.id_titulo)  '#13#10'             left ou' +
      'ter join fin_tb_m_rec_hch hch on '#13#10'                 (hch.id_titu' +
      'lo = mche.id_titulo and  '#13#10'                  hch.id_mch = mche.i' +
      'd_controle)   '#13#10'             left outer join cad_tb_c_cli cli on' +
      #13#10'                 (cli.id_cliente = rec.id_cliente)'#13#10'          ' +
      '   left outer join cad_tb_c_ctc ctc on      '#13#10'                 (' +
      'mch.id_conta = ctc.id_conta)         '#13#10'             left outer j' +
      'oin cad_tb_c_fun fci on      '#13#10'                (fci.id_funcionar' +
      'io = mch.id_responsavel) '#13#10'             left outer join cad_tb_c' +
      '_par par on      '#13#10'                (par.id_empresa = mch.id_empr' +
      'esa) '#13#10'             left outer join tb_vlr_total vlt on      '#13#10' ' +
      '               (vlt.id_empresa = par.id_empresa)'#13#10'          wher' +
      'e 1 = 2'
