  object FAT_SQ_M_NFE_ITE: TSQLDataSet
    SchemaName = 'sa'
    CommandText = 
      'select '#13#10'      false as INT_SELECAO,'#13#10'       NFI.*, '#13#10'       ITE' +
      '.DESCRICAO INT_DESC_ITEM, '#13#10'       ITE.ID_UND_VENDA INT_UND_VEND' +
      'A,  '#13#10'       GRU.TIPO_ITEM  AS  INT_TIPO_ITEM,'#13#10'       COR.DESCR' +
      'ICAO AS INT_NOMECOR, '#13#10'       TAM.DESCRICAO AS INT_NOMETAM,'#13#10'   ' +
      '    ITE.ID_NCM as INT_ID_NCM,'#13#10'       NCM.PER_IMPOSTO AS INT_NCM' +
      'PERIMPOSTO,'#13#10'       NCM.CEST AS INT_CEST_NCM,'#13#10'       NFI.QTDE -' +
      ' coalesce(NFI.QTDE_DEVOLVIDA,0) AS INT_SLDQTDE,'#13#10'       ITE.COD_' +
      'ONU AS INT_COD_ONU,'#13#10'       ITE.RT_COMPOSICAO AS INT_RT_COMPOSIC' +
      'AO,'#13#10'       ITE.RT_REGISTRO AS INT_RT_REGISTRO,'#13#10'       ITE.COD_' +
      'FABRICA AS INT_COD_FABRICA,'#13#10'       ITE.COD_BARRA AS INT_COD_BAR' +
      'RA,'#13#10'       ITE.SGQ_PERSONALIZADO AS INT_SGQ_PERSON,'#13#10'       FRN' +
      '.COD_FAB_DANFE AS INT_COD_FAB_DANFE,'#13#10'       GRU.ID_GRUPO AS INT' +
      '_ID_GRUPO, GRU.part_perc_fcp AS INT_PART_PERC_FCP,'#13#10'       GRU.O' +
      'RIGEM_MERCADORIA AS INT_ORIGEM'#13#10'from FAT_TB_M_NFE_ITE NFI'#13#10'LEFT ' +
      'OUTER JOIN CAD_TB_C_ITE ITE ON ITE.ID_ITEM=NFI.ID_ITEM'#13#10'left out' +
      'er join CAD_TB_C_GRU GRU on GRU.ID_GRUPO = ITE.ID_GRUPO'#13#10'LEFT OU' +
      'TER JOIN CAD_TB_C_COR COR ON COR.ID_COR=NFI.ID_COR'#13#10'LEFT OUTER J' +
      'OIN CAD_TB_C_TAM TAM ON TAM.ID_TAMANHO=NFI.ID_TAMANHO'#13#10'LEFT OUTE' +
      'R JOIN CAD_TB_C_NCM NCM ON NCM.ID_NCM = ITE.ID_NCM'#13#10#13#10'LEFT OUTER' +
      ' JOIN CAD_TB_C_FOR FRN ON FRN.ID_FORNECEDOR=ITE.ID_FORNECEDOR'#13#10#13 +
      #10'WHERE NFI.ID_FISCAL=:ID_FISCAL'
