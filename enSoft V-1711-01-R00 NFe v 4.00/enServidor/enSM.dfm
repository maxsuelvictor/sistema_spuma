      '2/2014'#39#13#10'                             AND  PERGUNTA_3 = 0 )  as ' +
      'QTDE_PERGUNTA_3_MELHOR, '#13#10'                         (SELECT COUNT' +
      '(*) FROM PCP_TB_M_PSC PSC '#13#10'                           WHERE  ID' +
      '_EMPRESA = PAR.ID_EMPRESA '#13#10'                             AND  DA' +
      'TA >= '#39'01/01/2014'#39#13#10'                             AND  DATA <= '#39'0' +
      '1/12/2014'#39#13#10'                             AND  PERGUNTA_3 = 1 ) a' +
      's QTDE_PERGUNTA_3_IGUAL, '#13#10'                         (SELECT COUN' +
      'T(*)FROM PCP_TB_M_PSC PSC '#13#10'                           WHERE  ID' +
      '_EMPRESA = PAR.ID_EMPRESA '#13#10'                             AND  DA' +
