    CommandText = 
      'select false as int_selecao , MCT.* , '#13#10#13#10' case mct.origem'#13#10'  wh' +
      'en '#39'0'#39' then   '#39'Manual'#39#13#10'  when '#39'1'#39'  then  '#39'Baixa de Conta a Rece' +
      'ber'#39#13#10'  when '#39'2'#39'  then  '#39'Baixa de Contas a Pagar'#39#13#10'  when '#39'3'#39'  t' +
      'hen  '#39'Devolu'#231#227'o de vendas'#39#13#10'  when '#39'4'#39'  then  '#39'Concilia'#231#227'o banc'#225 +
      'ria'#39#13#10'  when '#39'5'#39'  then  '#39'Nota Fiscal de Sa'#237'da'#39#13#10'  when '#39'6'#39'  then' +
      '  '#39'Nota de Cr'#233'dito'#39#13#10'  when '#39'7'#39'  then  '#39'Abertura de caixa'#39#13#10'  wh' +
      'en '#39'8'#39'  then  '#39'Controle de Caixa'#39#13#10'  when '#39'9'#39'  then  '#39'Movimenta'#231 +
      #227'o de cheque'#39#13#10'  when '#39'10'#39'  then  '#39'Concilia'#231#227'o banc'#225'ria'#39#13#10'end  a' +
      's int_origem,'#13#10#13#10'case mct.deb_cre'#13#10'when '#39'0'#39' then '#39'D'#233'bito'#39#13#10'when ' +
      #39'1'#39' then '#39'Cr'#233'dito'#39#13#10'end as int_debcre,'#13#10#13#10'PCT.DESCRICAO AS INT_N' +
      'OMEPCT, '#13#10'          TIF.DESCRICAO AS INT_NOMETIF, CTC.CEDENTE_NO' +
      'M AS INT_NOMECTC,'#13#10'OBR.DESCRICAO AS INT_DESC_OBR,'#13#10'CLI.NOME AS I' +
      'NT_NOMECLI'#13#10'from FIN_TB_M_CTA MCT'#13#10'LEFT OUTER JOIN CAD_TB_C_CLI ' +
