  object FAT_SQ_R_PVE_01: TSQLDataSet
    CommandText = 
      'SELECT '#13#10'   NFE.ID_EMPRESA as INT_ID_EMPRESA, '#13#10'   CAST(PAR.EMP_' +
      'RAZAO AS VARCHAR(60)) AS INT_NOMEEMP, '#13#10'   NFE.NUMERO, NFE.ID_PE' +
      'DIDO_VENDA, NFE.DTA_EMISSAO,'#13#10'   NFE.ID_CONDICAO_PAG,CPG.DESCRIC' +
      'AO AS INT_NOMECPG,'#13#10'   NFE.ID_VENDEDOR,    FUC.NOME AS INT_NOMEV' +
      'ND,'#13#10'   NFE.ID_EMITENTE,    CLI.NOME AS INT_NOMECLI,'#13#10'   CLI.ID_' +
      'PERFIL_CLI,  PEC.DESCRICAO AS INT_NOMEPEC,'#13#10'   CLI.ID_REGIAO, RE' +
      'G.DESCRICAO AS INT_NOMEREG, '#13#10'   CLI.ID_ROTA, ROT.DESCRICAO AS I' +
      'NT_NOMEROT,'#13#10'   CID.UF AS ESTADO, '#13#10'   CID.NOME||'#39'-'#39'||CID.UF AS ' +
      'INT_NOMECIDADE, '#13#10'   SUM(COALESCE(NIT.VLR_LIQUIDO*(NIT.QTDE-NIT.' +
      'QTDE_DEVOLVIDA),0)) AS VLR_BRUTO,'#13#10'   SUM(COALESCE(NIT.VLR_DESCO' +
      'NTO,0)) AS VLR_DESCONTO,'#13#10'   SUM(COALESCE(NIT.VLR_LIQUIDO*(NIT.Q' +
      'TDE-NIT.QTDE_DEVOLVIDA),0))-SUM(COALESCE(NIT.VLR_DESCONTO,0)) AS' +
      ' VLR_LIQUIDO,'#13#10'   SUM(NIT.QTDE) AS QTDE,'#13#10'   SUM(NIT.QTDE_DEVOLV' +
      'IDA) AS QTDE_DEV'#13#10#13#10'   , CAST('#39#39' AS VARCHAR(20)) AS INT_CODQUEBR' +
      'A1'#13#10'   , CAST('#39#39' AS VARCHAR(100)) AS INT_DESCQUEBRA1'#13#10'   , CAST(' +
      #39#39' AS VARCHAR(20)) AS INT_CODQUEBRA2'#13#10'   , CAST('#39#39' AS VARCHAR(10' +
      '0)) AS INT_DESCQUEBRA2'#13#10#13#10' '#13#10'FROM FAT_TB_M_NFE_ITE NIT'#13#10'LEFT OUT' +
      'ER JOIN FAT_TB_M_NFE NFE ON NFE.ID_FISCAL=NIT.ID_FISCAL'#13#10'LEFT OU' +
      'TER JOIN CAD_TB_C_PAR PAR ON PAR.ID_EMPRESA=NFE.ID_EMPRESA'#13#10'LEFT' +
      ' OUTER JOIN CAD_TB_C_FUN FUC ON FUC.ID_FUNCIONARIO=NFE.ID_VENDED' +
      'OR'#13#10'LEFT OUTER JOIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE=NFE.ID_EM' +
      'ITENTE'#13#10'LEFT OUTER JOIN CAD_TB_C_CID CID ON CID.ID_CIDADE=CLI.ID' +
      '_CIDADE'#13#10'LEFT OUTER JOIN CAD_TB_C_TME TME ON TME.ID_TIPO_MOV_EST' +
      'OQUE=NFE.ID_TIPO_MOV_ESTOQUE'#13#10'LEFT OUTER JOIN CAD_TB_C_ITE ITE O' +
      'N ITE.ID_ITEM=NIT.ID_ITEM'#13#10'LEFT OUTER JOIN CAD_TB_C_CFO CFO ON C' +
      'FO.ID_CFO=NFE.ID_CFO'#13#10'LEFT OUTER JOIN CAD_TB_C_CPG CPG ON CPG.ID' +
      '_CONDICAO_PAG=NFE.ID_CONDICAO_PAG'#13#10'LEFT OUTER JOIN CAD_TB_C_PEC ' +
      'PEC ON PEC.ID_PERFIL_CLI=CLI.ID_PERFIL_CLI'#13#10'LEFT OUTER JOIN CAD_' +
      'TB_C_FOR FOC ON FOC.ID_FORNECEDOR=ITE.ID_FORNECEDOR'#13#10'LEFT OUTER ' +
      'JOIN CAD_TB_C_GRU GRU ON GRU.ID_GRUPO=ITE.ID_GRUPO'#13#10'LEFT OUTER J' +
      'OIN PCP_TB_C_REG REG ON REG.ID_REGIAO=CLI.ID_REGIAO'#13#10'LEFT OUTER ' +
