      'ECPG,'#13#10'            VEI.ID_VEICULO,'#13#10'            VEI.ID_PLACA AS ' +
      'INT_PLACA,   VEI.DESCRICAO AS INT_NOMEVEI,'#13#10'          cast( case' +
      ' OCV.status'#13#10'             when 0 then '#39'Em Aberto'#39#13#10'             ' +
      'when 1 then '#39'Importado'#39#13#10'             when 2 then '#39'Cancelado'#39#13#10' ' +
      '          end as varchar(20)) as INT_OCVSITUACAO,'#13#10'          '#13#10' ' +
      '          sum(case gru.tipo_item'#13#10'              when '#39'09'#39'  then ' +
      'vlr_liquido'#13#10'           end) as vlr_liquido_ser,'#13#10#13#10'           s' +
      'um(case'#13#10'              when  gru.tipo_item <> '#39'09'#39'  then vlr_liq' +
      'uido'#13#10'           end) as vlr_liquido_pro'#13#10'           '#13#10'     '#13#10#13#10 +
      'FROM OFI_TB_M_OCV_ITE OCI'#13#10'LEFT OUTER JOIN CAD_TB_C_ITE    ITE  ' +
      'ON  ITE.ID_ITEM=OCI.ID_ITEM'#13#10'LEFT OUTER JOIN CAD_TB_C_GRU   GRU ' +
      'ON  GRU.ID_GRUPO = ITE.ID_GRUPO'#13#10'LEFT OUTER JOIN OFI_TB_M_OCV OC' +
      'V ON OCV.ID_ORCAMENTO =OCI.ID_ORCAMENTO'#13#10'LEFT OUTER JOIN CAD_TB_' +
      'C_FUN FUN ON FUN.ID_FUNCIONARIO=OCV.ID_FUNCIONARIO'#13#10'LEFT OUTER J' +
      'OIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE=OCV.ID_CLIENTE'#13#10'LEFT OUTE' +
      'R JOIN CAD_TB_C_CPG CPG ON CPG.ID_CONDICAO_PAG=OCV.ID_CONDICAO_P' +
      'AG'#13#10'LEFT OUTER JOIN CAD_TB_C_VEI VEI ON VEI.ID_PLACA=OCV.PLACA'#13#10 +
