      'STA'#13#10'   LEFT OUTER JOIN CAD_TB_C_VEI VEI ON VEI.ID_VEICULO=ORV.I' +
      'D_VEICULO'#13#10'   LEFT OUTER JOIN CAD_TB_C_MVE MVE ON MVE.ID_MVE = V' +
      'EI.ID_MVE'#13#10'   LEFT OUTER JOIN CAD_TB_C_FPG FPG ON FPG.ID_FORMA_P' +
      'AG=ORV.ID_FORMA_PAG'#13#10'   LEFT OUTER JOIN CAD_TB_C_CPG CPG ON CPG.' +
