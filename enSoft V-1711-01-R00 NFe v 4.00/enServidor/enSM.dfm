  object PCP_SQ_M_OPR_OPP: TSQLDataSet
    SchemaName = 'sa'
    CommandText = 
      'select false as int_selecao, OPP.*,'#13#10' cast( case'#13#10'      when rtr' +
      'im(pcp_desc_item_person) <>'#39#39' then  pcp_desc_item_person'#13#10'  else' +
      #13#10'      ite.descricao end as varchar(150) ) as int_nomeite,'#13#10'  I' +
      'TE.ID_UND_VENDA  as int_undvenda, '#13#10'  cor.descricao as int_nomec' +
      'or,'#13#10'  tam.descricao as int_nometam,'#13#10'  fun.nome as int_nomeresp' +
      ', ite.id_grupo as int_id_grupo, gru.descricao as int_nomegru,'#13#10' ' +
      ' fnn.nome as int_nomeresp_man,'#13#10' ( opp.qtde_a_produzir - opp.qtd' +
      'e_produzida ) as int_rest_a_produzir,'#13#10#13#10'  gru. rastrea_espumaca' +
      'o int_rastrea_espumacao,  '#13#10'  gru.rastrea_laminacao int_rastrea_' +
      'laminacao,'#13#10'  gru.rastrea_bordadeira int_rastrea_bordadeira, '#13#10' ' +
      ' gru.rastrea_corte_costura int_rastrea_corte_costura,'#13#10'  gru.ras' +
      'trea_colagem_tampo_alfin int_rastrea_colagem_tampo_alfin, '#13#10'  gr' +
      'u.rastrea_colagem_molas int_rastrea_colagem_molas,'#13#10'  gru.exibe_' +
      'na_impressao_op as int_exibe_na_impressao_op,'#13#10'  gru.rastrea_mon' +
      'tagem_caixa_box as int_rastrea_montagem_caixa_box'#13#10#13#10' from PCP_T' +
      'B_M_OPR_OPP OPP '#13#10'   left outer join cad_tb_c_ite ite on ite.id_' +
      'item = opp.id_item'#13#10'   left outer join cad_tb_c_gru gru on ite.i' +
      'd_grupo = gru.id_grupo'#13#10'   left outer join cad_tb_c_cor cor on c' +
      'or.id_cor = opp.id_cor'#13#10'   left outer join cad_tb_c_tam tam on t' +
      'am.id_tamanho = opp.id_tamanho'#13#10'   left outer join cad_tb_c_fun ' +
      'fun on fun.id_funcionario = opp.id_resp_alt'#13#10'   left outer join ' +
      'cad_tb_c_fun fnn on fnn.id_funcionario = opp.id_resp_man'#13#10'where ' +
      'ID_OPR=:ID_OPR'#13#10'order by opp.id_opr_opp'
