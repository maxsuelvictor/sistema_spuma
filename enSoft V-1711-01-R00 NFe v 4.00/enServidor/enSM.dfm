  object FIN_SQ_R_EXT_CLI_EST: TSQLDataSet
    CommandText = 
      'select cli.id_cliente,cli.lim_saldo,cli.dta_ult_compra,cli.dta_p' +
      'ri_compra, '#13#10#13#10'        (select coalesce(sum(rec.vlr_saldo),0) fr' +
      'om fin_tb_m_rec rec '#13#10'         where rec.vlr_saldo>0) as vlr_tot' +
      'al_tit_aberto, '#13#10#13#10'         (select sum(rec.vlr_saldo) from fin_' +
      'tb_m_rec rec where rec.dta_vencimento<CURRENT_DATE and rec.vlr_s' +
      'aldo>0) as vlr_total_tit_vencido, '#13#10#13#10'        (select COALESCE(s' +
      'um(((rec.vlr_saldo*(COALESCE(CTR.tax_juros_mensal,0)/30)/100)* '#13 +
      #10'              (CURRENT_DATE -  rec.dta_vencimento))),0) '#13#10'     ' +
      '     from fin_tb_m_rec rec                         '#13#10'          l' +
      'eft outer join cad_tb_c_par_ctr ctr on ctr.id_empresa=rec.id_emp' +
      'resa '#13#10'         where rec.dta_vencimento<CURRENT_DATE)  as vlr_t' +
      'otal_tit_juros, '#13#10#13#10#13#10'        (select COALESCE(sum(rec.vlr_saldo' +
      '+((rec.vlr_saldo*(COALESCE(CTR.tax_juros_mensal,0)/30)/100)* '#13#10' ' +
      '               (CURRENT_DATE -  rec.dta_vencimento))),0) from fi' +
      'n_tb_m_rec rec '#13#10'         left outer join cad_tb_c_par_ctr ctr o' +
      'n ctr.id_empresa=rec.id_empresa '#13#10'         where rec.dta_vencime' +
      'nto<CURRENT_DATE) as vlr_total_tit_corrigido,'#13#10#13#10'        (select' +
      ' COALESCE(sum(rec.vlr_saldo),0) from fin_tb_m_rec rec where rec.' +
      'dta_vencimento>=CURRENT_DATE and rec.vlr_saldo>0) as vlr_total_t' +
      'it_vencer,'#13#10#13#10'        (select COALESCE(sum(rec.vlr_saldo),0) fro' +
      'm fin_tb_m_rec rec '#13#10'         left outer join cad_tb_c_fpg fpg o' +
      'n fpg.id_forma_pag=rec.id_forma_pag '#13#10'         where fpg.doc_imp' +
      'resso=6 and rec.vlr_saldo>0 ) as  vlr_che_aberto,'#13#10#13#10'        (se' +
      'lect COALESCE(sum(rec.vlr_parcela),0) from fin_tb_m_rec rec wher' +
      'e rec.vlr_saldo=0) as vlr_total_tit_pagos,'#13#10#13#10'        (select CO' +
      'ALESCE(sum(nfe.vlr_liquido),0)  from fat_tb_m_nfe nfe '#13#10'        ' +
      ' LEFT OUTER JOIN CAD_TB_C_CFO CFO ON CFO.ID_CFO=NFE.ID_CFO'#13#10'    ' +
      '      WHERE CFO.ID_CLASSE IN ('#39'SP10'#39','#39'SP70'#39') AND NFE.NFE_COD_SIT' +
      ' not in ('#39'02'#39','#39'04'#39','#39'05'#39') AND NFE.STATUS = 1 AND'#13#10'             NF' +
      'E.FATURADA = true AND NFE.TIPO_NF='#39'S'#39')  as vlr_total_compras,'#13#10#13 +
      #10'         (select COALESCE(sum(nfe.vlr_liquido),0)  from fat_tb_' +
      'm_nfe nfe'#13#10'         WHERE NFE.STATUS = 1 AND                    ' +
      '                    '#13#10'             NFE.FATURADA = true AND NFE.T' +
      'IPO_NF='#39'R'#39') as vlr_total_dev '#13#10#13#10'        from cad_tb_c_cli cli w' +
      'here 1=2'
