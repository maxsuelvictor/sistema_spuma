      'd_plano as int_codpct_snt,'#13#10'        pst.descricao as int_descpct' +
      '_snt  from cad_tb_c_pct pct '#13#10'        LEFT OUTER JOIN PST ON PST' +
      '.ID_PLANO=substring(PCT.ID_PLANO from 1 for 6)||'#39'000'#39#13#10'WHERE 1=2'
