  object BUS_SQ_M_SLD_ITE: TSQLDataSet
    CommandText = 
      'select'#13#10'   (select COALESCE(sum(fea.qtde),0)'#13#10#9#9'from est_tb_m_fe' +
      'a fea'#13#10#9#9#9#9' left outer join cad_tb_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9'(tme.' +
      'id_tipo_mov_estoque = fea.id_tipo_mov_estoque)'#9#9#9#9' '#13#10#9#9'where fea' +
      '.id_item = ite.id_item and'#13#10'          fea.id_empresa = par.id_em' +
      'presa and'#13#10'          tme.tipo_movimento = 0    and  /* entradas ' +
      '*/'#9#9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tme.esto_movimenta = true and'#13#10'          f' +
      'ea."data" < '#39'01/01/2014'#39' and'#13#10'          fea.id_cor = cor.id_cor ' +
      '  and'#13#10'          fea.id_tamanho = tam.id_tamanho and'#13#10'          ' +
      'fea.id_almoxarifado = alm.id_almoxarifado) -'#13#10#13#10'   (select COALE' +
      'SCE(sum(fea.qtde),0)'#13#10#9#9'from est_tb_m_fea fea'#13#10#9#9#9#9' left outer j' +
      'oin cad_tb_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9'(tme.id_tipo_mov_estoque = fe' +
      'a.id_tipo_mov_estoque)'#9#9#9#9' '#13#10#9#9'where fea.id_item = ite.id_item a' +
      'nd'#13#10'          fea.id_empresa = par.id_empresa and'#13#10'          tme' +
      '.tipo_movimento = 1    and  /* saidas */'#9#9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tme.' +
      'esto_movimenta = true and'#13#10'          fea."data" < '#39'01/01/2014'#39' a' +
      'nd'#13#10'          fea.id_cor = cor.id_cor   and'#13#10'          fea.id_ta' +
      'manho = tam.id_tamanho and'#13#10'          fea.id_almoxarifado = alm.' +
      'id_almoxarifado) as saldo_anterior,'#13#10#13#10'   (select COALESCE(sum(f' +
      'ea.qtde),0)'#13#10#9#9'from est_tb_m_fea fea'#13#10#9#9#9#9' left outer join cad_t' +
      'b_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9'(tme.id_tipo_mov_estoque = fea.id_tipo' +
      '_mov_estoque)'#9#9#9#9' '#13#10#9#9'where fea.id_item = ite.id_item  and'#13#10'    ' +
      '      fea.id_empresa = par.id_empresa and'#13#10'          tme.tipo_mo' +
      'vimento = 0     and  /* entrada */'#9#9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tme.esto_m' +
      'ovimenta = true  and'#13#10'          fea."data" >= '#39'01/01/2014'#39' and '#13 +
      #10'          fea."data" <= '#39'01/01/2014'#39' and'#13#10'          fea.id_cor ' +
      '= cor.id_cor    and'#13#10'          fea.id_tamanho = tam.id_tamanho a' +
      'nd'#13#10'          fea.id_almoxarifado = alm.id_almoxarifado) as qtde' +
      '_entradas,'#13#10#13#10'  (select COALESCE(sum(fea.qtde),0)'#13#10#9#9'from est_tb' +
      '_m_fea fea'#13#10#9#9#9#9' left outer join cad_tb_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9 +
      '(tme.id_tipo_mov_estoque = fea.id_tipo_mov_estoque)'#9#9#9#9' '#13#10#9#9'wher' +
      'e fea.id_item = ite.id_item  and'#13#10'          fea.id_empresa = par' +
      '.id_empresa and'#13#10'          tme.tipo_movimento = 1     and  /* sa' +
      'idas */'#9#9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tme.esto_movimenta = true  and'#13#10'     ' +
      '     fea."data" >= '#39'01/01/2014'#39' and '#13#10'          fea."data" <= '#39'0' +
      '1/01/2014'#39' and'#13#10'          fea.id_cor = cor.id_cor   and'#13#10'       ' +
      '   fea.id_tamanho = tam.id_tamanho and'#13#10'          fea.id_almoxar' +
      'ifado = alm.id_almoxarifado) as qtde_saidas,'#13#10#13#10'  (select COALES' +
      'CE(sum(fea.qtde),0)'#13#10#9#9'from est_tb_m_fea fea'#13#10#9#9#9#9' left outer jo' +
      'in cad_tb_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9'(tme.id_tipo_mov_estoque = fea' +
      '.id_tipo_mov_estoque)'#9#9#9#9' '#13#10#9#9'where fea.id_item = ite.id_item  a' +
      'nd'#13#10'          fea.id_empresa = par.id_empresa and'#13#10'          tme' +
      '.tipo_movimento = 0     and  /* entrada */'#9#9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tm' +
      'e.esto_movimenta = true  and'#13#10'          fea."data" >= '#39'01/01/201' +
      '4'#39' and '#13#10'          fea."data" <= '#39'01/01/2014'#39' and'#13#10'          fea' +
      '.id_cor = cor.id_cor    and'#13#10'          fea.id_tamanho = tam.id_t' +
      'amanho and'#13#10'          fea.id_almoxarifado = alm.id_almoxarifado)' +
      ' -'#13#10#13#10'  (select COALESCE(sum(fea.qtde),0)'#13#10#9#9'from est_tb_m_fea f' +
      'ea'#13#10#9#9#9#9' left outer join cad_tb_c_tme tme on'#13#10#9#9#9#9#9#9#9#9#9#9'(tme.id_' +
      'tipo_mov_estoque = fea.id_tipo_mov_estoque)'#9#9#9#9' '#13#10#9#9'where fea.id' +
      '_item = ite.id_item  and'#13#10'          fea.id_empresa = par.id_empr' +
      'esa and'#13#10'          tme.tipo_movimento = 1     and  /* saidas */'#9 +
      #9#9'  '#9#9#9#9#9#9#9#9#13#10#9#9#9#9#9'tme.esto_movimenta = true  and'#13#10'          fea' +
      '."data" >= '#39'01/01/2014'#39' and '#13#10'          fea."data" <= '#39'01/01/201' +
      '4'#39' and'#13#10'          fea.id_cor = cor.id_cor   and'#13#10'          fea.i' +
      'd_tamanho = tam.id_tamanho and'#13#10'          fea.id_almoxarifado = ' +
      'alm.id_almoxarifado) as saldo,'#13#10#13#10#13#10' (select COALESCE(fes.vlr_cu' +
      'sto_medio,0) '#13#10'                               from est_tb_m_fes ' +
      'fes                                     '#13#10'                      ' +
      '            where fes.id_empresa = par.id_empresa and           ' +
      '      '#13#10'                 fes.id_item = ite.id_item  and         ' +
      '             '#13#10'                 COALESCE(fes.id_cor,0) = COALESC' +
