  object CAD_SQ_R_ITE: TSQLDataSet
    CommandText = 
      'select ITE.PRECO_AVISTA as prc_avista, '#13#10'           ITE.PRECO_AP' +
      'RAZO as prc_aprazo, 0 as int_cod_cat, CAST('#39#39' AS VARCHAR(60)) as' +
      ' int_desc_cat, ite.*, '#13#10'      loc.loca_rua as int_loca_rua,loc.l' +
      'oca_prateleira as int_loca_prateleira,loc.loca_escaninho as int_' +
      'loca_escaninho, '#13#10'       frn.fantasia as int_nomefan, '#13#10'       f' +
      'rn.descricao as int_nomefor,'#13#10'       gru.descricao as int_nomegr' +
      'u,'#13#10'       gru.tipo_item as int_tipoite,'#13#10'       gru.cores as in' +
      't_gru_cores,'#13#10'       ncm.descricao as int_nomencm,'#13#10'       ncm.c' +
      'est as int_cest,'#13#10'   case'#13#10'      when gru.tipo_item = '#39'00'#39' then ' +
      #39'Mercadoria para Revenda'#39#13#10'      when gru.tipo_item = '#39'01'#39' then ' +
      #39'Materia prima'#39#13#10'      when gru.tipo_item = '#39'02'#39' then '#39'Embalagem' +
      #39#13#10'      when gru.tipo_item = '#39'03'#39' then '#39'Produto em processo'#39#13#10' ' +
      '     when gru.tipo_item = '#39'04'#39' then '#39'Produto Acabado'#39#13#10'      whe' +
      'n gru.tipo_item = '#39'05'#39' then '#39'Subproduto'#39#13#10'      when gru.tipo_it' +
      'em = '#39'06'#39' then '#39'Produto Intermediario'#39#13#10'      when gru.tipo_item' +
      ' = '#39'07'#39' then '#39'Material de Uso e consumo'#39#13#10'      when gru.tipo_it' +
      'em = '#39'08'#39' then '#39'Ativo Imobilizado'#39#13#10'      when gru.tipo_item = '#39 +
      '09'#39' then '#39'Servi'#231'os'#39#13#10'      when gru.tipo_item = '#39'10'#39' then '#39'Outro' +
      's Insumos'#39#13#10'      when gru.tipo_item = '#39'99'#39' then '#39'Outros'#39#13#10'   en' +
      'd as int_nometip,'#13#10'coalesce(fes.saldo_fisico,0) as int_saldo,'#13#10'f' +
      'es.vlr_custo_medio as vlr_custo'#13#10'from cad_tb_c_ite ite'#13#10'left out' +
      'er join cad_tb_c_ite_loc loc on (loc.id_item=ite.id_item and loc' +
      '.id_empresa=0)'#13#10'left outer join cad_tb_c_for frn on (frn.id_forn' +
      'ecedor = ite.id_fornecedor)'#13#10'left outer join'#13#10'    ( select gru.i' +
      'd_grupo, gru.descricao,gru.tipo_item, '#13#10'           cast(coalesce' +
      '(string_agg(cor.descricao, '#39' - '#39' ),'#39'  Consultar a f'#225'brica'#39') as v' +
      'archar(100) ) as cores  from cad_tb_c_gru gru '#13#10'                ' +
      'left outer join cad_tb_c_gru_cor gcr on gcr.id_grupo = gru.id_gr' +
      'upo '#13#10'                left outer join cad_tb_c_cor cor on cor.id' +
      '_cor = gcr.id_cor '#13#10'      group by 1,2,3) gru on gru.id_grupo = ' +
      'ite.id_grupo '#13#10'left outer join cad_tb_c_ncm ncm on (ncm.id_ncm=i' +
      'te.id_ncm)'#13#10'left outer join est_tb_m_fes fes on (fes.id_item = i' +
      'te.id_item)'#13#10'WHERE 1=2'
    MaxBlobSize = -1
    Params = <>
