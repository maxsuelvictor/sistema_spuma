  object FIN_SQ_R_REC_ABE: TSQLDataSet
    CommandText = 
      'SELECT rec.id_titulo, rec.id_empresa, rec.num_parcela, rec.id_fo' +
      'rma_pag, rec.dta_emissao,rec.dta_original, rec.dta_vencimento, r' +
      'ec.car_taxa, fpg.doc_impresso,'#13#10'       rec.id_cliente, rec.id_lo' +
      'cal_titulo, rec.id_plano,   CLI.NOME AS INT_NOMECLI, CLI.TEL_FIX' +
      'O AS INT_TELFIXO,CLI.TEL_MOVEL AS INT_TELMOVEL,CLI.ENDERECO AS I' +
      'NT_ENDERECO,CLI.BAIRRO AS INT_BAIRRO,CLI.NUMERO AS INT_NUMERO,'#13#10 +
      'nfe.numero as int_numeronf, '#13#10' rec.id_vendedor, fun.nome as int_' +
      'nomeven,'#13#10'rec.id_vendedor as int_codvnd,fun.nome as int_nomevnd,' +
      #13#10'  FPG.DESCRICAO AS INT_NOMEFPG, '#13#10'  LTO.DESCRICAO AS INT_NOMEL' +
      'TO,'#13#10'  PCT.DESCRICAO AS INT_NOMEPCT,'#13#10'  rec.vlr_saldo,'#13#10'  rec.bo' +
      'l_nosso_numero,'#13#10'  ctc.banco as int_banco,'#13#10'  ctc.agencia as int' +
      '_agencia,'#13#10'  ctc.descricao as int_descconta,'#13#10'  ctc.conta_corren' +
      'te as int_contacorrente,'#13#10'  rec.vlr_parcela, rec.historico, '#39#39' a' +
      's prazo,'#13#10'  cid.id_cidade as int_codcidade,'#13#10'  cid.nome as int_n' +
      'omecid,'#13#10'  cid.uf as int_nomeuf,'#13#10'  (CURRENT_DATE -  rec.dta_ven' +
      'cimento) as INT_ATRASO,'#13#10'  CASE '#13#10'    WHEN (CURRENT_DATE -  rec.' +
      'dta_vencimento)<=0 THEN rec.vlr_saldo'#13#10'    WHEN (CURRENT_DATE - ' +
      ' rec.dta_vencimento)>0  THEN rec.vlr_saldo+((rec.vlr_saldo*(CTR.' +
      'tax_juros_mensal/30)/100)*(CURRENT_DATE -  rec.dta_vencimento))'#13 +
      #10'  END as INT_VLR_CORRIGIDO,'#13#10'  CASE '#13#10'    WHEN (CURRENT_DATE - ' +
      ' rec.dta_vencimento)<=0 THEN 0'#13#10'    WHEN (CURRENT_DATE -  rec.dt' +
      'a_vencimento)>0  THEN (rec.vlr_saldo+((rec.vlr_saldo*(CTR.tax_ju' +
      'ros_mensal/30)/100)*(CURRENT_DATE -  rec.dta_vencimento)))-rec.v' +
      'lr_saldo'#13#10'  END as INT_VLR_JUROS'#13#10#13#10'FROM FIN_TB_M_REC REC'#13#10'LEFT ' +
      'OUTER JOIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE = REC.ID_CLIENTE'#13#10 +
      'LEFT OUTER JOIN CAD_TB_C_FPG FPG ON FPG.ID_FORMA_PAG = REC.ID_FO' +
      'RMA_PAG'#13#10'LEFT OUTER JOIN CAD_TB_C_LTO LTO ON LTO.ID_LOCAL_TITULO' +
      ' = REC.ID_LOCAL_TITULO'#13#10'LEFT OUTER JOIN CAD_TB_C_PCT PCT ON PCT.' +
      'ID_PLANO = REC.ID_PLANO'#13#10'LEFT OUTER JOIN cad_tb_c_par_ctr CTR ON' +
      ' CTR.ID_EMPRESA = REC.ID_EMPRESA'#13#10'LEFT OUTER JOIN cad_tb_c_fun f' +
      'un ON fun.ID_FUNCIONARIO = REC.ID_VENDEDOR'#13#10'LEFT OUTER JOIN cad_' +
      'tb_c_ctc ctc ON ctc.ID_CONTA = REC.BOL_ID_CONTA'#13#10'LEFT OUTER JOIN' +
      ' cad_tb_c_cid cid ON cid.ID_CIDADE = CLI.ID_CIDADE'#13#10'LEFT OUTER J' +
      'OIN fat_tb_m_nfe nfe ON nfe.ID_FISCAL = REC.ID_FISCAL'#13#10'WHERE 1 =' +
      '2'
