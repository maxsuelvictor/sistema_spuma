      'LMOV,'#13#10'       case'#13#10'         when VEI.COMBUSTIVEL = 0 then '#39'Gaso' +
      'lina'#39#13#10'         when VEI.COMBUSTIVEL = 1 then '#39'Alcool'#39#13#10'        ' +
      ' when VEI.COMBUSTIVEL = 2 then '#39'Diesel'#39#13#10'         when VEI.COMBU' +
      'STIVEL = 3 then '#39'Gas Natural'#39#13#10'         when VEI.COMBUSTIVEL = 4' +
      ' then '#39'Eletrico'#39#13#10'         when VEI.COMBUSTIVEL = 5 then '#39'Flex'#39#13 +
      #10'       end as INT_NOMECOM,'#13#10'       VEI.ID_PLACA AS INT_PLACA,  ' +
      ' VEI.DESCRICAO AS INT_NOMEVEI,'#13#10'       TIO.DESCRICAO AS INT_NOME' +
      'TIO,'#13#10'       TIO.TIPO AS INT_TIPO_OS,'#13#10'       CID.UF AS INT_UFCL' +
      'I,'#13#10'       MVE.DESCRICAO AS INT_NOMEMVE,'#13#10'      cast(case status' +
      '_painel   '#13#10'              when  0  then '#39'Aguardando inicio do se' +
      'rvi'#231'o'#39'                        '#13#10'               when  1  then '#39'Pr' +
      #243'ximo Ordem de Servi'#231'o'#39#13#10'               when  2  then '#39'Em Servi'#231 +
      'o'#39#13#10'               when  3  then '#39'Pr'#233'-Inspe'#231#227'o'#39#13#10'               ' +
      'when  4  then '#39'Aguardando Lavagem'#39#13#10'               when  5  then' +
      ' '#39'Inspe'#231#227'o Final'#39#13#10'               when  6  then '#39'Aguardando Apro' +
      'va'#231#227'o'#39#13#10'               when  7  then '#39'Aguardando Pe'#231'as'#39#13#10'       ' +
      '        when  8  then '#39'Aguardando Servi'#231'o'#39#13#10'               when ' +
      ' 9  then '#39'Aguardando Pr'#233'-Entrega'#39#13#10'               when  10 then ' +
      #39'Aguardando Entrega'#39#13#10'               when  11 then '#39'Entregue'#39#13#10' ' +
      '           end  as varchar(30)) as int_statuspainel ,'#13#10'        e' +
      'qp.descricao as int_nomeeqp'#13#10'FROM OFI_TB_M_ORV ORV'#13#10'   LEFT OUTE' +
      'R JOIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE=ORV.ID_CLIENTE'#13#10'   LEF' +
