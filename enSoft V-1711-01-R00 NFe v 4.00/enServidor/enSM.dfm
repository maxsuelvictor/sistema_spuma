      #10'            VEI.ID_PLACA AS INT_PLACA,   VEI.DESCRICAO AS INT_N' +
      'OMEVEI,ORI.ID_MECANICO,'#13#10'            TIO.DESCRICAO AS INT_NOMETI' +
      'O,'#13#10'          cast( case orv.status'#13#10'             when  0 then '#39 +
      'Aberta'#39#13#10'             when 1 then '#39'Em andamento'#39#13#10'             w' +
      'hen 2 then '#39'Fechada'#39#13#10'             when 3 then '#39'Faturada'#39#13#10'     ' +
