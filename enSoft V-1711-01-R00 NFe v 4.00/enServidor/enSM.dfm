  object FIN_SQ_R_EXT_FOR_EST: TSQLDataSet
    CommandText = 
      'select "for".id_fornecedor,"for".lim_valor,'#13#10#13#10'        (select c' +
      'oalesce(sum(pag.vlr_saldo),0) from fin_tb_m_pag pag '#13#10'         w' +
      'here pag.vlr_saldo>0) as vlr_total_tit_aberto, '#13#10#13#10'         (sel' +
      'ect sum(pag.vlr_saldo) from fin_tb_m_pag pag where pag.dta_venci' +
      'mento<CURRENT_DATE and pag.vlr_saldo>0) as vlr_total_tit_vencido' +
      ', '#13#10#13#10'        (select COALESCE(sum(((pag.vlr_saldo*(COALESCE(CTR' +
      '.tax_juros_mensal,0)/30)/100)* '#13#10'              (CURRENT_DATE -  ' +
      'pag.dta_vencimento))),0) '#13#10'          from fin_tb_m_pag pag      ' +
      '                   '#13#10'          left outer join cad_tb_c_par_ctr ' +
      'ctr on ctr.id_empresa=pag.id_empresa '#13#10'         where pag.dta_ve' +
      'ncimento<CURRENT_DATE)  as vlr_total_tit_juros, '#13#10#13#10#13#10'        (s' +
      'elect COALESCE(sum(pag.vlr_saldo+((pag.vlr_saldo*(COALESCE(CTR.t' +
      'ax_juros_mensal,0)/30)/100)* '#13#10'                (CURRENT_DATE -  ' +
      'pag.dta_vencimento))),0) from fin_tb_m_pag pag '#13#10'         left o' +
      'uter join cad_tb_c_par_ctr ctr on ctr.id_empresa=pag.id_empresa ' +
      #13#10'         where pag.dta_vencimento<CURRENT_DATE) as vlr_total_t' +
      'it_corrigido,'#13#10#13#10'        (select COALESCE(sum(pag.vlr_saldo),0) ' +
      'from fin_tb_m_pag pag where pag.dta_vencimento>=CURRENT_DATE and' +
      ' pag.vlr_saldo>0) as vlr_total_tit_vencer,'#13#10#13#10'        (select CO' +
      'ALESCE(sum(pag.vlr_saldo),0) from fin_tb_m_pag pag '#13#10'         le' +
      'ft outer join cad_tb_c_fpg fpg on fpg.id_forma_pag=pag.id_forma_' +
      'pag '#13#10'         where fpg.doc_impresso=6 and pag.vlr_saldo>0 ) as' +
      '  vlr_che_aberto,'#13#10#13#10'        (select COALESCE(sum(pag.vlr_parcel' +
      'a),0) from fin_tb_m_pag pag where pag.vlr_saldo=0) as vlr_total_' +
      'tit_pagos,'#13#10#13#10'        (select COALESCE(sum(nfe.vlr_liquido),0)  ' +
      'from fat_tb_m_nfe nfe '#13#10'         LEFT OUTER JOIN CAD_TB_C_CFO CF' +
      'O ON CFO.ID_CFO=NFE.ID_CFO'#13#10'          WHERE CFO.ID_CLASSE IN ('#39'S' +
      'P10'#39','#39'SP70'#39') AND NFE.NFE_COD_SIT not in ('#39'02'#39','#39'04'#39','#39'05'#39') AND NFE' +
      '.STATUS = 1 AND'#13#10'             NFE.FATURADA = true AND NFE.TIPO_N' +
      'F='#39'S'#39')  as vlr_total_compras,'#13#10#13#10'         (select COALESCE(sum(n' +
      'fe.vlr_liquido),0)  from fat_tb_m_nfe nfe'#13#10'         WHERE NFE.ST' +
      'ATUS = 1 AND                                        '#13#10'          ' +
      '   NFE.FATURADA = true AND NFE.TIPO_NF='#39'R'#39') as vlr_total_dev '#13#10#13 +
      #10'        from cad_tb_c_for "for" where 1=2'
    MaxBlobSize = -1
