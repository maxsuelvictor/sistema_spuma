  object FIN_SQ_R_CXA_DIA_NFT: TSQLDataSet
    CommandText = 
      'select nfe.*,cli.nome as int_nomecli,cpg.descricao as int_nomecp' +
      'g,tme.descricao as int_nometme,'#13#10'   cast (case nfe_cod_sit '#13#10'   ' +
      '         when '#39'00'#39' then '#39'Documento regular'#39#13#10'            when '#39'0' +
      '1'#39' then '#39'Escritura'#231#227'o extempor'#226'nea de documento regular'#39#13#10'      ' +
      '      when '#39'02'#39' then '#39'Documento cancelado'#39#13#10'            when '#39'03' +
      #39' then '#39'Escritura'#231#227'o extempor'#226'nea de documento cancelado'#39#13#10'     ' +
      '       when '#39'04'#39' then '#39'NF-e denegada ou CT-e denegado'#39#13#10'        ' +
      '    when '#39'05'#39' then '#39'NF-e ou CT-e com numera'#231#227'o inutilizada'#39#13#10'   ' +
      '         when '#39'06'#39' then '#39'Documento fiscal complementar'#39#13#10'       ' +
      '     when '#39'07'#39' then '#39'Escritura'#231#227'o extempor'#226'nea de documento comp' +
      'lementar'#39#13#10'            when '#39'08'#39' then '#39'Documento fiscal emitido ' +
      'com base em regime especial ou norma espec'#237'fica'#39#13#10'          end ' +
      'as varchar(80) ) as int_descnfecodsit'#13#10#13#10'from fat_tb_m_nfe nfe '#13 +
      #10'left outer join cad_tb_c_cli cli on cli.id_cliente=nfe.id_emite' +
      'nte'#13#10'LEFT OUTER JOIN FIN_TB_M_CXA CXA ON CXA.ID_ABERTURA=NFE.ID_' +
      'ABERTURA  '#13#10'LEFT OUTER JOIN CAD_TB_C_CPG CPG ON CPG.ID_CONDICAO_' +
      'PAG=nfe.id_condicao_pag  '#13#10'LEFT OUTER JOIN CAD_TB_C_TME TME ON T' +
      'ME.ID_TIPO_MOV_ESTOQUE=nfe.ID_TIPO_MOV_ESTOQUE     '#13#10'where nfe.t' +
      'ipo_nf='#39'S'#39'  and nfe.faturada=true'#13#10' AND 1=2'
    MaxBlobSize = -1
    Params = <>
    SQLConnection = Conexao
    Left = 3120
    Top = 1080
  end
  object FAT_DP_R_COV_REC: TDataSetProvider
