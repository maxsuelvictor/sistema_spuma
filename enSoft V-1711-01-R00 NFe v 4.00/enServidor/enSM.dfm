      'TERIOR, '#13#10'               '#13#10#13#10'     CAST (CASE RCR.DEB_CRE'#13#10'      ' +
      '  WHEN 0 THEN 0'#13#10'        WHEN 1 THEN RCR.VLR_CREDITO'#13#10'      END ' +
      'AS NUMERIC(18,4)) AS ENTRADA,'#13#10'     CAST (CASE RCR.DEB_CRE'#13#10'    ' +
      '    WHEN 0 THEN RCR.VLR_CREDITO'#13#10'        WHEN 1 THEN 0'#13#10'      EN' +
