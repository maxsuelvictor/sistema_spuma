  object FAT_SQ_R_IQM_ITE: TSQLDataSet
    CommandText = 
      'select  ipt.*,'#13#10'  fun.nome AS desc_fun, ite.id_und_compra, ite.d' +
      'escricao AS desc_ite, '#13#10'  und.descricao AS desc_und,iqm.id_emite' +
      'nte,'#13#10'  frn.descricao, frn.fantasia, fcn.nome AS nome_apro_geren' +
      'te, '#13#10'  fco.nome AS nome_exec,  '#13#10'  iqm.dta_recebimento, iqm.dta' +
      '_emissao_nf, iqm.numero_nf,'#13#10#13#10'  case ipt.cnd_comercial '#13#10'      ' +
      'WHEN '#39'1'#39' THEN '#39'Aprovado'#39#13#10'      WHEN '#39'0'#39' THEN '#39'Reprovado'#39#13#10'  END' +
      ' DESC_CND_COMER, '#13#10#13#10' CASE ipt.QTD_PC_NF '#13#10'      WHEN '#39'1'#39' THEN '#39 +
      'Aprovado'#39#13#10'      WHEN '#39'0'#39' THEN '#39'Reprovado'#39#13#10' END DESC_QTD, '#13#10'   ' +
      '                                       '#13#10' CASE ipt.QUALIDADE '#13#10' ' +
      '     WHEN '#39'1'#39' THEN '#39'Aprovado'#39#13#10'      WHEN '#39'0'#39' THEN '#39'Reprovado'#39#13#10 +
      ' END DESC_QUAL, '#13#10'                                         '#13#10' CA' +
      'SE ipt.PRES_PRODUTO '#13#10'      WHEN '#39'1'#39' THEN '#39'Aprovado'#39#13#10'      WHEN' +
      ' '#39'0'#39' THEN '#39'Reprovado'#39#13#10' END DESC_PRED_PROD, '#13#10'                  ' +
      '                        '#13#10' CASE ipt.CERT_QUALIDADE '#13#10'      WHEN ' +
      #39'1'#39' THEN '#39'Aprovado'#39#13#10'      WHEN '#39'0'#39' THEN '#39'Reprovado'#39#13#10' END DESC_' +
      'CERT_QUAL, '#13#10'                                          '#13#10' CASE i' +
      'pt.RESULTADO '#13#10'      WHEN '#39'1'#39' THEN '#39'Aprovado'#39#13#10'      WHEN '#39'0'#39' TH' +
      'EN '#39'Reprovado'#39#13#10' END DESC_RESUL   '#13#10'                            ' +
      '              '#13#10' from fat_tb_m_iqm_ite ipt'#13#10'left outer join fat_' +
      'tb_m_iqm iqm on iqm.id_iqm = ipt.id_iqm'#13#10'left outer join cad_tb_' +
      'c_fun fun     on (fun.id_funcionario = ipt.id_responsavel)'#13#10'left' +
