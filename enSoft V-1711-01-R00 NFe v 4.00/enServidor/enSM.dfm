  object BUS_SQ_C_CSU_MTV_HST: TSQLDataSet
    CommandText = 
      'select                                                 '#13#10'   0 as' +
      ' ano,                         '#13#10'    fun.id_funcionario,         ' +
      '                          '#13#10'    ite.id_item,                    ' +
      '                       '#13#10'    ite.descricao as int_nomeite,      ' +
      '                    '#13#10#13#10'    coalesce(sum(nfi.QTDE-nfi.QTDE_DEVOL' +
      'VIDA),0) as total, '#13#10#13#10#13#10'    COALESCE(sum(case extract(month fro' +
      'm (NFE.DTA_EMISSAO)) '#13#10'         when 1 then nfi.QTDE-nfi.QTDE_DE' +
      'VOLVIDA            '#13#10'       end),0) as mes_01,                  ' +
      '                 '#13#10#13#10#13#10'    COALESCE(sum(case extract(month from ' +
      '(NFE.DTA_EMISSAO)) '#13#10'         when 2 then nfi.QTDE-nfi.QTDE_DEVO' +
      'LVIDA            '#13#10'       end),0) as mes_02,                    ' +
      '               '#13#10#13#10'    COALESCE(sum(case extract(month from (NFE' +
      '.DTA_EMISSAO)) '#13#10'         when 3 then nfi.QTDE-nfi.QTDE_DEVOLVID' +
      'A            '#13#10'       end),0) as mes_03,                        ' +
      '           '#13#10#13#10'   COALESCE(sum(case extract(month from (NFE.DTA_' +
      'EMISSAO)) '#13#10'         when 4 then nfi.QTDE-nfi.QTDE_DEVOLVIDA    ' +
      '        '#13#10'       end),0) as mes_04,                             ' +
      '      '#13#10#13#10'    COALESCE(sum(case extract(month from (NFE.DTA_EMIS' +
      'SAO)) '#13#10'        when 5 then nfi.QTDE-nfi.QTDE_DEVOLVIDA         ' +
      '    '#13#10'       end),0) as mes_05,                                 ' +
      '  '#13#10#13#10'    COALESCE(sum(case extract(month from (NFE.DTA_EMISSAO)' +
      ') '#13#10'         when 6 then nfi.QTDE-nfi.QTDE_DEVOLVIDA            ' +
      #13#10'       end),0) as mes_06,                                   '#13#10 +
      #13#10'    COALESCE(sum(case extract(month from (NFE.DTA_EMISSAO)) '#13#10 +
      '         when 7 then nfi.QTDE-nfi.QTDE_DEVOLVIDA            '#13#10'  ' +
      '     end),0) as mes_07,                                   '#13#10#13#10'  ' +
      '  COALESCE(sum(case extract(month from (NFE.DTA_EMISSAO)) '#13#10'    ' +
      '     when 8 then nfi.QTDE-nfi.QTDE_DEVOLVIDA            '#13#10'      ' +
      ' end),0) as mes_08,                                   '#13#10#13#10'    CO' +
      'ALESCE(sum(case extract(month from (NFE.DTA_EMISSAO)) '#13#10'        ' +
      ' when 9 then nfi.QTDE-nfi.QTDE_DEVOLVIDA            '#13#10'       end' +
      '),0) as mes_09,                                   '#13#10#13#10'    COALES' +
      'CE(sum(case extract(month from (NFE.DTA_EMISSAO)) '#13#10'         whe' +
      'n 10 then nfi.QTDE-nfi.QTDE_DEVOLVIDA           '#13#10'       end),0)' +
      ' as mes_10,                                   '#13#10#13#10'    COALESCE(s' +
      'um(case extract(month from (NFE.DTA_EMISSAO)) '#13#10'         when 11' +
      ' then nfi.QTDE-nfi.QTDE_DEVOLVIDA           '#13#10'       end),0) as ' +
      'mes_11,                                   '#13#10#13#10'    COALESCE(sum(c' +
      'ase extract(month from (NFE.DTA_EMISSAO)) '#13#10'         when 12 the' +
      'n nfi.QTDE-nfi.QTDE_DEVOLVIDA           '#13#10'       end),0) as mes_' +
      '12                                    '#13#10#13#10'    from cad_tb_c_fun ' +
      'fun                                    '#13#10'    left outer join cad' +
      '_tb_c_ite ite on ite.id_grupo_prod='#39'0'#39'    '#13#10'    LEFT OUTER JOIN ' +
