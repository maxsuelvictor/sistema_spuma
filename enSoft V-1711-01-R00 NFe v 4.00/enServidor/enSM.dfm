      'CPG CPG ON CPG.ID_CONDICAO_PAG=PED.ID_CONDICAO_PAG'#13#10'LEFT OUTER J' +
      'OIN CAD_TB_C_FUN ATE ON ATE.ID_FUNCIONARIO=PED.ID_ATENDENTE'#13#10'LEF' +
      'T OUTER JOIN CAD_TB_C_FUN RES ON RES.ID_FUNCIONARIO=PED.ID_RESPO' +
