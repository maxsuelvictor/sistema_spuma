  object CAD_SQ_C_ITE: TSQLDataSet
    SchemaName = 'dbo'
    CommandText = 
      'SELECT ITE.*,cast(null as bytea) as foto_temp, NCM.DESCRICAO AS ' +
      'INT_NOMENCM ,NCM.PER_IMPOSTO AS INT_PERIMPOSTONCM,'#13#10'  GRU.CTC_EP' +
      'I INT_CTC_EPI, GRU.DESCRICAO INT_DESC_GRUPO,'#13#10'   GRU.TIPO_ITEM I' +
      'NT_TIPO_ITEM,'#13#10'   GRU.SER_TERCEIRO AS INT_SERTERCEIRO,'#13#10'   GRU.O' +
      'RIGEM_MERCADORIA INT_ORIGEM, '#13#10'   GRU.ID_TRIBUTO INT_TRIBUTO, '#13#10 +
      '   GRU.MVA INT_MVA, FON.DESCRICAO AS INT_NOMEFOR,'#13#10'   GRU.ID_FTE' +
      '_ETQ AS INT_FTEETQ,'#13#10'   GRU.imprime_selo_etq AS INT_IMPSELO,'#13#10'  ' +
      ' GRU.ctc_epi As INT_CTCEPI,'#13#10'   GRU.id_st_pis_entrada as int_pis' +
      '_entrada,   GRU.id_st_pis_saida as int_pis_saida,'#13#10'   GRU.id_st_' +
      'cof_entrada as int_cof_entrada,'#13#10'   GRU.id_st_cof_saida as int_c' +
      'of_saida,'#13#10'   GRU.id_st_ipi_entrada as int_ipi_entrada,'#13#10'   GRU.' +
      'id_st_ipi_saida as int_ipi_saida,'#13#10'   GRU.imprime_msq_etq AS int' +
      '_impmsgetq,'#13#10'   GRU.msg_etq1 as int_msg_etq1, '#13#10'   GRU.msg_etq2 ' +
      'as int_msg_etq2, '#13#10'   GRU.msg_etq3 as int_msg_etq3, '#13#10'   GRU.msg' +
      '_etq4 as int_msg_etq4, '#13#10'   GRU.msg_etq5 as int_msg_etq5, '#13#10'   G' +
      'RU.msg_etq6 as int_msg_etq6,'#13#10'   GRU.msg_etq7 as int_msg_etq7,'#13#10 +
      '   GRU.dre_perc_ircs as int_dre_perc_ircs,'#13#10'   GRU.dre_perc_out_' +
      'ded_ven as int_dre_perc_out_ded_ven,'#13#10'   GRU.ctr_lot_entr as int' +
      '_ctr_lot_entr,'#13#10'   GRU.sgq_verif_cod_barra_ped AS int_verif_cod_' +
      'barra_ped,'#13#10'   GRP.descricao as int_nomegrp,'#13#10'   gru.imp_etq_sim' +
      'ples as int_impetqsimples, cast(gru.path_etq_simples as varchar(' +
      '100)) as int_pathetqsimples,'#13#10'   gru.utiliza_lote as int_lote_gr' +
      'u,gru.ali_interna_icms as int_ali_interna_icms, ncm.cest as int_' +
      'cest,'#13#10'   UND.INF_TEMPO_ITE_COJ AS INT_INF_TEMPO_ITE_COJ,'#13#10'   gr' +
      'u.tipo_produto as int_tipo_produto,'#13#10'   mar.descricao as int_nom' +
      'emar,'#13#10'   gru.sgq_bloco_espuma as int_sgqblocoespuma,  gru.sgq_c' +
      'alc_bloco_produzir as int_sgqcalcbloco,'#13#10#13#10'   cast ( case ite.et' +
      'iq_tipo_produto'#13#10'       when 0 then '#39'COLCH'#195'O DE ESPUMA'#39#13#10'       ' +
      'when 1 then '#39'COLCH'#195'O BOX CONJUGADO'#39#13#10'       when 2 then '#39'COLCH'#195'O' +
      ' AUXILIAR'#39#13#10'       when 3 then '#39'COLCH'#195'O MISTO'#39#13#10'   else '#39'?????'#39' ' +
      'end  as varchar(50) ) as int_etiq_tipo_produto,'#13#10'  '#13#10'   itt.desc' +
      'ricao as int_nome_item_caixa_itt'#13#10'  '#13#10#13#10'FROM CAD_TB_C_ITE ITE'#13#10' ' +
      'LEFT OUTER JOIN CAD_TB_C_GRU GRU ON GRU.ID_GRUPO=ITE.ID_GRUPO'#13#10' ' +
      'LEFT OUTER JOIN CAD_TB_C_FOR FON ON FON.ID_FORNECEDOR=ITE.ID_FOR' +
      'NECEDOR'#13#10' LEFT OUTER JOIN CAD_TB_C_NCM NCM ON NCM.ID_NCM = ITE.I' +
      'D_NCM'#13#10' LEFT OUTER JOIN CAD_TB_C_GRP GRP ON GRP.ID_CODIGO = ITE.' +
      'ID_GRUPO_PROD'#13#10' LEFT OUTER JOIN CAD_TB_C_UND UND ON UND.ID_UNIDA' +
      'DE = ITE.ID_UND_VENDA'#13#10' LEFT OUTER JOIN CAD_TB_C_MAR MAR ON MAR.' +
      'ID_MAR = ITE.ID_MARCA'#13#10' LEFT OUTER JOIN CAD_TB_C_ITE ITT ON ITT.' +
      'ID_ITEM = ITE.ID_ITEM_CAIXA'#13#10#13#10'where 1 = 2'
