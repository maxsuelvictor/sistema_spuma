      'MECID, CID.UF AS INT_NOMEEST, CLI.doc_cnpj_cpf AS INT_CPFCNPJ,'#13#10 +
      '       CLI.pessoa as int_pessoacli,FNL.NOME AS INT_NOMELIB,'#13#10#13#10' ' +
      '      FPI.NOME AS INT_NOMEPRI_IMPRES,  FSI.NOME AS INT_NOMESEG_I' +
      'MPRES,'#13#10'       FTI.NOME AS INT_NOMETER_IMPRES,'#13#10#13#10#13#10'       CLI.I' +
      'D_PERFIL_CLI AS INT_ID_PERFIL,'#13#10'        ORS.ID_ORS,'#13#10'cast( case ' +
      'ped.situacao_aprovacao '#13#10'        when 0 then '#39'Em espera'#39#13#10'      ' +
      '   when 1 then '#39'Aprovado'#39#13#10'         when 2 then '#39'Reprovado'#39#13#10'   ' +
      '    end as varchar(20) ) as int_sitaprov, '#13#10#13#10' cast( case ped.si' +
      'tuacao '#13#10'        when 0 then '#39'Em aberto'#39#13#10'         when 1 then '#39 +
      'Reprovado'#39#13#10'         when 2 then '#39'Em produ'#231#227'o'#39#13#10'         when 3 ' +
      'then '#39'Faturado'#39#13#10'         when 4 then '#39'Cancelado'#39#13#10'       end as' +
      ' varchar(20) ) as int_sitped,'#13#10#13#10'cast( case      '#13#10'           wh' +
      'en (ped.situacao = 0) and (ped.situacao_aprovacao = 0) then '#39'Dig' +
      'itado'#39#13#10'           when (ped.situacao = 0) and (ped.situacao_apr' +
      'ovacao = 1) then '#39'Aprovado'#39#13#10'           when (ped.situacao = 1) ' +
      'then '#39'Reprovado'#39#13#10'           when (ped.situacao = 2) then '#39'Em pr' +
      'odu'#231#227'o'#39#13#10'           when (ped.situacao = 3) then '#39'Faturado'#39#13#10'   ' +
      '        when (ped.situacao = 4) then '#39'Cancelado'#39#13#10'         end a' +
      's varchar(20) ) as int_sitped2,'#13#10#13#10'case      '#13#10'           when (' +
      'ped.situacao = 0) and (ped.situacao_aprovacao = 0) then 1 '#13#10'    ' +
      '       when (ped.situacao = 0) and (ped.situacao_aprovacao = 1) ' +
      'then 2 '#13#10'           when (ped.situacao = 2) then 3 '#13#10'           ' +
      'when (ped.situacao = 3) then 4'#13#10'           when (ped.situacao = ' +
      '4) then 5'#13#10'           when (ped.situacao = 1) then 6'#13#10'          ' +
      '   end  as int_sitOrd, '#13#10#13#10#13#10'cli.doc_ie_identidade as int_ie_rg_' +
      'cli,'#13#10#13#10'reg.descricao as int_nomereg,OPR.num_lote as int_numlote' +
      'opr,'#13#10'fun.tel_movel as  int_telmovel_ven, fun.tel_fixo as int_te' +
      'lfixo_ven,'#13#10'mta.nome as int_nomemta'#13#10'     '#13#10#13#10' --            (lp' +
      'ad(CAST(EXTRACT(HOUR FROM PED.HOR_PEDIDO) as varchar(2)),2,'#39'0'#39')|' +
      '|'#39':'#39'||'#13#10' --             lpad(CAST(EXTRACT(MINUTE FROM PED.HOR_PE' +
      'DIDO) as varchar(2)),2,'#39'0'#39'))  AS INT_HORA '#13#10'FROM FAT_TB_M_PED PE' +
      'D'#13#10'LEFT OUTER JOIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE = PED.ID_C' +
      'LIENTE'#13#10'LEFT OUTER JOIN CAD_TB_C_TME TME ON TME.ID_TIPO_MOV_ESTO' +
      'QUE = PED.ID_TIPO_MOV_ESTOQUE'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN FUN ' +
