      'tb_m_iqm iqm'#13#10'  where  '#13#10'       extract(year from iqm.dta_recebi' +
      'mento) = 2014 and'#13#10'       extract(month from  iqm.dta_recebiment' +
      'o) BETWEEN 07 and 12) as qtde_nfs_semestre2,'#13#10#13#10'        (select ' +
      'count(*)  '#13#10'            from fat_tb_m_iqm_ite_ana ana '#13#10'        ' +
      '         left outer join fat_tb_m_iqm iqm on   '#13#10'               ' +
      '             (iqm.id_iqm =  ana.id_iqm) '#13#10'            where iqm.' +
      'id_emitente = frn.id_fornecedor and       '#13#10'                  iq' +
      'm.id_empresa = 1 and'#13#10'                  extract(year from  iqm.d' +
      'ta_emissao_nf) = 2014 and '#13#10'                  extract(month from' +
      ' iqm.dta_emissao_nf) BETWEEN 01 and 06) as qtde_planoacao_semest' +
      're1, '#13#10#13#10'        (select count(*)  '#13#10'            from fat_tb_m_i' +
      'qm_ite_ana ana '#13#10'                 left outer join fat_tb_m_iqm i' +
      'qm on   '#13#10'                            (iqm.id_iqm =  ana.id_iqm)' +
      ' '#13#10'            where iqm.id_emitente = frn.id_fornecedor and    ' +
      '   '#13#10'                  iqm.id_empresa = 1 and'#13#10'                 ' +
      ' extract(year from  iqm.dta_emissao_nf) = 2014 and '#13#10'           ' +
      '       extract(month from iqm.dta_emissao_nf) BETWEEN 07 and 12)' +
      ' as qtde_planoacao_semestre2, '#13#10#13#10#13#10'(select count(*) from fat_tb' +
      '_m_iqm_ite qmi'#13#10'            left outer join fat_tb_m_iqm iqm on ' +
      #13#10'                 (iqm.id_iqm = qmi.id_iqm)    '#13#10'            wh' +
      'ere iqm.id_empresa = 1 and'#13#10'                 qmi.demerito > 0 an' +
      'd'#13#10'               extract(year from iqm.dta_recebimento) = 2014 ' +
      'and'#13#10'               extract(month from iqm.dta_recebimento) BETW' +
      'EEN 01 and 06) as qtde_nfs_demerito_sem1,'#13#10#13#10'(select count(*) fr' +
      'om fat_tb_m_iqm_ite qmi'#13#10'            left outer join fat_tb_m_iq' +
      'm iqm on '#13#10'                 (iqm.id_iqm = qmi.id_iqm)    '#13#10'     ' +
      '       where iqm.id_empresa = 1 and'#13#10'                 qmi.demeri' +
      'to > 0 and'#13#10'               extract(year from iqm.dta_recebimento' +
      ') = 2014 and'#13#10'               extract(month from iqm.dta_recebime' +
      'nto) BETWEEN 07 and 12) as qtde_nfs_demerito_sem2'#13#10#13#10#13#10'from cad_' +
      'tb_c_for frn'#13#10'where 1 = 2'#13#10'group by frn.id_fornecedor'
