      '    when NFE.DTA_EMISSAO>='#39'01/09/2016'#39#13#10'               and NFE.D' +
      'TA_EMISSAO<='#39'30/09/2016'#39#13#10'               then NIT.QTDE-NIT.QTDE_' +
      'DEVOLVIDA '#13#10'                 end),0) as mes_2,'#13#10#13#10'     COALESCE(' +
      'sum(case '#13#10'               when NFE.DTA_EMISSAO>='#39'01/09/2016'#39#13#10'  ' +
      '             and NFE.DTA_EMISSAO<='#39'30/09/2016'#39#13#10'               t' +
      'hen NIT.QTDE-NIT.QTDE_DEVOLVIDA '#13#10'                 end),0) as me' +
      's_3,'#13#10#13#10'      COALESCE(sum(case '#13#10'               when NFE.DTA_EM' +
