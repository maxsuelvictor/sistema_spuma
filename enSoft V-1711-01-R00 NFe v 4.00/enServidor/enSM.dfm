  object BUS_SQ_M_REC: TSQLDataSet
    CommandText = 
      'SELECT false as int_selecao,  REC.*,'#13#10'  CLI.NOME AS INT_NOMECLI,' +
      ' '#13#10'  FPG.DESCRICAO AS INT_NOMEFPG,'#13#10'  FPG.DOC_IMPRESSO AS INT_DO' +
      'CIMPRESSO, '#13#10'  FPG.SGQ_PRESTACAO_CONTA AS int_fpg_sgq_presta_con' +
      'ta,'#13#10'  LTO.DESCRICAO AS INT_NOMELTO,'#13#10'  PCT.DESCRICAO AS INT_NOM' +
      'EPCT,'#13#10'  PAR.EMP_FANTASIA AS INT_NOMEEMP,'#13#10'  CTC.DESCRICAO AS IN' +
      'T_NOMECTCBOL,'#13#10'  NFE.NUMERO AS INT_NUMERONF,'#13#10'  NFE.ID_PEDIDO_VE' +
      'NDA AS INT_NUMPEDVENDA,'#13#10' NFE.ID_ORDEM AS INT_NUMORDEM, NFE.ID_O' +
      'RS AS INT_ID_ORS,'#13#10'  cast( case rec.origem '#13#10'         when 0 the' +
      'n '#39'Manual'#39#13#10'          when 1 then '#39'Baixa do contas a receber'#39#13#10' ' +
      '         when 2 then '#39'Nota fiscal de Sa'#237'da'#39#13#10'          when 3 th' +
      'en '#39'Movimenta'#231#227'o de cheque'#39#13#10'        else '#39'?????'#39' end as varchar' +
      '(40)) int_descorigem,'#13#10#13#10' --   case'#13#10'     -- when rec.vlr_saldo<' +
      '=0 then null'#13#10'   --    else (CURRENT_DATE - rec.dta_vencimento) ' +
      'end  as INT_ATRASO,  '#13#10#13#10'    COALESCE( '#13#10'    (case'#13#10'      when r' +
      'ec.vlr_saldo<=0 then'#13#10'        case  '#13#10'                when pag.d' +
      'ta_pagamento <= rec.dta_vencimento then 0'#13#10'                when ' +
      'pag.dta_pagamento > rec.dta_vencimento then pag.dta_pagamento - ' +
      'rec.dta_vencimento'#13#10'              end'#13#10'       else '#13#10'           ' +
      ' case  '#13#10'                when CURRENT_DATE <= rec.dta_vencimento' +
      ' then 0'#13#10'                when CURRENT_DATE > rec.dta_vencimento ' +
      'then CURRENT_DATE - rec.dta_vencimento'#13#10'              end'#13#10'     ' +
      '    end),0)  as INT_ATRASO, '#13#10#13#10'              CASE '#13#10'           ' +
      '     WHEN (CURRENT_DATE -  rec.dta_vencimento)<=0 THEN rec.vlr_s' +
      'aldo '#13#10'                WHEN (CURRENT_DATE -  rec.dta_vencimento)' +
      '>0  THEN '#13#10'                     rec.vlr_saldo+((rec.vlr_saldo*(C' +
      'TR.tax_juros_mensal/30)/100)* '#13#10'                     (CURRENT_DA' +
      'TE -  rec.dta_vencimento)) '#13#10'              END as INT_VLR_CORRIG' +
      'IDO, '#13#10'              CASE '#13#10'                WHEN (CURRENT_DATE -' +
      '  rec.dta_vencimento)<=0 THEN 0 '#13#10'                WHEN (CURRENT_' +
      'DATE -  rec.dta_vencimento)>0  THEN '#13#10'                     ((rec' +
      '.vlr_saldo*(CTR.tax_juros_mensal/30)/100)* '#13#10'                   ' +
      '  (CURRENT_DATE -  rec.dta_vencimento))'#13#10'              END as IN' +
      'T_VLR_JUROS,'#13#10#13#10'          cast(case                             ' +
      '            '#13#10'                when (rec.DTA_VENCIMENTO < CURRENT' +
      '_DATE ) and   '#13#10'                    (REC.vlr_saldo>0) then '#39'1-Ve' +
      'ncido'#39'       '#13#10'                 when (CURRENT_DATE = REC.DTA_VEN' +
      'CIMENTO) and    '#13#10'                     (REC.vlr_saldo>0) then '#39'2' +
      '-Vence hoje'#39'   '#13#10'                 when  (REC.DTA_VENCIMENTO > CU' +
      'RRENT_DATE ) and  '#13#10'                      (REC.vlr_saldo>0) then' +
      ' '#39'3-A Vencer'#39'      '#13#10'                 when  (REC.vlr_saldo = 0) ' +
      'then '#39'4-Quitado'#39'   '#13#10'                ELSE '#39'5-???'#39'               ' +
      '                    '#13#10'               end as varchar(30)) as int_' +
      'situacao_titulo        '#13#10'  '#13#10'FROM '#13#10'  FIN_TB_M_REC REC'#13#10'LEFT OUT' +
      'ER JOIN CAD_TB_C_CLI CLI ON CLI.ID_CLIENTE = REC.ID_CLIENTE'#13#10'LEF' +
      'T OUTER JOIN CAD_TB_C_FPG FPG ON FPG.ID_FORMA_PAG = REC.ID_FORMA' +
      '_PAG'#13#10'LEFT OUTER JOIN CAD_TB_C_LTO LTO ON LTO.ID_LOCAL_TITULO = ' +
      'REC.ID_LOCAL_TITULO'#13#10'LEFT OUTER JOIN CAD_TB_C_PCT PCT ON PCT.ID_' +
      'PLANO = REC.ID_PLANO'#13#10'LEFT OUTER JOIN CAD_TB_C_PAR PAR ON PAR.ID' +
      '_EMPRESA = REC.ID_EMPRESA'#13#10' LEFT OUTER JOIN cad_tb_c_par_ctr CTR' +
      ' ON CTR.ID_EMPRESA=REC.ID_EMPRESA '#13#10'LEFT OUTER JOIN FAT_TB_M_NFE' +
      ' NFE ON NFE.ID_FISCAL = REC.ID_FISCAL'#13#10'LEFT OUTER JOIN CAD_TB_C_' +
      'CTC CTC ON CTC.ID_CONTA = REC.BOL_ID_CONTA'#13#10'left outer join  '#13#10' ' +
      '            (select ppp.id_titulo, max(dta_pagamento) as dta_pag' +
      'amento '#13#10'                 from fin_tb_m_rec_pag ppp '#13#10'          ' +
      '    group by 1) pag on (pag.id_titulo = rec.id_titulo)'#13#10'where 1 ' +
      '= 2'
    MaxBlobSize = -1
