      'IN CAD_TB_C_PAR PAR ON PAR.ID_EMPRESA = PAG.ID_EMPRESA'#13#10'LEFT OUT' +
      'ER JOIN CAD_TB_C_CCU CCU ON CCU.ID_CCUSTO = PAG.ID_CCUSTO'#13#10'where' +
