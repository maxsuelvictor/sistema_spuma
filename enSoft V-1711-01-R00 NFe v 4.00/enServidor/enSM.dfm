  object OFI_SQ_R_OFV_POC_002: TSQLDataSet
    CommandText = 
      'SELECT '#13#10'             '#13#10'             OCV.ID_ORCAMENTO, OCV.DTA_O' +
      'RCAMENTO,OCV.DTA_VALIDADE,'#13#10'             OCV.ID_CLIENTE,'#13#10'      ' +
