      '    '#13#10'    left outer join fat_tb_m_nfe_ite nfi on nfi.id_fiscal=' +
      'nfe.id_fiscal and nfi.id_item=ite.id_item        '#13#10#13#10'WHERE 1=2'#13#10 +
