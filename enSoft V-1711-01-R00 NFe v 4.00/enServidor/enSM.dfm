  object FIN_SQ_R_EXT_FOR_NCR: TSQLDataSet
    CommandText = 
      'SELECT PCR.ID_EMPRESA,PCR.ID_CREDITO,PCR.DTA_CREDITO,'#13#10'      PCR' +
      '.ID_FORNECEDOR,"FOR".DESCRICAO AS INT_NOMEFOR,PCR.HISTORICO,'#13#10#13#10 +
      '      CAST (CASE PCR.ORIGEM'#13#10'        WHEN 0 THEN '#39'Manual'#39#13#10'     ' +
      '   WHEN 1 THEN '#39'Baixa de Conta a Receber'#39#13#10'        WHEN 2 THEN '#39 +
      'Devolu'#231#227'o de Vendas'#39#13#10'        WHEN 3 THEN '#39'Nota Fiscal de Venda'#39 +
      #13#10'      END AS VARCHAR(30)) AS INT_DESCORIGEM,'#13#10#13#10'     (COALESCE' +
      '((SELECT SUM(COALESCE(PCC.VLR_CREDITO,0)) FROM FIN_TB_M_PCR PCC ' +
      #13#10'           WHERE PCC.ID_FORNECEDOR=PCR.ID_FORNECEDOR AND PCC.D' +
      'EB_CRE = 1 AND PCC.ID_CREDITO < PCR.ID_CREDITO ),0) - '#13#10'        ' +
      '  COALESCE((SELECT SUM(COALESCE(PCC.VLR_CREDITO,0)) FROM FIN_TB_' +
      'M_PCR PCC '#13#10'           WHERE PCC.ID_FORNECEDOR=PCR.ID_FORNECEDOR' +
      ' AND PCC.DEB_CRE = 0 AND PCC.ID_CREDITO < PCR.ID_CREDITO ),0)) '#13 +
      #10'          AS SALDO_ANTERIOR, '#13#10'               '#13#10#13#10'     CAST (CA' +
      'SE PCR.DEB_CRE'#13#10'        WHEN 0 THEN 0'#13#10'        WHEN 1 THEN PCR.V' +
      'LR_CREDITO'#13#10'      END AS NUMERIC(18,4)) AS ENTRADA,'#13#10'     CAST (' +
      'CASE PCR.DEB_CRE'#13#10'        WHEN 0 THEN PCR.VLR_CREDITO'#13#10'        W' +
      'HEN 1 THEN 0'#13#10'      END AS NUMERIC(18,4)) AS SAIDA'#13#10#13#10'FROM FIN_T' +
      'B_M_PCR PCR '#13#10'LEFT OUTER JOIN CAD_TB_C_FOR "FOR" ON "FOR".ID_FOR' +
      'NECEDOR=PCR.ID_FORNECEDOR'#13#10'WHERE 1=2'
