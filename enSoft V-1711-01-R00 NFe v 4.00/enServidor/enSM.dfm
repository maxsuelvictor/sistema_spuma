      'ST( '#39'V'#39' AS VARCHAR(1)) AS TIPO, FN.DOC_CNPJ AS COD_FABRICANTE,'#13#10 +
      '   FN.DESCRICAO AS DES_FABRICANTE,'#13#10'   PRO.ID_ITEM AS COD_PRODUT' +
      'O,'#13#10'   NF.ID_EMPRESA AS ID_EMPRESA,'#13#10'   PRO.DESCRICAO AS DES_PRO' +
      'DUTO, '#13#10'  CAST(COALESCE(PRO.ID_GRUPO,'#39'0.00.0.001'#39') AS VARCHAR(11' +
      ')) AS COD_GRUPO,'#13#10'  CAST(COALESCE(GP.DESCRICAO,'#39'OUTROS'#39') AS VARC' +
      'HAR(30)) AS DES_GRUPO,'#13#10'  CAST( '#39#39' AS VARCHAR(20)) AS COD_FAMILI' +
      'A,'#13#10'  CAST( '#39#39' AS VARCHAR(20)) AS DES_FAMILIA, '#13#10'  CAST( '#39#39' AS V' +
      'ARCHAR(20)) AS COD_SUB_FAMILIA,'#13#10'  CAST( '#39#39' AS VARCHAR(20)) AS D' +
      'ES_SUB_FAMILIA,'#13#10'  CAST('#39'E'#39' AS VARCHAR(1)) AS TIPO_COD_BARRA,'#13#10' ' +
      '  PRO.COD_BARRA AS COD_BARRA,'#13#10'   CAST(COALESCE(CU.DESCRICAO,'#39'ET' +
      'C'#39') AS VARCHAR(3)) AS EMBALAGEM,'#13#10'   CAST( '#39'UN'#39'  AS VARCHAR(3)) ' +
      'AS UND,'#13#10'   COALESCE(PRO.EMB_VENDA,1)*1000 as volume,'#13#10'  '#13#10'  CAS' +
      'T(EXTRACT('#39'YEAR'#39' FROM NF.DTA_EMISSAO) || '#13#10'  LPAD(CAST(EXTRACT('#39 +
      'MONTH'#39' FROM NF.DTA_EMISSAO) AS VARCHAR),2,'#39'0'#39') ||'#13#10'  LPAD(CAST(E' +
      'XTRACT('#39'DAY'#39' FROM NF.DTA_EMISSAO) AS VARCHAR),2,'#39'0'#39') AS VARCHAR(' +
      '10)) AS DTA_CADASTRO,'#13#10#13#10'  CAST(CASE PRO.ATIVO'#13#10'     WHEN '#39'T'#39' TH' +
      'EN '#39'A'#39#13#10'     WHEN '#39'F'#39' THEN '#39'I'#39#13#10'  END  AS VARCHAR(1)) AS ATIVO'#13#10 +
      'FROM  itens_nota inf'#13#10'LEFT OUTER JOIN CAD_TB_C_ITE PRO ON (PRO.I' +
      'D_ITEM = INF.ID_ITEM)'#13#10'LEFT OUTER JOIN CAD_TB_C_FOR FN ON FN.ID_' +
      'FORNECEDOR=PRO.ID_FORNECEDOR'#13#10'LEFT OUTER JOIN CAD_TB_C_GRU GP ON' +
      ' GP.ID_GRUPO=PRO.ID_GRUPO'#13#10'LEFT OUTER JOIN FAT_TB_M_NFE_ITE NFI ' +
      'ON NFI.ID_ITEM=PRO.ID_ITEM'#13#10'LEFT OUTER JOIN FAT_TB_M_NFE NF ON N' +
