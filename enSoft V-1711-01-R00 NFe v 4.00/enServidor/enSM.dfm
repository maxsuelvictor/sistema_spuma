  object ACE_SQ_M_VND: TSQLDataSet
    CommandText = 
      'SELECT CAST('#39'V'#39' AS VARCHAR(1)) AS TIPO, FIL.EMP_CNPJ, FOC.DOC_CN' +
      'PJ AS COD_FAB, FOC.DESCRICAO,'#13#10'        --PRO.ID_ITEM AS COD_BARR' +
      'A,'#13#10#13#10'       CAST( CASE '#13#10'             WHEN PRO.COD_BARRA<>'#39#39' TH' +
      'EN PRO.COD_BARRA '#13#10'             ELSE   CAST(PRO.ID_ITEM AS VARCH' +
      'AR(50))  END   AS  VARCHAR(50)) AS COD_BARRA, '#13#10#13#10#13#10#13#10#13#10'       C' +
      'AST( '#39#39' AS VARCHAR(20)) AS NUM_LOTE, CAST('#39#39' AS VARCHAR(20)) AS ' +
      'VAL_LOTE, CAST('#39#39' AS VARCHAR(20)) AS LIVRE1,'#13#10'       CAST('#39#39' AS ' +
      'VARCHAR(20)) AS LIVRE2, (NFI.QTDE)*1000 AS QTDE,'#13#10'        ROUND(' +
      '(NFI.VLR_LIQUIDO),2)*100 AS VLR_LIQ_TOTAL,CAST( '#39'BRL'#39' AS VARCHAR' +
      '(10)) AS MOEDA,'#13#10'        NFE.NUMERO AS ID_TRANSACAO, NFE.ID_EMPR' +
      'ESA,'#13#10#13#10'        CAST(EXTRACT('#39'YEAR'#39' FROM NFE.DTA_EMISSAO) || '#13#10' ' +
      '       LPAD(CAST(EXTRACT('#39'MONTH'#39' FROM NFE.DTA_EMISSAO) AS VARCHA' +
      'R),2,'#39'0'#39') ||'#13#10'        LPAD(CAST(EXTRACT('#39'DAY'#39' FROM NFE.DTA_EMISS' +
      'AO) AS VARCHAR),2,'#39'0'#39') AS VARCHAR(10)) AS DTA_ESTOQUE,'#13#10#13#10'      ' +
      ' CAST(CASE'#13#10'            WHEN NFE.NFE_COD_SIT='#39'02'#39' THEN '#39'C'#39#13#10'    ' +
      '        WHEN NFE.NFE_COD_SIT='#39'00'#39' AND NFE.FATURADA='#39'F'#39' THEN '#39'P'#39#13 +
      #10'            WHEN NFE.NFE_COD_SIT='#39'00'#39' AND NFE.FATURADA='#39'T'#39' AND ' +
      'NFE.TIPO_NF='#39'S'#39' THEN '#39'V'#39#13#10'            WHEN NFE.NFE_COD_SIT='#39'00'#39' ' +
      'AND NFE.FATURADA='#39'T'#39' AND NFE.TIPO_NF='#39'R'#39' THEN '#39'DV'#39#13#10'            ' +
      'WHEN NFE.NFE_COD_SIT='#39'00'#39' AND NFE.FATURADA='#39'T'#39' AND NFE.TIPO_NF='#39 +
      'S'#39' THEN '#39'DV'#39#13#10'            WHEN NFE.NFE_COD_SIT='#39'00'#39' AND NFE.FATU' +
      'RADA='#39'T'#39' AND NFE.TIPO_NF='#39'E'#39' THEN '#39'DV'#39#13#10'            WHEN NFE.NFE' +
      '_COD_SIT='#39'00'#39' AND NFE.FATURADA='#39'T'#39' AND NFE.TIPO_NF='#39'D'#39' THEN '#39'DV'#39 +
      #13#10'        END AS  VARCHAR(3)) AS TIPO_TRANSACAO,'#13#10#13#10'        NFI.' +
      'ID_CFO,'#13#10'      '#13#10'       CAST(CASE CLI.PESSOA'#13#10'           WHEN '#39'0' +
      #39' THEN '#39'1'#39#13#10'           WHEN '#39'1'#39' THEN '#39'2'#39#13#10'        END AS VARCHAR' +
      '(1)) AS TIPO_PDV,'#13#10#13#10'        CLI.DOC_CNPJ_CPF AS ID_PDV,'#13#10'      ' +
      '  CLI.NOME AS DES_PDV,'#13#10'        CAST(COALESCE(CLI.CEP,'#39'56300000'#39 +
      ') AS VARCHAR(8)) AS CEP_PDV,'#13#10'        CAST('#39#39' AS VARCHAR(20)) AS' +
      ' CLA_PDV,'#13#10#13#10'        CAST(CASE COALESCE(NFE.ID_VENDEDOR,0)'#13#10'    ' +
      '        WHEN 0 THEN '#39'00-OUTRO'#39#13#10'            ELSE  FUN.ID_FUNCION' +
      'ARIO ||'#39'-'#39' || FUN.NOME '#13#10'        END AS VARCHAR(50)) AS NOME_VND' +
      #13#10'FROM FAT_TB_M_NFE_ITE NFI'#13#10'LEFT OUTER JOIN FAT_TB_M_NFE NFE ON' +
      ' NFE.ID_FISCAL=NFI.ID_FISCAL'#13#10'LEFT OUTER JOIN CAD_TB_C_PAR FIL O' +
      'N FIL.ID_EMPRESA=NFE.ID_EMPRESA'#13#10'LEFT OUTER JOIN CAD_TB_C_ITE PR' +
      'O ON PRO.ID_ITEM=NFI.ID_ITEM'#13#10'LEFT OUTER JOIN CAD_TB_C_CLI CLI O' +
      'N CLI.ID_CLIENTE=NFE.ID_EMITENTE'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN F' +
      'UN ON FUN.ID_FUNCIONARIO=NFE.ID_VENDEDOR'#13#10'LEFT OUTER JOIN CAD_TB' +
      '_C_FOR FOC ON FOC.ID_FORNECEDOR=PRO.ID_FORNECEDOR'#13#10'WHERE 1=2 '#13#10'A' +
      'ND NFE.TIPO_NF <> '#39'E'#39#13#10'AND NFE.TIPO_NF <> '#39'D'#39#13#10' '#13#10'ORDER BY FIL.E' +
      'MP_CNPJ, FOC.DOC_CNPJ, PRO.ID_ITEM'
