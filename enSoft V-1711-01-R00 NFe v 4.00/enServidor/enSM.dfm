      'ENDERECOCLI, CLI.BAIRRO AS INT_BAIRROCLI,  '#13#10'       CLI.CEP AS I' +
      'NT_CEPCLI, CID.NOME AS INT_NOMECID, CID.UF AS INT_UFCID,'#13#10'      ' +
      ' CLI.NUMERO AS INT_NUMEROCLI,'#13#10'       case'#13#10'         when VEI.CO' +
      'MBUSTIVEL = 0 then '#39'Gasolina'#39#13#10'         when VEI.COMBUSTIVEL = 1' +
      ' then '#39'Alcool'#39#13#10'         when VEI.COMBUSTIVEL = 2 then '#39'Diesel'#39#13 +
      #10'         when VEI.COMBUSTIVEL = 3 then '#39'Gas Natural'#39#13#10'         ' +
      'when VEI.COMBUSTIVEL = 4 then '#39'Eletrico'#39#13#10'         when VEI.COMB' +
      'USTIVEL = 5 then '#39'Flex'#39#13#10'       end as INT_NOMECOM,'#13#10'       VEI.' +
      'ID_PLACA AS INT_PLACA,   VEI.DESCRICAO AS INT_NOMEVEI,'#13#10'       V' +
      'EI.CHASSI AS INT_CHASSIVEI, VEI.ANO_MODELO AS INT_ANOMODELO,'#13#10'  ' +
      '     VEI.ANO_FABRICACAO AS INT_ANOFABRIC, VEI.MOTOR AS INT_MOTOR' +
      'VEI,'#13#10'        MVE.DESCRICAO AS INT_MODELOMVE, COR.DESCRICAO AS I' +
      'NT_NOMECOR, '#13#10'       TIO.DESCRICAO AS INT_NOMETIO,'#13#10'       CPG.D' +
      'ESCRICAO AS int_nomecpg,'#13#10'       ANA.NOME AS INT_NOMEANA,'#13#10'     ' +
      '  eqp.descricao as int_nomeeqp, eqp.modelo as int_modeloeqp,'#13#10'  ' +
      '     eqp.serial as int_serialeqp, eqp.garantia as int_garantiaeq' +
      'p,'#13#10'       meq.descricao as int_nomemeq,meq.nro_serie as int_ser' +
      'iemeq,'#13#10'       mde.descricao as int_modelomeq,'#13#10'       meq.tipo ' +
      'as int_tipomeq,meq.id_placa as int_placa_meq,'#13#10'       meq.ano_mo' +
      'delo as int_anomodelo_meq,meq.ano_fabricacao as int_anofab_meq,'#13 +
      #10'       crm.descricao as int_nomecor_meq,meq.chassi as int_chass' +
