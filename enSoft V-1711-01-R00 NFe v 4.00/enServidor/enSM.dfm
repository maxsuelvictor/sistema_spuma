  object FIN_SQ_R_BCO_CCB: TSQLDataSet
    CommandText = 
      'SELECT CTC.ID_CONTA, TIF.ID_TIPO_FINANCEIRO, CTC.DESCRICAO AS IN' +
      'T_DESCRICAOCONTA,                                               ' +
      '  '#13#10'                         CAST (CASE TIF.NATUREZA'#13#10'          ' +
      '                  WHEN 0 THEN '#39'Sa'#237'das'#39#13#10'                        ' +
      '    WHEN 1 THEN '#39'Entradas'#39#13#10'                         END AS VARC' +
      'HAR(15) ) AS  INT_DESCNATUREZA, TIF.DESCRICAO AS INT_DESCTIF,   ' +
      '                                                                ' +
      '                                    '#13#10'                     COALE' +
      'SCE( (SELECT SUM(CTA.VLR_LANCAMENTO)                            ' +
      '                                     '#13#10'                         ' +
      'FROM FIN_TB_M_CTA CTA                                           ' +
      '                                  '#13#10'                            ' +
      'left outer join CAD_TB_C_TIF TIF2 on (TIF2.ID_TIPO_FINANCEIRO = ' +
      'CTA.ID_TIPO_FINANCEIRO)   '#13#10'                         WHERE CTA.I' +
      'D_CONTA = CTC.ID_CONTA                                          ' +
      '                          '#13#10'                         AND CTA.ID_' +
      'TIPO_FINANCEIRO = TIF.ID_TIPO_FINANCEIRO                        ' +
      '                                                                ' +
      '                              '#13#10'                         AND CTA' +
      '.DTA_LANCAMENTO >= '#39'01/01/2016'#39#13#10'                         AND CT' +
      'A.DTA_LANCAMENTO <= '#39'31/01/2016'#39'  '#13#10'                         AND' +
      ' TIF2.NATUREZA = '#39'1'#39'),0) AS VLR_CREDITO,                        ' +
      '                                  '#13#10'                            ' +
      '                                                                ' +
      '                                 '#13#10'                     COALESCE' +
      '( (SELECT SUM(CTA.VLR_LANCAMENTO)*(-1)                          ' +
      '                                  '#13#10'                         FRO' +
      'M FIN_TB_M_CTA CTA                                              ' +
      '                                '#13#10'                            le' +
      'ft outer join CAD_TB_C_TIF TIF2 on (TIF2.ID_TIPO_FINANCEIRO = CT' +
      'A.ID_TIPO_FINANCEIRO)   '#13#10'                         WHERE CTA.ID_' +
      'CONTA = CTC.ID_CONTA                                            ' +
      '                        '#13#10'                         AND CTA.ID_TI' +
      'PO_FINANCEIRO = TIF.ID_TIPO_FINANCEIRO                          ' +
      '                                                                ' +
      '                '#13#10'                         AND CTA.DTA_LANCAMENT' +
      'O >= '#39'01/01/2016'#39#13#10'                         AND CTA.DTA_LANCAMEN' +
      'TO <= '#39'31/01/2016'#39#13#10'                         AND TIF2.NATUREZA =' +
      ' '#39'0'#39'),0) AS VLR_DEBITO, '#13#10'                                      ' +
      '                                                                ' +
      '                       '#13#10'                     COALESCE( (SELECT ' +
      'SUM(CTA.VLR_LANCAMENTO)                                         ' +
      '                        '#13#10'                         FROM FIN_TB_M' +
      '_CTA CTA                                                        ' +
      '                      '#13#10'                           left outer jo' +
      'in CAD_TB_C_TIF TIF on (TIF.ID_TIPO_FINANCEIRO = CTA.ID_TIPO_FIN' +
      'ANCEIRO)      '#13#10'                         WHERE CTA.ID_CONTA = CT' +
      'C.ID_CONTA                                                      ' +
      '              '#13#10'                           AND CTA.CONCILIADO = ' +
      'true                                                            ' +
      '          '#13#10'                           AND CTA.DTA_LANCAMENTO < ' +
      ' '#39'01/01/2016'#39#13#10'                           AND TIF.NATUREZA = '#39'1'#39 +
      '),0) -                                                          ' +
      '                '#13#10'                                              ' +
      '                                                                ' +
      '               '#13#10'                     COALESCE( (SELECT SUM(CTA.' +
      'VLR_LANCAMENTO)                                                 ' +
      '                '#13#10'                         FROM FIN_TB_M_CTA CTA' +
      '                                                                ' +
      '              '#13#10'                           left outer join CAD_T' +
      'B_C_TIF TIF on (TIF.ID_TIPO_FINANCEIRO = CTA.ID_TIPO_FINANCEIRO)' +
      '      '#13#10'                         WHERE CTA.ID_CONTA = CTC.ID_CON' +
      'TA                                                              ' +
      '                                                                ' +
      '         '#13#10'                         AND CTA.DTA_LANCAMENTO <  '#39'0' +
      '1/01/2016'#39#13#10'                         AND TIF.NATUREZA = '#39'0'#39'),0) ' +
      'AS SALDO_ANTERIOR,                                              ' +
      '           '#13#10'                                                   ' +
      '                                                                ' +
      '          '#13#10'                     COALESCE( (SELECT SUM(CTA.VLR_L' +
      'ANCAMENTO)                                                      ' +
      '           '#13#10'                         FROM FIN_TB_M_CTA CTA     ' +
      '                                                                ' +
      '         '#13#10'                           left outer join CAD_TB_C_T' +
      'IF TIF on (TIF.ID_TIPO_FINANCEIRO = CTA.ID_TIPO_FINANCEIRO)     ' +
      ' '#13#10'                         WHERE CTA.ID_CONTA = CTC.ID_CONTA   ' +
      '                                                                ' +
      '                                                                ' +
      '      '#13#10'                         AND CTA.DTA_LANCAMENTO <=  '#39'31/' +
      '01/2016'#39#13#10'                         AND TIF.NATUREZA = '#39'1'#39'),0) - ' +
      '                                                                ' +
      '         '#13#10'                                                     ' +
      '                                                                ' +
      '        '#13#10'                     COALESCE( (SELECT SUM(CTA.VLR_LAN' +
      'CAMENTO)                                                        ' +
      '         '#13#10'                         FROM FIN_TB_M_CTA CTA       ' +
      '                                                                ' +
      '       '#13#10'                           left outer join CAD_TB_C_TIF' +
      ' TIF on (TIF.ID_TIPO_FINANCEIRO = CTA.ID_TIPO_FINANCEIRO)      '#13 +
      #10'                         WHERE CTA.ID_CONTA = CTC.ID_CONTA     ' +
      '                                                                ' +
      '                                                                ' +
      '     '#13#10'                         AND CTA.DTA_LANCAMENTO <=  '#39'31/0' +
      '1/2016'#39#13#10'                         AND TIF.NATUREZA = '#39'0'#39'),0) AS ' +
      'SALDO_ATUAL                                                     ' +
      '        '#13#10'        '#13#10'                     FROM FIN_TB_M_CTA CTA  ' +
      '                                                                ' +
      '                    '#13#10'                     LEFT OUTER JOIN CAD_T' +
      'B_C_CTC CTC ON CTC.ID_CONTA = CTA.ID_CONTA                      ' +
      '                           '#13#10'                     LEFT OUTER JOI' +
      'N CAD_TB_C_TIF TIF ON TIF.ID_TIPO_FINANCEIRO = CTA.ID_TIPO_FINAN' +
      'CEIRO                  '#13#10'                     LEFT OUTER JOIN CA' +
      'D_TB_C_PAR PAR ON PAR.ID_EMPRESA = CTA.ID_EMPRESA '#13#10'            ' +
      '         LEFT OUTER JOIN CAD_TB_C_PCT PCT ON PCT.ID_PLANO = CTA.' +
      'ID_PLANO                                             '#13#10'         ' +
      '                WHERE 1=2                                       ' +
      '           '#13#10'                           '#13#10'                      ' +
      '       '#13#10'                     Group by CTC.ID_CONTA, TIF.ID_TIPO' +
      '_FINANCEIRO, CTC.DESCRICAO, TIF.DESCRICAO                   '#13#10'  ' +
      '                   ORDER BY CTC.ID_CONTA, TIF.ID_TIPO_FINANCEIRO'
    MaxBlobSize = -1
    Params = <>
    SQLConnection = Conexao
    Left = 3120
    Top = 1596
  end
