      '.doc_im as IM,'#13#10'   '#39' '#39' as SUFRAMA,'#13#10'   cli.endereco, cli.numero,' +
      ' cli.end_complemento, cli.bairro    '#13#10'from cad_tb_c_cli cli'#13#10'lef' +
      't outer join cad_tb_c_cid cid on (cid.ID_CIDADE = cli.ID_CIDADE)' +
      #13#10'where 1 = 2 and  exists (select nfe.id_fiscal from fat_tb_m_nf' +
      'e nfe '#13#10'              where'#13#10'                     nfe.dta_emissa' +
      'o >= '#39'01/01/2000'#39' and nfe.dta_emissao <= '#39'01/01/2016'#39'  '#13#10'       ' +
      '         and (nfe.tipo_nf = '#39'S'#39' or nfe.tipo_nf = '#39'R'#39')'#13#10'         ' +
      '       and nfe.id_emitente = cli.id_cliente '#13#10'                an' +
      'd nfe.numero > 0    '#13#10'                and nfe.status = 1'#13#10'      ' +
