      'do_fisico then saldo_fisico'#13#10'    else'#13#10'       cubagem_da_op end ' +
      'as qtde_a_abater_no_sld'#13#10'  '#13#10'    /*cast(case '#13#10#9#9#9' when cubagem_' +
      'da_op > cubagem_esp_1 then '#13#10'            case '#13#10'                ' +
      'when (cubagem_da_op - cubagem_esp_1) < cubagem_esp_1 then cubage' +
      'm_esp_1 - (cubagem_da_op - cubagem_esp_1)'#13#10'                when ' +
      '(cubagem_da_op - cubagem_esp_1) = cubagem_esp_1 then 0'#13#10'        ' +
      '        when (cubagem_da_op - cubagem_esp_1) > cubagem_esp_1 the' +
      'n '#13#10'                         (cubagem_da_op - cubagem_esp_1) / c' +
      'ubagem_esp_1'#13#10'             end'#13#10'    else 0'#13#10'    end as numeric(1' +
      '8,4)) as cubagem_dif_a_produzir */'#13#10'  '#13#10'    '#13#10#13#10' from'#13#10'('#13#10'with t' +
