  object FIN_SQ_R_PAG_ABE_PPG: TSQLDataSet
    CommandText = 
      'Select '#39'A PARTIR DE 181 DIAS'#39' Prazo,      '#13#10'  Pag.ID_FORMA_PAG, ' +
      'Pag.NUM_PARCELA, Pag.DTA_EMISSAO,        Pag.DTA_VENCIMENTO, Pag' +
      '.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,        (current_da' +
      'te-Pag.DTA_VENCIMENTO) INT_ATRASO,  '#13#10'      FPG.DESCRICAO AS INT' +
      '_NOMEFPG,        Pag.VLR_SALDO, pag.vlr_parcela  from FIN_TB_M_P' +
      'AG Pag  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORNECEDOR=Pa' +
      'g.ID_FORNECEDOR'#13#10'  left outer join cad_tb_c_fpg FPG ON FPG.ID_FO' +
      'RMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0) and Pag.DTA_' +
      'VENCIMENTO<current_date and       (current_date-Pag.DTA_VENCIMEN' +
      'TO)>=181  AND'#13#10' PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND        PAG.D' +
      'TA_EMISSAO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')  '#13#10' Un' +
      'ion All  '#13#10'Select '#39'DE 151 A 180 DIAS'#39' Prazo,    '#13#10'    Pag.ID_FOR' +
      'MA_PAG, Pag.NUM_PARCELA, Pag.DTA_EMISSAO,  '#13#10'      Pag.DTA_VENCI' +
      'MENTO, Pag.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,        (' +
      'current_date-Pag.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICA' +
      'O AS INT_NOMEFPG,    '#13#10'    Pag.VLR_SALDO, pag.vlr_parcela  from ' +
      'FIN_TB_M_PAG Pag  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FOR' +
      'NECEDOR=Pag.ID_FORNECEDOR  left outer join cad_tb_c_fpg '#13#10'FPG ON' +
      ' FPG.ID_FORMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0) an' +
      'd       Pag.DTA_VENCIMENTO<current_date and       (current_date-' +
      'Pag.DTA_VENCIMENTO)>=151 and '#13#10'      (current_date-Pag.DTA_VENCI' +
      'MENTO)<=180  AND PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND        PAG.' +
      'DTA_EMISSAO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')'#13#10'   U' +
      'nion All '#13#10' Select '#39'DE 121 A 150 DIAS'#39' Prazo,        Pag.ID_FORM' +
      'A_PAG, Pag.NUM_PARCELA, Pag.DTA_EMISSAO,        Pag.DTA_VENCIMEN' +
      'TO, Pag.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,   '#13#10'     (c' +
      'urrent_date-Pag.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICAO' +
      ' AS INT_NOMEFPG,        Pag.VLR_SALDO, pag.vlr_parcela  from FIN' +
      '_TB_M_PAG Pag '#13#10' Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORN' +
      'ECEDOR=Pag.ID_FORNECEDOR  left outer join cad_tb_c_fpg FPG ON FP' +
      'G.ID_FORMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0)  and ' +
      #13#10'      Pag.DTA_VENCIMENTO<current_date and       (current_date-' +
      'Pag.DTA_VENCIMENTO)>=121 and       (current_date-Pag.DTA_VENCIME' +
      'NTO)<=150  AND PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND   '#13#10'     PAG.' +
      'DTA_EMISSAO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')   '#13#10'U' +
      'nion All  '#13#10'Select '#39'DE 91 A 120 DIAS'#39' Prazo,     '#13#10'   Pag.ID_FOR' +
      'MA_PAG, Pag.NUM_PARCELA, Pag.DTA_EMISSAO,    '#13#10'   Pag.DTA_VENCIM' +
      'ENTO, Pag.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,        (c' +
      'urrent_date-Pag.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICAO' +
      ' AS INT_NOMEFPG,     '#13#10'   Pag.VLR_SALDO, pag.vlr_parcela  from F' +
      'IN_TB_M_PAG Pag  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORN' +
      'ECEDOR=Pag.ID_FORNECEDOR  '#13#10'left outer join cad_tb_c_fpg FPG ON ' +
      'FPG.ID_FORMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0)  an' +
      'd       Pag.DTA_VENCIMENTO<current_date and     '#13#10'  (current_dat' +
      'e-Pag.DTA_VENCIMENTO)>=91 and       (current_date-Pag.DTA_VENCIM' +
      'ENTO)<=120  AND PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND        PAG.D' +
      'TA_EMISSAO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39') '#13#10'  Un' +
      'ion All  '#13#10'Select '#39'DE 61 A 90 DIAS'#39' Prazo,  '#13#10'      Pag.ID_FORMA' +
      '_PAG, Pag.NUM_PARCELA, Pag.DTA_EMISSAO,        Pag.DTA_VENCIMENT' +
      'O, Pag.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,        (curr' +
      'ent_date-Pag.DTA_VENCIMENTO) INT_ATRASO,       '#13#10' FPG.DESCRICAO ' +
      'AS INT_NOMEFPG,        Pag.VLR_SALDO, pag.vlr_parcela  from FIN_' +
      'TB_M_PAG Pag  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORNECE' +
      'DOR=Pag.ID_FORNECEDOR '#13#10' left outer join cad_tb_c_fpg FPG ON FPG' +
      '.ID_FORMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0)  and  ' +
      '     Pag.DTA_VENCIMENTO<current_date and       (current_date-Pag' +
      '.DTA_VENCIMENTO)>=61 and   '#13#10'   (current_date-Pag.DTA_VENCIMENTO' +
      ')<=90  AND PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND        PAG.DTA_EM' +
      'ISSAO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')  '#13#10' Union A' +
      'll  '#13#10'Select '#39'DE 30 A 60 DIAS'#39' Prazo,      '#13#10'  Pag.ID_FORMA_PAG,' +
      ' Pag.NUM_PARCELA, Pag.DTA_EMISSAO,        Pag.DTA_VENCIMENTO, Pa' +
      'g.ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,     '#13#10'   (current' +
      '_date-Pag.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICAO AS IN' +
      'T_NOMEFPG,        Pag.VLR_SALDO, pag.vlr_parcela  from FIN_TB_M_' +
      'PAG Pag'#13#10'  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORNECEDOR' +
      '=Pag.ID_FORNECEDOR  left outer join cad_tb_c_fpg FPG ON FPG.ID_F' +
      'ORMA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0)  and       ' +
      #13#10'Pag.DTA_VENCIMENTO<current_date and       (current_date-Pag.DT' +
      'A_VENCIMENTO)>=30 and       (current_date-Pag.DTA_VENCIMENTO)<=6' +
      '0  AND PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND     '#13#10'  PAG.DTA_EMISS' +
      'AO <= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')   '#13#10'Union All ' +
      ' '#13#10'Select '#39'MENOS DE 30 DIAS'#39' Prazo,        Pag.ID_FORMA_PAG, Pag' +
      '.NUM_PARCELA, Pag.DTA_EMISSAO,     '#13#10'   Pag.DTA_VENCIMENTO, Pag.' +
      'ID_FORNECEDOR, FOC.DESCRICAO AS INT_NOMEFOR,        (current_dat' +
      'e-Pag.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICAO AS INT_NO' +
      'MEFPG,    '#13#10'    Pag.VLR_SALDO, pag.vlr_parcela  from FIN_TB_M_PA' +
      'G Pag  Left Outer Join CAD_TB_C_FOR FOC On FOC.ID_FORNECEDOR=Pag' +
      '.ID_FORNECEDOR'#13#10'  left outer join cad_tb_c_fpg FPG ON FPG.ID_FOR' +
      'MA_PAG = PAG.ID_FORMA_PAG  Where (Pag.VLR_SALDO>0) and       Pag' +
      '.DTA_VENCIMENTO<current_date and       '#13#10'(current_date-Pag.DTA_V' +
      'ENCIMENTO)>=0 and       (current_date-Pag.DTA_VENCIMENTO)<30  AN' +
      'D PAG.DTA_EMISSAO >= '#39'27/11/2011'#39' AND    '#13#10'    PAG.DTA_EMISSAO <' +
      '= '#39'27/11/2014'#39'  AND PAG.ID_EMPRESA in ('#39'001'#39')   order by prazo, ' +
      'dta_vencimento'
