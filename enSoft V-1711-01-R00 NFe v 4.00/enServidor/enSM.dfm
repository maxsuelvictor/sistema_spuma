      'e fun.vnd_interno_externo '#13#10#9'when 0 then '#39'Interno'#39#13#10#9'when 1 then' +
      ' '#39'Externo'#39#13#10'end as tipo_vendedor,'#13#10#13#10'case fun.vnd_interno_extern' +
