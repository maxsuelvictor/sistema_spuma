      'M PAG.DTA_EMISSAO)='#39'4'#39#13#10'THEN PAG.VLR_SALDO'#13#10'ELSE 0 END) AS MES_4' +
      ','#13#10#13#10#13#10'SUM('#13#10'CASE WHEN EXTRACT(MONTH FROM PAG.DTA_EMISSAO)='#39'5'#39#13#10 +
      'THEN PAG.VLR_SALDO'#13#10'ELSE 0 END) AS MES_5,'#13#10#13#10#13#10'SUM('#13#10'CASE WHEN E' +
      'XTRACT(MONTH FROM PAG.DTA_EMISSAO)='#39'6'#39#13#10'THEN PAG.VLR_SALDO'#13#10'ELSE' +
      ' 0 END) AS MES_6,'#13#10#13#10#13#10'SUM('#13#10'CASE WHEN EXTRACT(MONTH FROM PAG.DT' +
      'A_EMISSAO)='#39'7'#39#13#10'THEN PAG.VLR_SALDO'#13#10'ELSE 0 END) AS MES_7,'#13#10#13#10'SUM' +
      '('#13#10'CASE WHEN EXTRACT(MONTH FROM PAG.DTA_EMISSAO)='#39'8'#39#13#10'THEN PAG.V' +
      'LR_SALDO'#13#10'ELSE 0 END) AS MES_8,'#13#10#13#10'SUM('#13#10'CASE WHEN EXTRACT(MONTH' +
      ' FROM PAG.DTA_EMISSAO)='#39'9'#39#13#10'THEN PAG.VLR_SALDO'#13#10'ELSE 0 END) AS M' +
