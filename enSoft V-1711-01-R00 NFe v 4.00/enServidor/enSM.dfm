      ' when 0 then '#39'Em aberto'#39#13#10'           when 1 then '#39'Reprovado'#39#13#10'  ' +
      '         when 2 then '#39'Em produ'#231#227'o'#39#13#10'           when 3 then '#39'Fatu' +
      'rado'#39#13#10'           when 4 then '#39'Cancelado'#39#13#10'         end as varch' +
      'ar(20) ) as int_sitped,'#13#10'         cast( case     '#13#10'           wh' +
