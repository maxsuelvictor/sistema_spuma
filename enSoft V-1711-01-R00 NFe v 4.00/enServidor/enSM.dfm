      'select pit.*,'#13#10' '#13#10' cast( case situacao_aprovacao '#13#10'           wh' +
      'en 0 then '#39'Em espera'#39#13#10'           when 1 then '#39'Aprovado'#39#13#10'      ' +
      '     when 2 then '#39'Reprovado'#39#13#10'         end as varchar(20) ) as i' +
