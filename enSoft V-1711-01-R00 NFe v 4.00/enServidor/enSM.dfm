      'ED.ID_RESP_CONF'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN FNL ON FNL.ID_FUNC' +
      'IONARIO=PED.ID_USUARIO_LIB'#13#10'LEFT OUTER JOIN CAD_TB_C_CID CID ON ' +
      'CID.ID_CIDADE=CLI.ID_CIDADE'#13#10'LEFT OUTER JOIN PCP_TB_M_ORS ORS ON' +
      ' ORS.ID_PEDIDO=PED.ID_PEDIDO'#13#10'LEFT OUTER JOIN PCP_TB_C_REG REG O' +
      'N REG.ID_REGIAO = CLI.ID_REGIAO'#13#10#13#10'LEFT OUTER JOIN CAD_TB_C_FUN ' +
      'FPI ON FPI.ID_FUNCIONARIO=PED.id_func_pri_impressao'#13#10'LEFT OUTER ' +
