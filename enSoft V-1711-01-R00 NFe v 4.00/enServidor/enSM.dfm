  object BUS_SQ_M_PED_ITE_GER: TSQLDataSet
    SchemaName = 'sa'
    CommandText = 
      'SELECT'#13#10' true as int_selecao,'#13#10' 0 as int_seqitem, PED_I.*, '#13#10'   ' +
      '    ITE.DESCRICAO AS INT_NOMEITE, COR.DESCRICAO AS INT_NOMECOR, ' +
      #13#10'       ITE.ID_UND_VENDA AS INT_ID_UND_VENDA ,'#13#10'       ITE.LOCA' +
      '_RUA AS  INT_RUA,'#13#10'       ITE.LOCA_PRATELEIRA AS INT_PRATELEIRA,' +
      #13#10'       ITE.LOCA_ESCANINHO AS INT_ESCANINHO, '#13#10'       LOC.LOCA_' +
      'RUA AS  INT_RUA2,'#13#10'       LOC.LOCA_PRATELEIRA AS INT_PRATELEIRA2' +
      ','#13#10'       LOC.LOCA_ESCANINHO AS INT_ESCANINHO2, '#13#10#13#10'GRU.TIPO_ITE' +
      'M  AS  INT_TIPO_ITEM,'#13#10'GRU.SER_TERCEIRO AS INT_SERTERCEIRO,'#13#10'GRU' +
      '.UTILIZA_LOTE AS INT_LOTE_GRU,'#13#10'       TAM.DESCRICAO AS INT_NOME' +
      'TAM,'#13#10'       CUL.DESCRICAO AS INT_NOMECUL,'#13#10'MEC.NOME AS INT_NOME' +
      'MEC '#13#10'FROM FAT_TB_M_PED_ITE PED_I '#13#10'LEFT OUTER JOIN FAT_TB_M_PED' +
      ' PED ON PED.ID_PEDIDO = PED_I.ID_PEDIDO'#13#10'LEFT OUTER JOIN CAD_TB_' +
      'C_ITE ITE ON ITE.ID_ITEM = PED_I.ID_ITEM'#13#10'LEFT OUTER JOIN CAD_TB' +
      '_C_ITE_LOC LOC ON LOC.ID_EMPRESA=PED.ID_EMPRESA AND  LOC.ID_ITEM' +
      ' = PED_I.ID_ITEM'#13#10'left outer join CAD_TB_C_GRU GRU on GRU.ID_GRU' +
      'PO = ITE.ID_GRUPO'#13#10'LEFT OUTER JOIN CAD_TB_C_COR COR ON COR.ID_CO' +
      'R = PED_I.ID_COR'#13#10'LEFT OUTER JOIN CAD_TB_C_TAM TAM ON TAM.ID_TAM' +
      'ANHO = PED_I.ID_TAMANHO'#13#10'LEFT OUTER JOIN CAD_TB_C_CUL CUL ON CUL' +
      '.ID_CULTURA = PED_I.ID_CULTURA'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN MEC' +
      ' ON MEC.ID_FUNCIONARIO=PED_I.ID_MECANICO'#13#10'WHERE PED_I.ID_PEDIDO=' +
      ':ID_PEDIDO'
    DataSource = BUS_DS_M_PED_GER
