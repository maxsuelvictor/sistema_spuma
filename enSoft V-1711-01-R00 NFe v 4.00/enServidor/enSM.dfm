      '     WHEN ITE.COD_BARRA<>'#39#39'  THEN 0'#13#10'      ELSE  1  END    AS IN' +
      'D_ITE_BAR,'#13#10'      FRN.DOC_CNPJ,PAR.EMP_CNPJ, FRN.DESCRICAO AS NO' +
