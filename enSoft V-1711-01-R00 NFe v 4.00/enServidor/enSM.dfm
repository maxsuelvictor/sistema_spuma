      'alesce(sum(mtm.valor_total),0) from cad_tb_c_mtv mtv1 '#13#10'       l' +
      'eft outer join cad_tb_c_mtv_mes mes1 on MES1.id_mtv=mtv1.id_mtv ' +
      '                    '#13#10'       where mtv1.ano=mtv.ano and mtv1.id_' +
      'grupo_prod=mtv.id_grupo_prod) as prev_valor_total,'#13#10#13#10#13#10'     (se' +
      'lect sum(qtde_mes_01) from cad_tb_c_mtv mtv1                    ' +
      '                   '#13#10'        left outer join cad_tb_c_mtv_mes me' +
      's1 on MES1.id_mtv=mtv1.id_mtv                                   ' +
      '           '#13#10'        where mtv1.ano=mtv.ano and mtv1.id_grupo_pr' +
      'od=mtv.id_grupo_prod) as prev_qtde_mes_01,'#13#10#13#10'     (select sum(q' +
      'tde_mes_01*valor_mes_01) from cad_tb_c_mtv mtv1   '#13#10'         lef' +
      't outer join cad_tb_c_mtv_mes mes1 on MES1.id_mtv=mtv1.id_mtv   ' +
      '                                          '#13#10'        where mtv1.a' +
