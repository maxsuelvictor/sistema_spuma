  object FIN_SQ_R_CSC_EMP: TSQLDataSet
    CommandText = 
      'SELECT PAR.ID_EMPRESA,PAR.EMP_FANTASIA,CLI.*, CID.UF INT_UF, CID' +
      '.NOME AS INT_NOMECID, '#13#10'                cast( CASE SITUACAO     ' +
      '                                                          '#13#10'    ' +
      '              WHEN 0 THEN '#39'Normal'#39'                              ' +
      '                         '#13#10'                  WHEN 1 THEN '#39'Especi' +
      'al'#39'                                                        '#13#10'   ' +
      '               WHEN 2 THEN '#39'Cr'#233'dito encerrado'#39'                  ' +
      '                             '#13#10'                END AS VARCHAR(30' +
      ') ) AS int_situacao,                                            ' +
      ' '#13#10'                cast( CASE CLI.SEXO                          ' +
      '                                         '#13#10'                  WHE' +
      'N 0 THEN '#39'Feminino'#39'                                             ' +
      '           '#13#10'                  WHEN 1 THEN '#39'Masculino'#39'          ' +
      '                                             '#13#10'                E' +
      'ND AS VARCHAR(10) ) AS int_sexo,                                ' +
      '                 '#13#10'                cast( CASE TIPO_CLIENTE      ' +
      '                                                     '#13#10'         ' +
      '         WHEN 0 THEN '#39'Consumidor'#39'                               ' +
      '                       '#13#10'                  WHEN 1 THEN '#39'Revenda'#39 +
      '                                                         '#13#10'     ' +
      '           END AS VARCHAR(10) ) AS int_tipocliente,             ' +
      '                             '#13#10'                  PEC.DESCRICAO A' +
      'S INT_NOMEPER,                                                  ' +
      ' '#13#10'               FUN.NOME AS INT_NOMEVND                       ' +
      '                                     '#13#10'               FROM CAD_T' +
      'B_C_CLI CLI                                                     ' +
      '         '#13#10'               LEFT OUTER JOIN CAD_TB_C_PAR PAR ON PA' +
      'R.ID_EMPRESA>0                               '#13#10'               LE' +
      'FT OUTER JOIN CAD_TB_C_FUN FUN ON FUN.ID_FUNCIONARIO=CLI.ID_VEND' +
      'EDOR             '#13#10'               LEFT OUTER JOIN CAD_TB_C_CID C' +
      'ID ON CID.ID_CIDADE=CLI.ID_CIDADE                    '#13#10'         ' +
      '      LEFT OUTER JOIN CAD_TB_C_PEC PEC ON PEC.ID_PERFIL_CLI = CL' +
      'I.ID_PERFIL_CLI          '#13#10#13#10'             WHERE NOT EXISTS (SELE' +
      'CT * FROM FAT_TB_M_NFE NFE                                     '#13 +
      #10'                    left outer join cad_tb_c_cfo cfo on cfo.id_' +
      'cfo=nfe.id_cfo                     '#13#10'                    where C' +
      'FO.ID_CLASSE IN ('#39'SP10'#39','#39'SP70'#39') and nfe.tipo_nf='#39'S'#39' and nfe.stat' +
      'us=1 and nfe.faturada=true and '#13#10'                        nfe.nfe' +
      '_cod_sit not in ('#39'02'#39','#39'04'#39','#39'05'#39') and                          '#13#10 +
      '                        nfe.DTA_EMISSAO<='#39' 01/01/1990'#39#13#10'        ' +
      '               AND nfe.DTA_EMISSAO>=('#39'01/01/1990'#39') AND  '#13#10'      ' +
      '                  nfe.id_empresa=PAR.id_empresa and nfe.id_emite' +
      'nte=cli.id_cliente) AND 1=2'
