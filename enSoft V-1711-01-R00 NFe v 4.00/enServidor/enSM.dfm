      'OIN CAD_TB_C_NAT NIJ ON NIJ.ID_CLASSE = CIJ.ID_CLASSE'#13#10'LEFT OUTE' +
      'R JOIN CAD_TB_C_NAT NEF ON NEF.ID_CLASSE = CEF.ID_CLASSE'#13#10'LEFT O' +
