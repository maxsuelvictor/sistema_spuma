      'ural'#39#13#10'         when VEI.COMBUSTIVEL = 4 then '#39'Eletrico'#39#13#10'      ' +
      '   when VEI.COMBUSTIVEL = 5 then '#39'Flex'#39#13#10'       end as INT_NOMEC' +
      'OM,'#13#10'       VEI.ID_PLACA AS INT_PLACA,   VEI.DESCRICAO AS INT_NO' +
