      'e on nfe.id_fiscal = nfi.id_fiscal'#13#10'          left outer join ca' +
      'd_tb_c_cfo cfo on cfo.id_cfo    = nfe.id_cfo  '#13#10'         where n' +
      'fi.id_item = ite.id_item and'#13#10'            cfo.id_classe in ('#39'SP1' +
