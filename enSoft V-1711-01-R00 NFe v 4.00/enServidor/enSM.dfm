  object OFI_SQ_R_OFV_COM_001: TSQLDataSet
    CommandText = 
      'SELECT '#13#10'             MEC.NOME   AS    INT_NOMEMEC,'#13#10'           ' +
      '  MEC.PER_COMISSAO AS INT_PER_COMISSAO, '#13#10'             ORV.ID_OR' +
      'DEM, ORV.DTA_EMISSAO,'#13#10'             ORV.ID_CLIENTE,'#13#10'           ' +
      '  CLI.NOME AS INT_NOMECLI,'#13#10'             ANA.NOME AS INT_NOMEANA' +
      ','#13#10'             CPG.DESCRICAO AS INT_NOMECPG,'#13#10'            VEI.I' +
      'D_VEICULO,'#13#10'            VEI.ID_PLACA AS INT_PLACA,   VEI.DESCRIC' +
      'AO AS INT_NOMEVEI,ORI.ID_MECANICO,'#13#10'            TIO.DESCRICAO AS' +
      ' INT_NOMETIO, '#13#10'           case orv.status'#13#10'             when  0' +
      ' then '#39'Aberta'#39#13#10'             when 1 then '#39'Em andamento'#39#13#10'       ' +
      '      when 2 then '#39'Fechada'#39#13#10'             when 3 then '#39'Faturada'#39 +
      #13#10'             when 4 then '#39'Cancelada'#39#13#10'           end as int_or' +
      'vsituacao,'#13#10'           orv.can_data, orv.can_motivo, '#13#10'         ' +
      '  orv.dta_fechamento,'#13#10#13#10'           coalesce( sum (case gru.tipo' +
      '_item '#13#10'               when '#39'09'#39' then ttt.vlr_desconto '#13#10'       ' +
      '      end),0) as  vlr_desc_servicos ,'#13#10#13#10'          coalesce( sum' +
      '(case                '#13#10'               when gru.tipo_item  <> '#39'09' +
      #39' then ttt.vlr_desconto '#13#10'             end),0) as   vlr_desc_pe'#231 +
      'as,'#13#10' '#13#10'           coalesce( sum (case gru.tipo_item '#13#10'         ' +
      '    when '#39'09'#39' then    '#13#10'                                        ' +
      '     (COALESCE(ttt.VLR_UNITARIO*(ttt.QTDE-ttt.QTDE_DEVOLVIDA),0)' +
      ') - '#13#10'                                             ((COALESCE(tt' +
      't.VLR_DESCONTO,0)/ttt.QTDE)*(ttt.qtde-ttt.qtde_devolvida)) '#13#10#13#10' ' +
      '           end),0) as   vlr_total_servicos,            '#13#10#13#10'     ' +
      '      coalesce( sum(case               '#13#10'                when gr' +
      'u.tipo_item  <> '#39'09'#39' then '#13#10'                                    ' +
      '         (COALESCE(ttt.VLR_UNITARIO*(ttt.QTDE-ttt.QTDE_DEVOLVIDA' +
      '),0)) - '#13#10'                                             ((COALESC' +
      'E(ttt.VLR_DESCONTO,0)/ttt.QTDE)*(ttt.qtde-ttt.qtde_devolvida)) '#13 +
      #10'           end),0) as   vlr_total_pe'#231'as    '#13#10#13#10'FROM OFI_TB_M_OR' +
      'V_ITE ORI'#13#10'LEFT OUTER JOIN CAD_TB_C_ITE    ITE  ON  ITE.ID_ITEM=' +
      'ORI.ID_ITEM'#13#10'LEFT OUTER JOIN CAD_TB_C_GRU   GRU ON  GRU.ID_GRUPO' +
      ' = ITE.ID_GRUPO'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN MEC ON  MEC.ID_FUN' +
      'CIONARIO=ORI.ID_MECANICO'#13#10'LEFT OUTER JOIN OFI_TB_M_ORV ORV ON OR' +
      'V.ID_ORDEM =ORI.ID_ORDEM'#13#10'LEFT OUTER JOIN OFI_TB_C_TIO TIO ON TI' +
      'O.ID_TIPO_OS = ORV.ID_TIPO_OS'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN ANA ' +
      'ON ANA.ID_FUNCIONARIO=ORV.ID_ANALISTA'#13#10'LEFT OUTER JOIN CAD_TB_C_' +
      'CLI CLI ON CLI.ID_CLIENTE=ORV.ID_CLIENTE'#13#10'LEFT OUTER JOIN CAD_TB' +
      '_C_CPG CPG ON CPG.ID_CONDICAO_PAG=ORV.ID_CONDICAO_PAG'#13#10'LEFT OUTE' +
      'R JOIN CAD_TB_C_VEI VEI ON VEI.ID_VEICULO=ORV.ID_VEICULO'#13#10' left ' +
      'outer join (select nfe.id_ordem,nfe.tipo_nf, CFO.ID_CLASSE,nfe.s' +
      'tatus, nfe.faturada,nfe.nfe_cod_sit,nfe.dta_emissao, nfe.ind_nf,' +
