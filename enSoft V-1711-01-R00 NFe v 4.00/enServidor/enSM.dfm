      ' PSC '#13#10'                           WHERE  ID_EMPRESA = PAR.ID_EMP' +
      'RESA '#13#10'                             AND  DATA >= '#39'01/01/2014'#39#13#10' ' +
      '                            AND  DATA <= '#39'01/12/2014'#39#13#10'         ' +
      '                    AND  PERGUNTA_5 = 1 ) as QTDE_PERGUNTA_5_SIM' +
      '  , '#13#10'                         (SELECT COUNT(*)FROM PCP_TB_M_PSC' +
      ' PSC '#13#10'                           WHERE  ID_EMPRESA = PAR.ID_EMP' +
      'RESA '#13#10'                             AND  DATA >= '#39'01/01/2014'#39#13#10' ' +
      '                            AND  DATA <= '#39'01/12/2014'#39#13#10'         ' +
      '                    AND  PERGUNTA_5 = 0 )  as QTDE_PERGUNTA_5_NA' +
      'O, '#13#10'                         (SELECT COUNT(*) FROM PCP_TB_M_PSC' +
      ' PSC '#13#10'                           WHERE  ID_EMPRESA = PAR.ID_EMP' +
      'RESA '#13#10'                             AND  DATA >= '#39'01/01/2014'#39#13#10' ' +
      '                            AND  DATA <= '#39'01/12/2014'#39#13#10'         ' +
      '                    AND  PERGUNTA_1 = 0 ) as QTDE_PERGUNTA_1_MEN' +
      'OS_DE_1_ANO, '#13#10'                         (SELECT COUNT(*)FROM PCP' +
      '_TB_M_PSC PSC '#13#10'                           WHERE  ID_EMPRESA = P' +
      'AR.ID_EMPRESA '#13#10'                             AND  DATA >= '#39'01/01' +
      '/2014'#39#13#10'                             AND  DATA <= '#39'01/12/2014'#39#13#10 +
      '                             AND  PERGUNTA_1 = 1 )  as QTDE_PERG' +
      'UNTA_1_MAIS_DE_1_ANO, '#13#10'                         (SELECT COUNT(*' +
      ')FROM PCP_TB_M_PSC PSC '#13#10'                           WHERE  ID_EM' +
      'PRESA = PAR.ID_EMPRESA '#13#10'                             AND  DATA ' +
