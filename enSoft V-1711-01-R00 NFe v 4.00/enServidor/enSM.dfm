  object OFI_SQ_R_OFV_POS_EST: TSQLDataSet
    CommandText = 
      'SELECT'#13#10#9'NFE.ID_EMPRESA,'#13#10#9'PAR.EMP_FANTASIA,'#13#10#9'COUNT (DISTINCT N' +
      'FE.ID_FISCAL) AS QTDE_NOTA,'#13#10#9#13#10#9'(SELECT COUNT(DISTINCT ORV.ID_O' +
      'RDEM) '#13#10#9#9'FROM OFI_TB_M_ORV ORV '#13#10#9#9'LEFT OUTER JOIN FAT_TB_M_NFE' +
      ' NF ON NF.ID_ORDEM =ORV.ID_ORDEM'#13#10#9#9'LEFT OUTER JOIN CAD_TB_C_CFO' +
      ' CF ON CF.ID_CFO = NF.ID_CFO'#13#10#9#9'WHERE'#9'1=2 ) AS QTDE_VEICULO,'#13#10#13#10 +
      #9'SUM (COALESCE (NIT.VLR_UNITARIO * (NIT.QTDE - NIT.QTDE_DEVOLVID' +
      'A),0)) AS VLR_BRUTO,'#13#10#9'SUM (COALESCE (NIT.VLR_DESCONTO, 0)) AS V' +
      'LR_DESCONTO,'#13#10#9'SUM (COALESCE (NFE.VLR_OUTRAS_DESP, 0)) AS VLR_DE' +
      'SPESA,'#13#10#9'SUM (COALESCE (NIT.VLR_UNITARIO * (NIT.QTDE - NIT.QTDE_' +
      'DEVOLVIDA),0)) - '#13#10#9#9'SUM ((COALESCE (NIT.VLR_DESCONTO, 0) / NIT.' +
      'QTDE) * (NIT.QTDE - NIT.QTDE_DEVOLVIDA)) AS VLR_LIQUIDO,'#13#10#9'SUM (' +
      'CASE'#9'WHEN GRU.TIPO_ITEM = '#39'09'#39' THEN COALESCE (NIT.VLR_UNITARIO *' +
      ' (NIT.QTDE - NIT.QTDE_DEVOLVIDA),0) - '#13#10#9#9'((COALESCE (NIT.VLR_DE' +
      'SCONTO, 0) / NIT.QTDE) * (NIT.QTDE - NIT.QTDE_DEVOLVIDA))'#9'END ) ' +
      'AS VLR_LIQUIDO_SER,'#13#10#9'SUM (CASE'#9'WHEN GRU.TIPO_ITEM <> '#39'09'#39' THEN'#9 +
      'COALESCE (NIT.VLR_UNITARIO * (NIT.QTDE - NIT.QTDE_DEVOLVIDA),0) ' +
      '- '#13#10#9#9'((COALESCE (NIT.VLR_DESCONTO, 0) / NIT.QTDE) * (NIT.QTDE -' +
      ' NIT.QTDE_DEVOLVIDA))'#9'END) AS VLR_LIQUIDO_PRO'#13#10#13#10'FROM'#9'FAT_TB_M_N' +
      'FE_ITE NIT'#13#10'LEFT OUTER JOIN FAT_TB_M_NFE NFE ON NFE.ID_FISCAL = ' +
