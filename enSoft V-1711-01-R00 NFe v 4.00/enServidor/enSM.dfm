  object FIN_SQ_R_CXA_DIA_NPD: TSQLDataSet
    CommandText = 
      'select nfe.*,cli.nome as int_nomecli,cpg.descricao as int_nomecp' +
      'g,tme.descricao as int_nometme from fat_tb_m_nfe nfe '#13#10'left oute' +
      'r join cad_tb_c_cli cli on cli.id_cliente=nfe.id_emitente'#13#10'LEFT ' +
      'OUTER JOIN CAD_TB_C_CPG CPG ON CPG.ID_CONDICAO_PAG=nfe.id_condic' +
      'ao_pag  '#13#10'LEFT OUTER JOIN CAD_TB_C_TME TME ON TME.ID_TIPO_MOV_ES' +
      'TOQUE=nfe.ID_TIPO_MOV_ESTOQUE'#13#10'where nfe.tipo_nf='#39'S'#39' and nfe.nfe' +
      '_cod_sit not in ('#39'02'#39') and nfe.faturada=false'#13#10'and 1=2'
    MaxBlobSize = -1
    Params = <>
    SQLConnection = Conexao
    Left = 3120
    Top = 1032
  end
