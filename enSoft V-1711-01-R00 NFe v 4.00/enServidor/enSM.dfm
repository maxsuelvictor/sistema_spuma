  object OFI_SQ_R_OFV_POC_001: TSQLDataSet
    CommandText = 
      'SELECT'#13#10'       OCV.*,'#13#10'       CLI.doc_cnpj_cpf AS INT_CPFCNPJ,'#13#10 +
      '       CLI.pessoa as int_pessoacli,'#13#10'       CLI.NOME AS INT_NOME' +
      'CLI,'#13#10'       CLI.TEL_FIXO AS INT_TELFIX,'#13#10'       CLI.TEL_MOVEL A' +
