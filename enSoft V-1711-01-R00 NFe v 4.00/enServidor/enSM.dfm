      'ttt'#13#10'    LEFT OUTER JOIN CAD_TB_C_FAM FAM ON  FAM.ID_FAMILIA=ttt' +
      '.ID_FAMILIA'#13#10'    LEFT OUTER JOIN CAD_TB_C_FOR FRN ON (FRN.ID_FOR' +
      'NECEDOR=ttt.ID_FORNECEDOR)'#13#10'where '#13#10'   1 = 2 and'#13#10'   ttt.id_empr' +
      'esa in ('#39'1'#39') and'#13#10'   ttt.id_familia in ('#39'1'#39','#39'3'#39')  and '#13#10'   not e' +
      'xists (SELECT FFM.ID_ITEM FROM EST_TB_M_FEA FFM'#13#10'               ' +
