      'm_nfe nfe on (nfe.id_fiscal = nfi.id_fiscal)'#13#10#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9'w' +
      'here nfe.tipo_nf = '#39'E'#39#13#10#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9#9'and nfi.id_item = ite.' +
