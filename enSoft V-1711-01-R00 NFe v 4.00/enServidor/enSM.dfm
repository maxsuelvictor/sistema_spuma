      'ace(replace(cm.cpf_cnpj,'#39'.'#39','#39#39'),'#39'-'#39','#39#39') AS cpf_cnpj ,cm.n_caixa,' +
      ' '#13#10'case cm.cancelado '#13#10#9'when '#39'S'#39' then '#39'90'#39#13#10#9'when  '#39'N'#39' then '#39'00'#39 +
      ' '#13#10'end situacao, '#13#10'(select valor from br_itens_rz brr '#13#10#9'where '#13 +
      #10#9#9'brr.serial = cd.serial_impressora '#13#10'    and brr.sigla = '#39'CRO'#39 +
      #13#10'    and brr.data_movimento =  '#13#10#9#9#9'(select max(brz.data_movime' +
      'nto) from br_itens_rz brz '#13#10#9#9#9#9'where brz.serial = cd.serial_imp' +
      'ressora '#13#10#9#9#9#9#9'and brz.sigla = '#39'CRO'#39')) AS CRO,'#13#10#13#10'(select valor ' +
      'from br_itens_rz brr '#13#10#9'where '#13#10#9#9'brr.serial = cd.serial_impress' +
      'ora '#13#10'    and brr.sigla = '#39'CRZ'#39#13#10'    and brr.data_movimento = '#13#10 +
      '      (select max(brz.data_movimento) from br_itens_rz brz '#13#10'   ' +
      '     where brz.serial = cd.serial_impressora'#13#10'          and brz.' +
      'sigla = '#39'CRZ'#39')) AS CRZ, '#13#10#13#10'(select distinct na.id_classe '#13#10'  FR' +
      'OM br_cupom_detalhe Cx '#13#10'  left outer join cad_tb_c_cfo cf on re' +
      'place(CF.id_cfo,'#39'.'#39','#39#39')= cast(cx.cfop as varchar(5)) '#13#10'  left ou' +
      'ter join cad_tb_c_nat na on na.id_classe = cf.id_classe '#13#10'  wher' +
      'e cx.n_cupom=cd.n_cupom and cx.serial_impressora = cd.serial_imp' +
      'ressora ) as classe, '#13#10'  '#13#10'Sum(cd.valor_total) Vlr_total, '#13#10'Sum(' +
      'cd.desconto) Vlr_desconto, '#13#10'Sum(case when cm.cancelado='#39'S'#39'  the' +
      'n cd.valor_total end) vlr_cancelado, '#13#10'Sum(case when (not cd.ali' +
      'quota in ('#39'F1'#39','#39'II'#39')) and (not cd.cst_icms in ('#39'40'#39','#39'41'#39','#39'90'#39')) ' +
      ' then cd.valor_total end) base_icms, '#13#10'Sum(case when (not cd.ali' +
      'quota in ('#39'F1'#39','#39'II'#39')) and (not cd.cst_icms in ('#39'40'#39','#39'41'#39','#39'90'#39')) ' +
      ' then (cd.valor_total * CAST(replace(replace(cd.aliquota,'#39'.'#39','#39#39')' +
      ','#39','#39','#39'.'#39')  AS NUMERIC(18,2) )  )/100 end) Vlr_icms, '#13#10'Sum(case w' +
      'hen cd.aliquota = '#39'F1'#39'  then cd.valor_total end) base_icms_sub, ' +
      #13#10'Sum(case when cd.aliquota = '#39'0,00'#39' then cd.valor_total end) VL' +
      'R_ISENTO '#13#10'FROM br_cupom_detalhe Cd '#13#10'left outer join br_cupom_m' +
      'estre cm on cm.serial_impressora=cd.serial_impressora and cm.n_c' +
      'upom=cd.n_cupom '#13#10'left outer join cad_tb_c_par par on trim(par.e' +
      'mp_cnpj)=trim(replace(replace(replace(cm.cnpj_empresa,'#39'.'#39','#39#39'),'#39'/' +
      #39','#39#39'),'#39'-'#39','#39#39')) '#13#10#13#10'Where 1=2'#13#10'group by 1,2,3,4,5,6,7'
