  object CAD_SQ_C_CLI: TSQLDataSet
    SchemaName = 'dbo'
    CommandText = 
      'SELECT false as int_selecao, CLI.*, '#13#10'   CID.UF INT_UF,  CID.NOM' +
      'E AS INT_NOMECID,'#13#10'   CDD.UF INT_AVAL_UF,  CDD.NOME AS INT_AVAL_' +
      'NOMECID,'#13#10'                cast( CASE SITUACAO '#13#10'                ' +
      '  WHEN 0 THEN '#39'Normal'#39#13#10'                  WHEN 1 THEN '#39'Especial'#39 +
      ' '#13#10'                  WHEN 2 THEN '#39'Cr'#233'dito encerrado'#39#13#10'          ' +
      '      END AS VARCHAR(30) ) AS int_situacao, '#13#10'               cas' +
      't( CASE CLI.SEXO '#13#10'                  WHEN 0 THEN '#39'Feminino'#39#13#10'   ' +
      '               WHEN 1 THEN '#39'Masculino'#39#13#10'                END AS V' +
      'ARCHAR(10) ) AS int_sexo ,'#13#10#13#10'             cast( CASE CLI.PESSOA' +
      ' '#13#10'                  WHEN 0 THEN '#39'F'#237'sica'#39#13#10'                  WHE' +
      'N 1 THEN '#39'Jur'#237'dica'#39#13#10'                END AS VARCHAR(10) ) AS int' +
      '_pessoa,'#13#10#13#10'    REG.DESCRICAO AS INT_NOMEREG,'#13#10'    REG.DESC_PERC' +
      ' AS INT_REG_DESC_PERC,'#13#10'    REG.unifica_desc_reg AS INT_UNIF_DES' +
      'C_REG,'#13#10'    PAI.DESCRICAO AS INT_NOMEPAI,'#13#10'   FUN.NOME AS INT_NO' +
      'MERESP,'#13#10'    FUN2.NOME AS INT_NOMEVND,'#13#10'  PEC.DESCRICAO AS INT_N' +
      'OMEPEC'#13#10#13#10'FROM CAD_TB_C_CLI CLI'#13#10'LEFT OUTER JOIN CAD_TB_C_CID CI' +
      'D ON CID.ID_CIDADE=CLI.ID_CIDADE'#13#10'LEFT OUTER JOIN CAD_TB_C_CID C' +
      'DD ON CDD.ID_CIDADE=CLI.AVAL_ID_CIDADE'#13#10'LEFT OUTER JOIN PCP_TB_C' +
      '_REG REG ON (REG.ID_REGIAO = CLI.ID_REGIAO)'#13#10'LEFT OUTER JOIN CAD' +
      '_TB_C_PAI PAI ON (PAI.ID_PAIS = CLI.COD_PAIS)'#13#10'LEFT OUTER JOIN C' +
      'AD_TB_C_FUN FUN ON (FUN.ID_FUNCIONARIO=CLI.ID_RESPONSAVEL)'#13#10'LEFT' +
      ' OUTER JOIN CAD_TB_C_PEC PEC ON (PEC.ID_PERFIL_CLI=CLI.ID_PERFIL' +
      '_CLI)'#13#10'LEFT OUTER JOIN CAD_TB_C_FUN FUN2 ON (FUN2.ID_FUNCIONARIO' +
      '=CLI.ID_VENDEDOR)'#13#10'where 1 = 2'
    MaxBlobSize = -1
