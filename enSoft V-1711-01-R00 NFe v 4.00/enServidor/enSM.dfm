  object FIN_SQ_R_PAG_ABE: TSQLDataSet
    CommandText = 
      'SELECT pag.id_empresa, pag.num_parcela, pag.id_forma_pag, pag.dt' +
      'a_emissao, pag.dta_vencimento, '#13#10'       pag.id_fornecedor, pag.i' +
      'd_local_titulo, pag.id_plano, pag.id_ccusto,pag.id_obra,'#13#10'      ' +
      '  pag.pre_conta,pag.pre_cheque,pag.pre_banco,pag.pre_emitente,pa' +
      'g.pre_agencia,  '#13#10'  FON.DESCRICAO AS INT_NOMEFOR, '#13#10'  FPG.DESCRI' +
      'CAO AS INT_NOMEFPG, '#13#10'  LTO.DESCRICAO AS INT_NOMELTO,'#13#10'  PCT.DES' +
      'CRICAO AS INT_NOMEPCT,'#13#10'  CCU.descricao AS INT_NOMECCU, '#13#10'  OBR.' +
      'descricao AS INT_NOMEOBR, '#13#10'  pag.vlr_saldo, pag.vlr_parcela, PA' +
      'G.historico, '#39#39' as prazo,'#13#10'  (CURRENT_DATE -  pag.dta_vencimento' +
      ') as INT_ATRASO,'#13#10'  CASE '#13#10'    WHEN (CURRENT_DATE -  pag.dta_ven' +
      'cimento)<=0 THEN pag.vlr_saldo'#13#10'    WHEN (CURRENT_DATE -  pag.dt' +
      'a_vencimento)>0  THEN pag.vlr_saldo+((pag.vlr_saldo*(CTR.tax_jur' +
      'os_mensal/30)/100)*(CURRENT_DATE -  pag.dta_vencimento))'#13#10'  END ' +
      'as INT_VLR_CORRIGIDO,'#13#10'  CASE '#13#10'    WHEN (CURRENT_DATE -  pag.dt' +
      'a_vencimento)<=0 THEN 0'#13#10'    WHEN (CURRENT_DATE -  pag.dta_venci' +
      'mento)>0  THEN (pag.vlr_saldo+((pag.vlr_saldo*(CTR.tax_juros_men' +
      'sal/30)/100)*(CURRENT_DATE -  pag.dta_vencimento)))-pag.vlr_sald' +
      'o'#13#10'  END as INT_VLR_JUROS'#13#10#13#10'FROM FIN_TB_M_PAG PAG'#13#10'LEFT OUTER J' +
      'OIN CAD_TB_C_FOR FON ON FON.ID_FORNECEDOR = PAG.ID_FORNECEDOR'#13#10'L' +
      'EFT OUTER JOIN CAD_TB_C_FPG FPG ON FPG.ID_FORMA_PAG = PAG.ID_FOR' +
      'MA_PAG'#13#10'LEFT OUTER JOIN CAD_TB_C_LTO LTO ON LTO.ID_LOCAL_TITULO ' +
      '= PAG.ID_LOCAL_TITULO'#13#10'LEFT OUTER JOIN CAD_TB_C_PCT PCT ON PCT.I' +
      'D_PLANO = PAG.ID_PLANO'#13#10'LEFT OUTER JOIN cad_tb_c_ccu CCU ON CCU.' +
      'id_ccusto= PAG.ID_CCUSTO'#13#10'LEFT OUTER JOIN cad_tb_c_par_ctr CTR O' +
      'N CTR.ID_EMPRESA=PAG.ID_EMPRESA'#13#10' LEFT OUTER JOIN ctc_tb_c_obr O' +
      'BR ON OBR.ID_OBRA=PAG.ID_OBRA'#13#10'where 1=2'
