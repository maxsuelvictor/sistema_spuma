      'id_st_icm,'#13#10#9'nit.vlr_desconto,'#13#10#9'nit.icm_n_valor,'#13#10#9'ite.id_und_v' +
      'enda,  '#13#10#9'cpg.descricao as int_nomecpg,'#13#10#9'tme.descricao as int_n' +
      'ometme,'#13#10#9'nfe.dta_emissao as "data de emiss'#227'o nota fiscal",'#13#10#9'nf' +
      'e.id_cfo as "cfop",'#13#10#9#39#39' as gerente,'#13#10#9'cfo.id_classe || '#39'-'#39' || n' +
      'at.descricao as "natureza",'#13#10#9'nit.vlr_unitario,'#13#10#9'ven.doc_cpf as' +
      ' "cpf consultor/vendedor",nfe.dta_documento, '#13#10#13#10'case'#13#10#9'when (nf' +
      'e.tipo_nf = '#39'e'#39') or (nfe.tipo_nf = '#39'd'#39') then frn.descricao'#13#10#9'whe' +
      'n (nfe.tipo_nf = '#39's'#39') or (nfe.tipo_nf = '#39'r'#39') then cli.nome '#13#10'end' +
      ' as cliente_fornecedor,'#13#10#13#10'cast (case'#13#10#9'when (nfe.tipo_nf = '#39'e'#39')' +
      ' or (nfe.tipo_nf = '#39'd'#39') then frn.doc_cnpj'#13#10#9'when (nfe.tipo_nf = ' +
      #39's'#39') or (nfe.tipo_nf = '#39'r'#39') then cli.doc_cnpj_cpf'#13#10'end as varcha' +
      'r (20)) as cpf_cnpj,'#13#10#13#10'case'#13#10#9'when (nfe.tipo_nf = '#39'e'#39') or (nfe.' +
