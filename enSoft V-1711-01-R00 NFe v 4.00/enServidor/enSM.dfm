  object FAT_SQ_R_NFE_DPI: TSQLDataSet
    CommandText = 
      'select'#13#10#9#39#39' as sap,'#13#10#9'par.emp_fantasia as "empresa",'#13#10#9'ite.descr' +
      'icao as "produto",'#13#10#9'nfe.numero as "numero nota fiscal",'#13#10#9'nfe.s' +
