  object FIN_SQ_R_REC_ABE_PPG: TSQLDataSet
    CommandText = 
      'Select '#39'A PARTIR DE 181 DIAS'#39' Prazo,      '#13#10'  rec.ID_FORMA_PAG, ' +
      'rec.NUM_PARCELA, rec.DTA_EMISSAO, fpg.doc_impresso,rec.car_taxa,' +
      ' rec.dta_original,      rec.DTA_VENCIMENTO, rec.ID_CLIENTE, CLI.' +
      'NOME AS INT_NOMECLI,        (current_date-rec.DTA_VENCIMENTO) IN' +
      'T_ATRASO,  '#13#10'      FPG.DESCRICAO AS INT_NOMEFPG,        rec.VLR_' +
      'SALDO, rec.vlr_parcela  from FIN_TB_M_REC rec  Left Outer Join C' +
      'AD_TB_C_CLI CLI On CLI.ID_CLIENTE=rec.ID_CLIENTE'#13#10'  left outer j' +
      'oin cad_tb_c_fpg FPG ON FPG.ID_FORMA_PAG = rec.ID_FORMA_PAG  Whe' +
      're 1=2 and (rec.VLR_SALDO>0) and rec.DTA_VENCIMENTO<current_date' +
      ' and       (current_date-rec.DTA_VENCIMENTO)>=181  AND'#13#10' rec.DTA' +
      '_EMISSAO >= '#39'27/11/2011'#39' AND        rec.DTA_EMISSAO <= '#39'27/11/20' +
      '14'#39'  AND rec.ID_EMPRESA in ('#39'001'#39')  '#13#10' Union All  '#13#10'Select '#39'DE 1' +
      '51 A 180 DIAS'#39' Prazo,    '#13#10'    rec.ID_FORMA_PAG, rec.NUM_PARCELA' +
      ', rec.DTA_EMISSAO, fpg.doc_impresso,rec.car_taxa, rec.dta_origin' +
      'al, '#13#10'      rec.DTA_VENCIMENTO, rec.ID_CLIENTE, CLI.NOME AS INT_' +
      'NOMECLI,        (current_date-rec.DTA_VENCIMENTO) INT_ATRASO,   ' +
      '     FPG.DESCRICAO AS INT_NOMEFPG,    '#13#10'    rec.VLR_SALDO, rec.v' +
      'lr_parcela  from FIN_TB_M_REC rec  Left Outer Join CAD_TB_C_CLI ' +
      'CLI On CLI.ID_CLIENTE=rec.ID_CLIENTE  left outer join cad_tb_c_f' +
      'pg '#13#10'FPG ON FPG.ID_FORMA_PAG = rec.ID_FORMA_PAG  Where 1=2 and (' +
      'rec.VLR_SALDO>0) and       rec.DTA_VENCIMENTO<current_date and  ' +
      '     (current_date-rec.DTA_VENCIMENTO)>=151 and '#13#10'      (current' +
      '_date-rec.DTA_VENCIMENTO)<=180  AND rec.DTA_EMISSAO >= '#39'27/11/20' +
      '11'#39' AND        rec.DTA_EMISSAO <= '#39'27/11/2014'#39'  AND rec.ID_EMPRE' +
      'SA in ('#39'001'#39')'#13#10'   Union All '#13#10' Select '#39'DE 121 A 150 DIAS'#39' Prazo,' +
      '        rec.ID_FORMA_PAG, rec.NUM_PARCELA, rec.DTA_EMISSAO,   fp' +
      'g.doc_impresso,rec.car_taxa, rec.dta_original,    rec.DTA_VENCIM' +
      'ENTO, rec.ID_CLIENTE, CLI.NOME AS INT_NOMECLI,   '#13#10'     (current' +
      '_date-rec.DTA_VENCIMENTO) INT_ATRASO,        FPG.DESCRICAO AS IN' +
      'T_NOMEFPG,        rec.VLR_SALDO, rec.vlr_parcela  from FIN_TB_M_' +
      'REC rec '#13#10' Left Outer Join CAD_TB_C_CLI CLI On CLI.ID_CLIENTE=re' +
      'c.ID_CLIENTE  left outer join cad_tb_c_fpg FPG ON FPG.ID_FORMA_P' +
      'AG = rec.ID_FORMA_PAG  Where 1=2 and (rec.VLR_SALDO>0)  and '#13#10'  ' +
      '    rec.DTA_VENCIMENTO<current_date and       (current_date-rec.' +
      'DTA_VENCIMENTO)>=121 and       (current_date-rec.DTA_VENCIMENTO)' +
      '<=150  AND rec.DTA_EMISSAO >= '#39'27/11/2011'#39' AND   '#13#10'     rec.DTA_' +
      'EMISSAO <= '#39'27/11/2014'#39'  AND rec.ID_EMPRESA in ('#39'001'#39')   '#13#10'Union' +
      ' All  '#13#10'Select '#39'DE 91 A 120 DIAS'#39' Prazo,     '#13#10'   rec.ID_FORMA_P' +
      'AG, rec.NUM_PARCELA, rec.DTA_EMISSAO,   fpg.doc_impresso,rec.car' +
      '_taxa, rec.dta_original,'#13#10'   rec.DTA_VENCIMENTO, rec.ID_CLIENTE,' +
      ' CLI.NOME AS INT_NOMECLI,        (current_date-rec.DTA_VENCIMENT' +
      'O) INT_ATRASO,        FPG.DESCRICAO AS INT_NOMEFPG,     '#13#10'   rec' +
      '.VLR_SALDO, rec.vlr_parcela  from FIN_TB_M_REC rec  Left Outer J' +
