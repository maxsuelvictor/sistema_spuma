      'NCIONARIO=NFE.ID_VENDEDOR  '#13#10'          LEFT OUTER JOIN CAD_TB_C_' +
      'CFO CFO ON CFO.ID_CFO=NFE.ID_CFO           '#13#10'           WHERE  1' +
      '=2 AND PAR.ID_EMPRESA = '#39'1'#39' AND '#13#10'          '#13#10'          NFE.DTA_' +
