      'a_entrega'#13#10'           else 0 end as int_atraso, '#13#10#13#10'           s' +
      'um(case gru.tipo_item'#13#10'              when '#39'09'#39'  then vlr_liquido' +
      #13#10'           end) as vlr_liquido_ser,'#13#10#13#10'           sum(case'#13#10'  ' +
      '            when  gru.tipo_item <> '#39'09'#39'  then vlr_liquido'#13#10'     ' +
      '      end) as vlr_liquido_pro,'#13#10'           sum(vlr_liquido) as v' +
      'lr_total_sem_desp,'#13#10'           orv.vlr_despesas + sum(vlr_liquid' +
