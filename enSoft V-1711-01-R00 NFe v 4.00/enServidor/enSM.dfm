      '       sum(rcr.vlr_credito) as vlr_total, count(*) as tot_parcel' +
      'as'#13#10#9'from fin_tb_m_rcr rcr    '#13#10'    left outer join cad_tb_c_fpg' +
      ' fpg on (fpg.id_forma_pag = rcr.id_forma_pag)'#13#10'  where   '#13#10'     ' +
      'rcr.id_empresa   = 1 AND'#13#10'     rcr.id_abertura  = 0 and'#13#10'     fp' +
      'g.doc_impresso = 3  '#13#10'  group by rcr.id_credito,rcr.id_empresa, ' +
      'rcr.id_maquineta, rcr.id_forma_pag, fpg.descricao'#13#10')'#13#10'select dbc' +
      '.*, maq.descricao as int_nomemaq, cast( coalesce('#39'De'#39' || CAST(ta' +
      'p.qtde_parc_ini as char) || '#39'at'#233#39' ||'#13#10'                          ' +
      '     CAST(tap.qtde_parc_fin as char),'#39'SEM FAIXA'#39') as varchar(15)' +
      ') as Faixa,'#13#10'       COALESCE(tap.taxa,0) as taxa, tap.dias_acres' +
      '_vencto'#13#10'from det_nota_rcr_cartao dbc'#13#10'      left outer join cad' +
      '_tb_c_fpg_tap tap on '#13#10'                                         ' +
      '   ( tap.id_empresa       =  dbc.id_empresa    AND'#13#10'            ' +
      '                                  tap.id_maquineta    =  dbc.id_' +
      'maquineta AND  '#13#10'                                              t' +
      'ap.id_forma_pag   =  dbc.id_forma_pag AND'#13#10'                     ' +
      '                         tap.qtde_parc_ini <=  dbc.tot_parcelas ' +
      '  AND '#13#10'                                              tap.qtde_p' +
      'arc_fin >= dbc.tot_parcelas)'#13#10'      left outer join cad_tb_c_mct' +
      ' maq on (maq.id_maquineta= dbc.id_maquineta)'#13#10'where 1 = 2'#13#10'order' +
      ' by dbc.id_credito, maq.descricao, dbc.id_forma_pag'
