  object OFI_SQ_M_ORV: TSQLDataSet
    CommandText = 
      'SELECT false as int_selecao, ORV.*,'#13#10'       CLI.doc_cnpj_cpf AS ' +
      'INT_CPFCNPJ,'#13#10'       CLI.pessoa as int_pessoacli,'#13#10'       CLI.NO' +
      'ME AS INT_NOMECLI,'#13#10'       CLI.TEL_FIXO AS INT_TELFIX,'#13#10'       C' +
      'LI.TEL_MOVEL AS INT_TELMOV,'#13#10'       CLI.substituto_tributario AS' +
      ' INT_SUBST_TRIB,'#13#10'       case'#13#10'         when VEI.COMBUSTIVEL = 0' +
      ' then '#39'Gasolina'#39#13#10'         when VEI.COMBUSTIVEL = 1 then '#39'Alcool' +
      #39#13#10'         when VEI.COMBUSTIVEL = 2 then '#39'Diesel'#39#13#10'         whe' +
      'n VEI.COMBUSTIVEL = 3 then '#39'Gas Natural'#39#13#10'         when VEI.COMB' +
      'USTIVEL = 4 then '#39'Eletrico'#39#13#10'         when VEI.COMBUSTIVEL = 5 t' +
      'hen '#39'Flex'#39#13#10'       end as INT_NOMECOM,'#13#10'       VEI.ID_PLACA AS I' +
      'NT_PLACA,   VEI.DESCRICAO AS INT_NOMEVEI,'#13#10'       TIO.DESCRICAO ' +
      'AS INT_NOMETIO,'#13#10'       TIO.TIPO AS INT_TIPO_OS,'#13#10'       CID.UF ' +
      'AS INT_UFCLI,'#13#10'       MVE.DESCRICAO AS INT_NOMEMVE,'#13#10#13#10'      VEI' +
      '.ANO_MODELO AS INT_ANOMODELO, '#13#10'      VEI.ANO_FABRICACAO AS INT_' +
      'ANOFABRIC, VEI.MOTOR AS INT_MOTORVEI, '#13#10#13#10'      cast(case status' +
      '_painel   '#13#10'              when  0  then '#39'Aguardando inicio do se' +
      'rvi'#231'o'#39'                        '#13#10'               when  1  then '#39'Pr' +
      #243'ximo Ordem de Servi'#231'o'#39#13#10'               when  2  then '#39'Em Servi'#231 +
      'o'#39#13#10'               when  3  then '#39'Pr'#233'-Inspe'#231#227'o'#39#13#10'               ' +
      'when  4  then '#39'Aguardando Lavagem'#39#13#10'               when  5  then' +
      ' '#39'Inspe'#231#227'o Final'#39#13#10'               when  6  then '#39'Aguardando Apro' +
      'va'#231#227'o'#39#13#10'               when  7  then '#39'Aguardando Pe'#231'as'#39#13#10'       ' +
      '        when  8  then '#39'Aguardando Servi'#231'o'#39#13#10'               when ' +
      ' 9  then '#39'Aguardando Pr'#233'-Entrega'#39#13#10'               when  10 then ' +
      #39'Aguardando Entrega'#39#13#10'               when  11 then '#39'Entregue'#39#13#10' ' +
      '           end  as varchar(30)) as int_statuspainel ,'#13#10'        e' +
      'qp.descricao as int_nomeeqp,'#13#10'        meq.descricao as int_nomem' +
      'eq,'#13#10'        meq.tipo_mei as int_tipomei,'#13#10'        ATE.NOME AS I' +
      'NT_NOMEATE'#13#10'FROM OFI_TB_M_ORV ORV'#13#10'   LEFT OUTER JOIN CAD_TB_C_C' +
      'LI CLI ON CLI.ID_CLIENTE=ORV.ID_CLIENTE'#13#10'   LEFT OUTER JOIN CAD_' +
      'TB_C_FUN ANA ON ANA.ID_FUNCIONARIO=ORV.ID_ANALISTA'#13#10'   LEFT OUTE' +
      'R JOIN CAD_TB_C_FUN ATE ON ATE.ID_FUNCIONARIO=ORV.ID_ATENDENTE'#13#10 +
      '   LEFT OUTER JOIN CAD_TB_C_VEI VEI ON VEI.ID_VEICULO=ORV.ID_VEI' +
      'CULO'#13#10'   LEFT OUTER JOIN CAD_TB_C_MVE MVE ON MVE.ID_MVE = VEI.ID' +
      '_MVE'#13#10'   LEFT OUTER JOIN CAD_TB_C_FPG FPG ON FPG.ID_FORMA_PAG=OR' +
      'V.ID_FORMA_PAG'#13#10'   LEFT OUTER JOIN CAD_TB_C_CPG CPG ON CPG.ID_CO' +
      'NDICAO_PAG=ORV.ID_CONDICAO_PAG'#13#10'   LEFT OUTER JOIN OFI_TB_C_TIO ' +
      'TIO ON TIO.ID_TIPO_OS = ORV.ID_TIPO_OS'#13#10'   LEFT OUTER JOIN CAD_T' +
      'B_C_CID CID ON CID.ID_CIDADE = CLI.ID_CIDADE'#13#10'   LEFT OUTER JOIN' +
      ' CAD_TB_C_EQP EQP ON EQP.ID_EQUIPAMENTO = ORV.ID_EQUIPAMENTO'#13#10'  ' +
      ' LEFT OUTER JOIN PCP_TB_C_MEQ MEQ ON MEQ.ID_MEQ = ORV.ID_MEQ'#13#10'wh' +
      'ere 1 = 2'
