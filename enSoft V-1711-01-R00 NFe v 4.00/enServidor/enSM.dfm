      '   end as varchar(20)) as INT_ORVSITUACAO,'#13#10'           orv.dta_e' +
      'ntrega,'#13#10'           orv.can_data, orv.can_motivo, '#13#10'           o' +
      'rv.vlr_despesas, orv.dta_emissao, '#13#10'           case'#13#10'           ' +
