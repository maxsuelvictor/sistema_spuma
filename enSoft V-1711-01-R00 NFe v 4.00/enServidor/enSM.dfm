  object FIN_SQ_R_CXA_DIA_MOP: TSQLDataSet
    CommandText = 
      'SELECT'#13#10'  CTA.DEB_CRE,'#13#10'  CTA.DTA_MOVIMENTO, CTA.NUM_DOC, TIF.SI' +
      'GLA, CTA.ID_PLANO, CTA.ID_TIPO_FINANCEIRO, CTA.HISTORICO,'#13#10'  COA' +
      'LESCE(SUM(CASE WHEN CTA.DEB_CRE=1 THEN CTA.VLR_LANCAMENTO ELSE 0' +
      ' END), 0) VLR_CREDITODIA,'#13#10'  COALESCE(SUM(CASE WHEN CTA.DEB_CRE=' +
      '0 THEN CTA.VLR_LANCAMENTO ELSE 0 END), 0) VLR_DEBITODIA,'#13#10'  COAL' +
      'ESCE(SUM(CTA.VLR_LANCAMENTO * (CASE WHEN CTA.DEB_CRE=0 THEN -1 E' +
      'LSE 1 END)), 0) VLR_SALDODIA,'#13#10'  (SELECT COALESCE(SUM(I_CTA.VLR_' +
      'LANCAMENTO * (CASE WHEN I_CTA.DEB_CRE=0 THEN -1 ELSE 1 END)), 0)' +
      #13#10'   FROM  FIN_TB_M_CTA I_CTA'#13#10'   WHERE I_CTA.DTA_MOVIMENTO <= C' +
      'TA.DTA_MOVIMENTO order by CTA.DTA_MOVIMENTO  ) VLR_ACUMULADO'#13#10'FR' +
      'OM FIN_TB_M_CTA CTA'#13#10'LEFT OUTER JOIN CAD_TB_C_TIF TIF ON TIF.ID_' +
      'TIPO_FINANCEIRO = CTA.ID_TIPO_FINANCEIRO'#13#10'LEFT OUTER JOIN CAD_TB' +
      '_C_PCT PCT ON PCT.ID_PLANO = CTA.ID_PLANO'#13#10'LEFT OUTER JOIN FIN_T' +
      'B_M_CXA CXA ON CXA.ID_ABERTURA= CTA.ID_ABERTURA'#13#10'WHERE  CTA.DTA_' +
      'MOVIMENTO BETWEEN '#39'14.10.2011'#39' AND '#39'31.12.2014'#39' AND CTA.TIPO_LAN' +
      'CAMENTO=0 AND 1=2     -- 0=CAIXA, 1=BANCO'#13#10'GROUP BY 1,2,3,4,5,6,' +
      '7'#13#10'ORDER BY   CTA.DEB_CRE, CTA.DTA_MOVIMENTO'
    MaxBlobSize = -1
    Params = <>
    SQLConnection = Conexao
    Left = 3120
    Top = 896
  end
