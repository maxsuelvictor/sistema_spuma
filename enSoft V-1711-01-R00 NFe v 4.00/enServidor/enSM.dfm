  object BUS_SQ_M_ORV: TSQLDataSet
    CommandText = 
      'SELECT ORV.*,'#13#10'       CLI.doc_cnpj_cpf AS INT_CPFCNPJ,'#13#10'       C' +
      'LI.pessoa as int_pessoacli,'#13#10'       CLI.NOME AS INT_NOMECLI,'#13#10'  ' +
