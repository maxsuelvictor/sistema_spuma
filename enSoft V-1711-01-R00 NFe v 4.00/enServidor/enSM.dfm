      '                               '#13#10'    left outer join fat_tb_m_nf' +
      'e nfe on nfe.id_vendedor=FUN.id_funcionario                     ' +
      '       '#13#10'                       and extract(year from nfe.dta_em' +
      'issao)='#39'2000'#39'                 '#13#10'                       and NFE.N' +
      'FE_COD_SIT not in ('#39'02'#39','#39'04'#39','#39'05'#39') AND                          ' +
