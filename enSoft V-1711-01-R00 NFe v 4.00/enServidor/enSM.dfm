      'CID ON'#13#10'      CID.ID_CIDADE = CLI.ID_CIDADE'#13#10'LEFT OUTER JOIN CAD' +
      '_TB_C_MVE MVE ON '#13#10'      MVE.ID_MVE=VEI.ID_MVE                  ' +
      '  '#13#10'LEFT OUTER JOIN CAD_TB_C_COR COR ON'#13#10'     COR.ID_COR = VEI.I' +
